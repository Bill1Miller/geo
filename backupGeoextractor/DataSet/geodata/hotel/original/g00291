<SCRIPT LANGUAGE=javascript>
<!--
// mimics the functionality of javascript's 1.3 split method
// returns a javascript array
function customSplit(sText, sDelimiter)
{
	if (sText.length > 0)
	{
		var sPos = sText.indexOf(sDelimiter,0);
		if (sPos > 0)
		{
			var ary = new Array();
			var i = 0;
			var j = 0;
			while (sPos > 0)
			{
				ary[i] = sText.substring(j, sPos);
				i = i + 1;
				j = sPos + 1;
				sPos = sText.indexOf(sDelimiter,j+1);
			}
			// get the last part
			if (j < sText.length)
			{
				ary[i] = sText.substring(j);
			}
			return ary;
		}
		return false;
	}
	return false;
}
	
// gets the index of the clicked control in a collection ...
// i.e. two or more checkboxes having the same name; 
// proven to work in IE 3.x and Netscape 3.x
function getIndex(optControl)
{
	var sVal = optControl.value;
	var sName = optControl.name;
	var idx;
	var cnt = window.document.forms[0].elements.length;
	var fld;
	var j = 0;
	for (var i = 0; i < cnt; i++)
	{
		if (document.forms[0].elements[i].name == sName)
		{
			fld = document.forms[0].elements[i];
			if (fld.value == sVal)
			{
				idx = j;
				break;
			}
				j = j + 1;
		}
	}
	return idx;
}	
//-->
</SCRIPT><script language=javascript>
var alerted = false;
var modNumber = 300;
var timeToAlert = 900;
var timeOut = 1210;
var alertMessage = 'Your browser has been idle for ' + (timeToAlert/60) + ' minutes.\nThis session will time-out in ' + ((timeOut - timeToAlert - 10)/60) + ' minutes if no actions are taken.';
var redirectTo, startday, clockStart;

	redirectTo = "http://www.quikbook.com/main.asp?Time_Out=1";
	

//------------------------------------------------------
//function UserTime()
//{
//  var d, s = "";
//  var c = ":";
//  d = new Date();
//  s += d.getHours() + c;
//  s += d.getMinutes() + c;
//  s += d.getSeconds();
// return(s);
//}
//------------------------------------------------------
startday = new Date();
clockStart = startday.getTime();

function initStopwatch() 
{ 
 var myTime = new Date(); 
        var timeNow = myTime.getTime();
        var timeDiff = timeNow - clockStart; 
        this.diffSecs = timeDiff/1000; 
        return(this.diffSecs); 
}
//------------------------------------------------------
function getSecs() 
{ 
    var mySecs = initStopwatch(); 
    var mySecs1 = "" + mySecs;
    var timeOut_NoMore = false;
    mySecs1 = parseInt(mySecs1.substring(0,mySecs1.indexOf(".")),10);
    
	if (!isNaN(mySecs1) && mySecs1 > 0)
	{
		if (mySecs1 % modNumber == 0 && mySecs1 >= timeToAlert)
		{
			if (mySecs1 >= timeOut)
			{
				timeOut_NoMore = true;
				window.location = redirectTo;
			}
			else
			{
				if (alerted == false)
				{
					alert(alertMessage);
					alerted = true;
					modNumber = 1;
				}
			}
		}
	}
	if (timeOut_NoMore == false)
	{
    window.setTimeout('getSecs()',1000); 
	}
}
//------------------------------------------------------
</script>
<script language="JavaScript1.2" type="text/javascript">

function BookMarkThisPage(url, title) 
{


	if (window.sidebar) 
	{ 
		window.sidebar.addPanel(title, url,"");
	} 
	else if( window.external ) 
	{ 
		window.external.AddFavorite( url, title); 
	} 
	else if(window.opera && window.print) 
	{ 
		return true; 
	}

}

</script>


<script id="clienteventhandlersjs" language="javascript">
<!--

//Tom Lee IBM 3/6/03: Same day NR 9PM EST cut-off, next day Comm 12AM EST cut-off
//Tom Lee IBM 7/1/03: Same day Comm 9PM EST cut-off
//Tom Lee IBM 12/19/03: Same day NR 5PM EST cut-off, same day Comm 5PM EST cut-off
	var curStatComm = false;
	var curStatNR = false;
	var sSysDiffComm = parseInt("26500") * 1000;
	var sSysDiffNR = parseInt("26500") * 1000;
	window.setTimeout("curStatComm = true;", sSysDiffComm);
	window.setTimeout("curStatNR = true;", sSysDiffNR);


function ViewCalender()
{
 	winDate = window.open("calframe1.html","dateCal","status=yes,scrollbars=no,resizable=yes,width=375,height=500");
 	self.name = "mainWin";
}
					
//--------------------------------------------------------------------------------
//Abhijit Sen
//--------------------------------------------------------------------------------
function CheckDateValidity(Month, Day, Year)
{
   var daysinFeb;
   var Today_Day = "28";
   var Today_Month = "7";
   var Today_Year = "2006";
   var Default_Day = "29";
   var Default_Month = "7";
   var Default_Year = "2006";
   
   var dTodaysDate, dFirstResDate, dPassedDate;
   
   //In Javascript the month Jan is 00 so we subtract 1
   dTodaysDate = new Date(Today_Year, (Today_Month - 1), Today_Day);
   
   //In Javascript the month Jan is 00 so we subtract 1
   dFirstResDate = new Date(Default_Year, (Default_Month - 1), Default_Day);
     
   //Validating the date that the user selected and passed to this function  
   if (Month == 2)
   {
		//Abhijit Sen - PWC
		//Checking for leap year and 29/28 days for february
		
		//Feb cannot have more than 29 days for any given year
		if (Day > 29)
		{
			alert("Invalid Arrival Date");
			return false;
		}
		else
		{
			//If year mod 4 != 0 then use 28 days for Feb
			if (((Year % 4) != 0))
				daysinFeb = 28;
			else
			{	
				//If year mod 400 = 0 then use 29 days for Feb
				if (((Year % 400) == 0))
					daysinFeb = 29;
				else
				{
					//If year mod 100 = 0 then use 28 days for Feb
					if (((Year % 100) == 0) && (Day >= 29))
						daysinFeb = 28;
					else
						daysinFeb = 29;
				}
			}
			if (Day > daysinFeb)
			{
				alert("Invalid Arrival Date");
				return false;
			}
		}
   }      
   else
   {
      if (((Month == 4) || (Month == 6) || (Month == 9) || (Month == 11)) && (Day > 30))
	  {
			alert("Invalid Arrival Date");
			return false;
      }
   }  

   //Check to see the city. If it is NetRate then return true else return false
   var bIsNetRate;
   var sMarketType = "";
   
   if (sMarketType == "N")
		bIsNetRate = true;
	else
		bIsNetRate = false;
	

   if (Year < Today_Year)
   {
	  if (bIsNetRate)
	  {
		alert ("Arrival Date has to be on or after " + Today_Month + "/" + Today_Day + "/" + Today_Year);
		return false;
	  }
	  else
	  {
		alert ("Arrival Date has to be on or after " + Default_Month + "/" + Default_Day + "/" + Default_Year);
		return false;	  
	  }
   }   
      
   if (Today_Year == Year && Month < Today_Month)
   {
	  if (bIsNetRate)
	  {
		alert ("Arrival Date has to be on or after " + Today_Month + "/" + Today_Day + "/" + Today_Year);
		return false;
	  }
	  else
	  {
		alert ("Arrival Date has to be on or after " + Default_Month + "/" + Default_Day + "/" + Default_Year);
		return false;	  
	  }
   }   
    
   //Tom Lee IBM 3/6/03: Same day NR 9PM EST cut-off
   //Tom Lee IBM 12/19/03: Same day NR 5PM EST cut-off
   if (Today_Year == Year && Today_Month == Month && Today_Day == Day && curStatNR)
   {
	  if (bIsNetRate)
	  {
		alert ("This same day reservation is valid only before 5PM EST");
		return false;
	  }
   }   
   
   //Tom Lee IBM 3/6/03: Next day Comm 12AM EST cut-off
   //Tom Lee IBM 7/1/03: Same day Comm 9PM EST cut-off
   //Tom Lee IBM 12/19/03: Same day Comm 5PM EST cut-off
//   if (Today_Year == Year && Today_Month == Month && Today_Day == Day - 1 && curStatComm)
   if (Today_Year == Year && Today_Month == Month && Today_Day == Day && curStatComm)
   {
	  if (!bIsNetRate)
	  {
		alert ("This same day reservation is valid only before 5PM EST");
		return false;
	  }
   }   
   
	//Modified by Abhijit Sen - PWC
	//This has been modified to do same day reservations for net rate cities
    //In Javascript the month Jan is 00 so we subtract 1
    dPassedDate = new Date(Year, (Month -1), Day);

	if (bIsNetRate)
	{
		//Same day reservation for net rate
		if (dPassedDate >= dTodaysDate)
			return true;
		else
		{
			alert("Arrival Date has to be on or after " + Today_Month + "/" + Today_Day + "/" + Today_Year);
			return false;
		}
	}
	else
	{
		//PWC Abhijit Sen - May 15th 2002.
		//Since autofaxing went into production they want reservations to be made for 
		//any day >= (System day + 1) for commission hotels (Tom Lee 3/5/03: was + 2)
		if (dPassedDate >= dFirstResDate)
			return true;
		else
		{
			//Tom Lee IBM 7/1/03: old
			//if ((dPassedDate < dFirstResDate) && (dPassedDate >= dTodaysDate))
			//Tom Lee IBM 7/1/03: new
			if ((dPassedDate < dFirstResDate) && (dPassedDate > dTodaysDate))
			{
				alert('Because your arrival date is so near, we need to complete this reservation by phone to assure your satisfaction. Please call 800-789-9887 now and a Quikbook reservation agent will help you. (You can make online reservations for arrival on or after 7/29/2006. If your date is flexible click OK, enter the new date, and continue online.)');
				return false;
			}
			else
			{
				//Tom Lee IBM 7/1/03
				if (dPassedDate >= dTodaysDate)
					return true;

				if (dPassedDate < dTodaysDate)
				{
					alert ("Arrival Date has to be on or after " + Default_Month + "/" + Default_Day + "/" + Default_Year);
					return false;
				}	
			}	
		}
	}
}


function submit1_onclick() 
{
	mm = window.document.frmBook.month.selectedIndex  ;
	dd = window.document.frmBook.day.selectedIndex ;
	yy = window.document.frmBook.year.selectedIndex ;
	
	Month = parseInt(window.document.frmBook.month.options[mm].value,10) ;
	Day = parseInt(window.document.frmBook.day.options[dd].value,10) ;
	Year  = parseInt(window.document.frmBook.year.options[yy].value,10) ;
	
	if (CheckDateValidity(Month, Day, Year) == true)
	{
		document.avail_search_mix.txtDate.value = Month + "/" + Day + "/" + Year
		document.avail_search_mix.txtNights.value = document.frmBook.nights.options[document.frmBook.nights.selectedIndex].value
		document.avail_search_mix.txtnoadults.value = document.frmBook.adults.options[document.frmBook.adults.selectedIndex].value
		document.avail_search_mix.txtnochild.value = document.frmBook.children.options[document.frmBook.children.selectedIndex].value
		document.avail_search_mix.txtRooms.value = document.frmBook.rooms.options[document.frmBook.rooms.selectedIndex].value
		document.avail_search_mix.submit();
	}
}

//-->
</script>



