	<script language="JavaScript">
		var packArray = new Array();
		var displayArray = new Array();
		var submitArray = new Array();
		displayArray[0] = 156735;
		function getPack() {
			var i = 0;
			var j = 0;
			var k = 0;
			var v_found = false;
			var v_add = '';
			var v_remove = '';
			i = 0;
			for (i; i < document.miniguide_pack.pack.length; i++) {
				if (document.miniguide_pack.pack[i].checked) {
					submitArray[j] = document.forms.miniguide_pack.pack[i].value;
					j = j + 1;
				}
			}
			i = 0;
			for (i; i < submitArray.length; i++) {
				j = 0;
				v_found = false;
				for (j; j < packArray.length; j++) {
					if (submitArray[i] = packArray[j]) {
						v_found = true;
					}
				}
				if (!v_found) {
					if (v_add.length != 0) {
						v_add = v_add + ',';
					}
					v_add = v_add + submitArray[i];
				}
			}
			document.miniguide_nav.pack_add.value = v_add;
			i = 0;
			for (i; i < displayArray.length; i++) {
				j = 0;
				for (j; j < packArray.length; i++) {
					if (displayArray[i] = packArray[j]) {
						k = 0;
						for (k; k < submitArray.length; i++) {
							if (displayArray[i] = submitArray[k]) {
								if (v_remove.length != 0) {
									v_remove = v_remove + ',';
								}
								v_remove = v_remove + submitArray[k];
							}
						}
					}
				}
			}
			document.miniguide_nav.pack_remove.value = v_remove;
		}
		function gotoPage( p_pg) {
			document.forms.miniguide_nav.pg.value = p_pg;
			document.forms.miniguide_nav.submit();
		}
		function sortPage( p_sort) {
			document.forms.miniguide_nav.sort.value = p_sort;
			document.forms.miniguide_nav.pg.value = 1;
			document.forms.miniguide_nav.submit();
		}
		function savePage() {
			getPack();
			document.forms.miniguide_nav.submit();
		}
	</script>
