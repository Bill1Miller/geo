function doTellYourFriends()
{
    var tgtUrl = "/TellFriends-xHotel+Fusion+__2D__+San+Francisco+__2D__+Hotel+Fusion+Reviews+__2D__+TripAdvisor-w__2F__Hotel_Review__2D__g60713__2D__d243535__2D__Reviews__2D__Hotel_Fusion__2D__San_Francisco_California__2E__html";

    window.open ( tgtUrl, "TellYourFriends", "toolbar=0,resizable=1,menubar=0,location=0,status=0,scrollbars=3,width=580,height=430,screenX=30,screenY=25,left=30,top=25");
}

function showCascadeSearch(sHotelID, bFromPop, sLinkType)
{
    if(sHotelID == null || sHotelID == '')
    {
        return;
    }
    var tgtUrl = "/HotelDateSearch?d=" + sHotelID + "&returnTo=" + "__2F__Hotel_Review__2D__g60713__2D__d243535__2D__Reviews__2D__Hotel_Fusion__2D__San_Francisco_California__2E__html" + "&fromPop=" + bFromPop;
    var sFromSource = 'Hotel_Review';
    if(sFromSource !="")
    {
        
        if(sLinkType && sLinkType != "")
        {
          sFromSource += sLinkType;
        }

        tgtUrl += "&from=" + sFromSource;
    }


        var w = window.open ( tgtUrl, "CascadeSearch", "toolbar=0,resizable=1,menubar=0,location=0,status=0,scrollbars=1,width=245,height=610,screenX=5,screenY=5,left=5,top=5");
  

    if (w != null)
    {
        w.opener = self;
        w.focus();
    }
}

