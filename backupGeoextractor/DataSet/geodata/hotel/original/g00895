					function showMessage(frmObj, message)
					{
						alert(message);
					        if (frmObj.type == "hidden")
					           return false;
						else{
					          frmObj.focus();
						  return false;
						}
					}
					
					//Checks if required information has been entered
					function CheckRequired(frmObj, desc)
					{
						if (arguments.length != 2)
							{
								if (frmObj.id != "")
									desc = frmObj.id;
								else
									desc = "This";
							}
						switch (frmObj.type)
						{
							case "text":
								if (frmObj.value == "")
									return showMessage(frmObj, desc + " is a required field.");
								else
									return true; 
								break;
								
							case "password":
								if (frmObj.value == "")
									return showMessage(frmObj, desc + " is a required field.");
								else
									return true; 
								break;
					
							case "checkbox":
								return true;
								break;
								
							case "select-multiple":
								if (frmObj.options[frmObj.selectedIndex].value == 0)
									return showMessage(frmObj, desc + " is a required field.");
								else
									return true; 
								break;
								
							case "select-one":
								if (frmObj.options[frmObj.selectedIndex].value == 0)
									return showMessage(frmObj, desc + " is a required field.");
								else
									return true; 
								break;		
					
							case "hidden":
								if (frmObj.value == "")
									return showMessage(frmObj, desc + " is a required field.");
								else
									return true; 
								break;
						}
						switch (frmObj[0].type)
						{
							case "radio":
								for (var i=0; i < frmObj.length; i++){
					                        	if (frmObj[i].checked == true)
										return true;
								}
								return showMessage(frmObj[0], desc + " is a required field.");
								break;
						}
						return true;
					}
				
					function validateLoginForm(form)
		
					{		
						if (!CheckRequired(form.txtUserName, "User Name"))
							return false;
							
						if (!CheckRequired(form.psdPassword, "Password"))
							return false;
							
						return true;
					}
