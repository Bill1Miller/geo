        function showInfoSection(n) {

              var numGroups = 5;

      	document.getElementById("info-group" + n).className = "isShown";
		document.getElementById("info-tab" + n).className = "current";
		for (i=1; i <= numGroups; i++) {
		  if(n != i) {
		      hideInfoSection(i);
		  }
		}
      }

      function hideInfoSection(i) {
	      var groupName = 'info-group' + i;
	      var tabName = "info-tab" + i;
    		document.getElementById(groupName).className = "isHidden";
    		document.getElementById(tabName).className = "notCurrent";
      }

      function displayLess(n) {
        document.getElementById("short" + n).className = "isShown";
        document.getElementById("long" + n).className = "isHidden";
      }
      function displayMore(n) {
        document.getElementById("short" + n).className = "isHidden";
        document.getElementById("long" + n).className = "isShown";
      }

      function showAllRooms(n) {
        for(i=6; i <= n; i++) {
          document.getElementById("rate" + i).className = 'isShown';

        }
          document.getElementById("allrates").className = 'isHidden';
          document.getElementById("fewerrates").className = 'isShown';
      }

      function showFewerRooms(n) {
        for (i=6; i<=n; i++) {
          document.getElementById("rate" + i).className = 'isHidden';

        }
          document.getElementById("allrates").className = 'isShown';
          document.getElementById("fewerrates").className = 'isHidden';
      }

      function resizeImage(photo) {
       var curImg      = document.getElementById(photo);
       var imgWidth  = curImg.width;
       var imgHeight = curImg.height;

       if(imgWidth > 668) {
         var imgWidthDiff = 668/imgWidth;
         imgWidth = 668;
         imgHeight = Math.round(imgHeight*imgWidthDiff);

         curImg.width = imgWidth;
         curImg.height = imgHeight;
       }
     }


  
