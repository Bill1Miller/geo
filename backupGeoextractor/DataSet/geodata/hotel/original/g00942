gVersion="1.2";
var RE={"%09":/\t/g, "%20":/ /g, "%23":/\#/g,"%26":/\&/g,"%2B":/\+/g,"%3F":/\?/g,"%5C":/\\/g};
  
//--></SCRIPT><SCRIPT LANGUAGE="Javascript1.3"><!--
gVersion="1.3";
  
