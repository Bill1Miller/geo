    function submitSignIn(signInForm)
    {                      
        new Ajax.Updater('commentAdd', '/controls/ajax/addcomment.aspx', {method: 'post', evalScripts: true, parameters:Form.serialize(signInForm)});
    }
    function submitComment(commentForm)
    {
        if(Form.Element.getValue('commentText').length == 0) {                 
            new Ajax.Updater('commentAdd', '/controls/ajax/AddComment.aspx?error=Please fill out comment before submitting', {method: 'post', evalScripts: true});
        }
        else
        {
            $('commentError').style.display = "none";                    
            new Ajax.Updater('commentBlock', '/controls/ajax/submitcomment.aspx', {method: 'post', evalScripts: true, insertion: Insertion.Bottom, parameters:Form.serialize(commentForm) + "&ciId=371885"});                                                             
            $('commentText').value = "";                    
        }
    }    
    function displayComment(commentId)
    {
        $('commentBlock').style.display = "";
        var pars = 'ci=371885&m=36538';
        new Ajax.Updater('contentItemFeedback', '/controls/ajax/submitcontentitemfeedback.aspx', {method:'post', evalScripts: true, parameters: pars});return false;       
    }
    function submitContentItemFeedback(ciId, rating)
    {   
        var pars = 'ci=' + ciId + '&rating=' + rating;
        new Ajax.Updater('contentItemFeedback', '/controls/ajax/submitcontentitemfeedback.aspx', {method:'post', evalScripts: true, parameters: pars});return false;       
    }
    function setFavoriteContentItem(ciId, remove)
    {   
        var pars = 'ci=' + ciId;
        if(remove)
            pars += '&remove=' + remove;
        new Ajax.Updater('addToFav', '/controls/ajax/setFavoriteContentItem.aspx', {method:'post', evalScripts: true, parameters: pars});return false;       
    }
    function sendToAFriend(stfForm)
    {
        if(Form.Element.getValue('stfEmail').length == 0 || $('stfEmail').value == 'Enter an email address') {
            $('stfError').style.display = "";                    
        }
        else
        {
            $('stfError').style.display = "none";                    
            new Ajax.Updater('sendtofriend', '/controls/ajax/sendtoafriendCI.aspx', {method: 'post', evalScripts: true, parameters:Form.serialize(stfForm) + "&ciId=371885"});                                                             
            $('stfEmail').value = "";                    
        }
    }
    function clearSTF()
    {
        if($('stfEmail').value == 'Enter an email address')
            $('stfEmail').value = '';        
    }
