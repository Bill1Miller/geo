<script>
	var ns4=document.layers
	var ie4=document.all
	var ns6=document.getElementById&&!document.all
	
	function closeit(){
		if (ie4)
			document.all.wait.style.visibility="hidden"
		else if (ns6)
			document.getElementById("wait").style.visibility="hidden"
		else if (ns4)
			document.layers.wait.visibility="hide"
	}

	function showit(){
		if (ie4)
			document.all.wait.style.visibility="visible"
		else if (ns6)
			document.getElementById("wait").style.visibility="visible"
		else if (ns4)
			document.layers.wait.visibility="show"
	}

	function changeit(str1,styl1,str2,styl2) {
		if (document.getElementById && navigator.userAgent.substr(0,7) == "Mozilla") {
			document.getElementById("content1").firstChild.nodeValue = str1;
			document.getElementById("content1").className = styl1;
			document.getElementById("content2").firstChild.nodeValue = str2;
			document.getElementById("content2").className = styl2;
		}
	}
</script>
