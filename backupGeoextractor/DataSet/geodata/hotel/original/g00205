var imgArray = new Array('http://images.bookit.com/LQ793/LQ793_168682.jpg','http://images.bookit.com/LQ793/LQ793_168681.jpg','http://images.bookit.com/LQ793/LQ793_168683.jpg','http://images.bookit.com/LQ793/LQ793_168684.jpg','http://images.bookit.com/LQ793/LQ793_168685.jpg','http://images.bookit.com/LQ793/LQ793_168686.jpg','http://images.bookit.com/LQ793/LQ793_168687.jpg');
var imgndx=0;
function NextImage() {
	if(imgndx + 1 > 6)
		imgndx = 0;
	else
		imgndx++;
		
	var imageId = 'image' + imgndx;
	document.images['mainimage'].src = imgArray[imgndx];
	document.getElementById('maindesc').innerHTML = document.getElementById(imageId).alt;
}

function PrevImage() {
	if(imgndx -1 < 0)
		imgndx = 6;
	else
		imgndx--;
		
	var imageId = 'image' + imgndx;
	
	document.images['mainimage'].src = imgArray[imgndx];
	document.getElementById('maindesc').innerHTML = document.getElementById(imageId).alt;
}

function ClickedImage (imagesrc, imagedesc, cnt)
{
	document.images['mainimage'].src = imagesrc;
	document.getElementById('maindesc').innerHTML = imagedesc;
	imgndx = cnt;
}
-->
</script>
