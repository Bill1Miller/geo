	function lastday(mon, yr)
	{
		var start_date= new Date(yr, mon, 1);
		var end_date= new Date(yr, mon+1, 1);
		lday =( end_date - start_date);
		lday = lday/(24*60*60000);
		return lday;
	}
	
	function idays(m,d,y)
	{
		lday=lastday(m.options[m.selectedIndex].value-1 ,
	    y.options[y.selectedIndex].value);
		d.length=lday;
	    for(i=0;i<lday;i++)
	    {
			d.options[i].text=i+1;
			d.options[i].value=i+1;
	    }
	}
	
	function fnToday()
	{
		var strDate = new Date();
		window.document.quicksearch.checkInMonth.value=strDate.getMonth()+parseInt(1);
		window.document.quicksearch.checkInDay.value=strDate.getDate();
		window.document.quicksearch.checkInYear.value=strDate.getYear();
	}

	function fnSubmit()
	{
		var strMonth = parseInt(window.document.quicksearch.checkInMonth.options[window.document.quicksearch.checkInMonth.options.selectedIndex].value,10);
		var strDate = window.document.quicksearch.checkInDay.options[window.document.quicksearch.checkInDay.options.selectedIndex].value;
		var strYear = window.document.quicksearch.checkInYear.options[window.document.quicksearch.checkInYear.options.selectedIndex].value;
		window.document.quicksearch.arrive.value=strMonth+"/"+strDate+"/"+strYear;
		var  night = parseInt(window.document.quicksearch.Nights.options[window.document.quicksearch.Nights.options.selectedIndex].value);
		var check_Out=new Date();
		check_Out=new Date(strMonth+"/"+strDate+"/"+strYear);
		check_Out.setDate(check_Out.getDate()+night);
		var check = parseInt(check_Out.getMonth())+parseInt(1);
		window.document.quicksearch.depart.value= check+"/"+check_Out.getDate()+"/"+check_Out.getYear();
		validator();
		
		var check_In=new Date();
		check_In=new Date(strMonth+"/"+strDate+"/"+strYear);
		var strCurrentDate = new Date('07/28/2006');
		if(check_In<strCurrentDate)
		{
			alert("Check-In Date cannot be passed date.");
