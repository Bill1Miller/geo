        document.getElementById('infoMapIFRAME').src = "/metrotravelguidecom/hotels/infomap.jsp;jsessionid=qqeCON-ZFKc4?Lat=47.60107&Lon=-122.3354&Label=Best_Western_Pioneer_Square_Hotel&Id=3454&amp;trackid=10010&amp;referertrackid=10003&amp;refererpageid=8&amp;trafficID=241920122&amp;ServerID=1055&amp;subAffiliateID=0" ;
