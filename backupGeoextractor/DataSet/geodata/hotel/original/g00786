    mapIsOn = "true";
    var prop = (answer == 1) ? "visible" : "hidden";
    if (prop == "hidden"){
        document.getElementById('infoMapDIV').style.display = "none";
    }else{
        document.getElementById('infoMapDIV').style.display = "";
    }
