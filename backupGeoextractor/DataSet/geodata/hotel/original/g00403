    function openOpeningHours () {
      var width = 350;
      var height = 520;
      var name = 'openinghours';
      var url = 'http://www.dilos.com/openinghours/';
      var features = 'width='+width+',height='+height+',toolbar=no,directories=no,location=no,status=no,menubar=no,resizable=no,scrollbars';
      window.open(url, name, features);
    }
