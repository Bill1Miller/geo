<script>
function setCookie(name,value,days)
{
	var expires = "";

	if (days)
	{
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		expires = "; expires="+date.toGMTString();
	}
	document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name)
{
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++)
	{
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

function eraseCookie(name)
{
	createCookie(name,"",-1);
}


function saveCookies(form){
  if(form.ssid && form.ssid.type!="hidden"){
    setCookie("JWSearchSSID",form.ssid.value,100);
  }
  if(form.hotspot_name && form.hotspot_name.type!="hidden"){
    setCookie("JWSearchHotSpotName",form.hotspot_name.value,100);
  }
  if(form.location_type_id && form.location_type_id.type!="hidden"){
    setCookie("JWSearchLocationTypeId",form.location_type_id.value,100);
  }
  if(form.technology_id && form.technology_id.type=="select-one"){
    setCookie("JWSearchTechnologyId",form.technology_id.value,100);
  }else if(form.technology_id && form.technology_id.length>0){
    for(i=0;i<form.technology_id.length;i++){
      if(form.technology_id[i].checked){
        setCookie("JWSearchTechnologyId",form.technology_id[i].value,100);
      }
    }
  }
  if(form.venue_group_id && form.venue_group_id.type!="hidden"){
    setCookie("JWSearchVenueGroupId",form.venue_group_id.value,100);
  }
  if(form.provider_id && form.provider_id.type!="hidden"){
    setCookie("JWSearchProviderId",form.provider_id.value,100);
  }

  if(form.pay_free && form.pay_free.type=="select-one"){
    setCookie("JWSearchPayFree",form.pay_free.value,100);
  }else if(form.pay_free && form.pay_free.length>0){
    for(i=0;i<form.pay_free.length;i++){
      if(form.pay_free[i].checked){
        setCookie("JWSearchPayFree",form.pay_free[i].value,100);
      }
    }
  }

  if(form.address && form.address.type!="hidden"){
    setCookie("JWSearchAddress",form.address.value,100);
  }
  if(form.city && form.city.type!="hidden"){
    setCookie("JWSearchCity",form.city.value,100);
  }
  if(form.country_id && form.country_id.type!="hidden"){
    setCookie("JWSearchCountryId",form.country_id.value,100);
  }
  if(form.state_id && form.state_id.type!="hidden"){
    setCookie("JWSearchStateId",form.state_id.value,100);
  }
  if(form.zip && form.zip.type!="hidden"){
    setCookie("JWSearchZip",form.zip.value,100);
  }
  if(form.narrow_search && form.narrow_search.type!="hidden"){
    setCookie("JWSearchNarrowSearch",form.narrow_search.value,100);
  }
  if(form.radius && form.radius.type!="hidden"){
    setCookie("JWSearchRadius",form.radius.value,100);
  }
}


function grabCookies(form){
  if(form.venue_group_id && readCookie("JWSearchVenueGroupId")!=null && form.venue_group_id.type!="hidden"){
    form.venue_group_id.value=readCookie("JWSearchVenueGroupId");
  }
  if(form.ssid && readCookie("JWSearchSSID")!=null && form.ssid.type!="hidden"){
    form.ssid.value=readCookie("JWSearchSSID");
  }
  if(form.technology_id && readCookie("JWSearchTechnologyId")!=null && form.technology_id.type!="hidden"){
    if(form.technology_id.type=="select-one"){
      form.technology_id.value=readCookie("JWSearchTechnologyId");
    } else if(form.technology_id.length > 0){
      for(i=0;i<form.technology_id.length;i++){
        if(form.technology_id[i].value == readCookie("JWSearchTechnologyId") ){
          form.technology_id[i].checked=true;
        }
      }
    }
  }

  if(form.pay_free && readCookie("JWSearchPayFree")!=null && form.pay_free.type!="hidden"){
    if(form.pay_free.type=="select-one"){
      form.pay_free.value=readCookie("JWSearchPayFree");
    } else if(form.pay_free.length > 0){
      for(i=0;i<form.pay_free.length;i++){
        if(form.pay_free[i].value == readCookie("JWSearchPayFree") ){
          form.pay_free[i].checked=true;
        }
      }
    }
  }

  if(form.hotspot_name && readCookie("JWSearchHotSpotName")!=null && form.hotspot_name.type!="hidden"){
    form.hotspot_name.value=readCookie("JWSearchHotSpotName");
  }
  if(form.location_type_id && readCookie("JWSearchLocationTypeId")!=null && form.location_type_id.type!="hidden"){
    form.location_type_id.value=readCookie("JWSearchLocationTypeId");
  }
  if(form.provider_id && readCookie("JWSearchProviderId")!=null && form.provider_id.type!="hidden"){
    form.provider_id.value=readCookie("JWSearchProviderId");
  }

  if(form.address && readCookie("JWSearchAddress")!=null && form.address.type!="hidden"){
    form.address.value=readCookie("JWSearchAddress");
  }
  if(form.city && readCookie("JWSearchCity")!=null && form.city.type!="hidden"){
    form.city.value=readCookie("JWSearchCity");
  }
  if(form.country_id && readCookie("JWSearchCountryId")!=null && form.country_id.type!="hidden"){
    form.country_id.value=readCookie("JWSearchCountryId");
    change_state_list(form);
  }
  if(form.state_id && readCookie("JWSearchStateId")!=null && form.state_id.type!="hidden"){
    form.state_id.value=readCookie("JWSearchStateId");
  }
  if(form.zip && readCookie("JWSearchZip")!=null && form.zip.type!="hidden"){
    form.zip.value=readCookie("JWSearchZip");
  }
  if(form.narrow_search && readCookie("JWSearchNarrowSearch")!=null && form.narrow_search.type!="hidden"){
    form.narrow_search.value=readCookie("JWSearchNarrowSearch");
  }
  if(form.radius && readCookie("JWSearchRadius")!=null && form.radius.type!="hidden"){
    form.radius.value=readCookie("JWSearchRadius");
  }

}

</script>



