					var ratingWindowHandle;

					function openRatingDescription(rating) {
						var w=410;
						var h=240;
						var x = ((screen.width / 2) - (w / 2)); /*This will center the pop up horizontally*/
						var y = ((screen.height / 2) - (h / 2));  /*This will center the pop up vertiaclly*/
						ratingURL = "/hotels/" + rating + ".htm";
						ratingWindowHandle = window.open(ratingURL, 'Description', ('width=410,height=240,resizable=1' + ",top=" + y + ",left=" + x));
					}
					function closeRatingDescription() {
						if (ratingWindowHandle) {
							ratingWindowHandle.close();
						}
					}
					
