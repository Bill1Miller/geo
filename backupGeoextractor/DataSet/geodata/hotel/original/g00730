	<script language="JavaScript">
	<!-- //
	var c1 = new Object();
	var c2 = new Object();
	var b = navigator.appName;
	var v = navigator.appVersion;
	
	//alert ("b: " + b);
	//alert ("v: " + v);
	
	// check to see if the browser is NS 6 or above OR IE 5 or above
	function pass() {
		// it's NS and it's 6 and above (NS 6 has a version of 5)
		if ((b.indexOf("Netscape") >= 0) && (parseFloat(v) >= 5)) {
			return 1;
		}
		
		// it's IE
		if (b.indexOf("Microsoft") >= 0) {
			if ((v.indexOf("MSIE 5") > 0) || (v.indexOf("MSIE 6") > 0)) {
				return 1;
			}
		}
		
		return 0;
	}
	
	var myPass = pass();
	
	// display calendar
	function displayCalendar(day, month, year, whichMth) {
	
	    day     = parseInt(day);
	    month   = parseInt(month-1);
	    year    = parseInt(year);
	    var i   = 0;
	    var now = new Date();
	
	    var days         = getDaysInMonth(month+1,year);
	    var firstOfMonth = new Date (year, month, 1);
		//alert ("first of month is " + firstOfMonth);
	    var startingPos  = firstOfMonth.getDay();
		//alert ("starting position is " + startingPos);
	    days += startingPos;
		
		// prefill non-day weekdays with blanks
		var col = 0;
		document.writeln ("<tr valign=top align=center>");
		for (i = 0; i < startingPos; i++) {
	        document.writeln ("<td><font face=\"verdana, arial, sans-serif\" size=1>&nbsp;</font></td>");
			col++;
	    }
		
		// print out days of the month and grey out days older than the inputed day
	    for (i = startingPos; i < days; i++)
	    {
	        if (col == 0) { document.writeln("<tr valign=top align=center>"); }
			if ((i-startingPos+1) < day) {
	        document.write ("<td><font face=\"verdana, arial, sans-serif\" size=1>" + (i-startingPos+1) + "</font></td>");
			}
			else {
					if (myPass || (whichMth == 1)) {
	        		document.writeln ("<td><font face=\"verdana, arial, sans-serif\" size=1><a href=\"/ae/scr/nws_oth_sr.cfm?p=re&e=0&c=&wd=" + (month+1) + "/" + (i-startingPos+1) + "/" + year + "\">" + (i-startingPos+1) + "</a></font></td>");
				}
			}
	        if (col == 6) {
	           document.writeln ("</TR>")
	           col = 0
	        }
	        else {
	           col++
	        }
	    }
		    // fill up remaining blanks
	    if (col > 0) {
	    for (i=col; i<7; i++)  {
	          document.writeln ("<td><font face=\"verdana, arial, sans-serif\" size=1>&nbsp;</font></td>");
	    }
	    document.writeln ("</TR>");
		}
	}

	// get number of days in month
	function getDaysInMonth(month,year)  {
	    var days
	    switch (parseInt(month)) {
	      case 4 :
	      case 6 :
	      case 9 :
	      case 11 :
	                days=30;
	                break;
	      case 2 :
	                if (isLeapYear(year)) { days=29; }
	                else  { days=28; }
	                break;
	      default : days=31;
	    }
	    return (days);
	}

	// check for leap year
	function isLeapYear (Year) {
	    if (((Year % 4)==0) && ((Year % 100)!=0) || ((Year % 400)==0)) {
	        return (true);
	    }
	    else {
	        return (false);
	    }
	}

	
	function toggleCalendar(cnum) {
	
		if (myPass) {
		c1 = document.getElementById("calendar1");
		c2 = document.getElementById("calendar2");
		
		if (cnum == 2) {
			c1.style.display = 'none';
			c2.style.display = 'block';
		} else {
			c1.style.display = 'block';
			c2.style.display = 'none';
		}
		} 
		
	} 
	//-->
	</script>
	
	

