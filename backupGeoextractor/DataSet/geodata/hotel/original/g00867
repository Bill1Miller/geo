function set_state()
{
	var SeForm = document.resForm;
	if (SeForm.country.value != 'US' &&  SeForm.country.value != 'CA') {
		SeForm.state.options.selectedIndex =0;
	}
}


function alter_out()
{
	var inmonth = document.resForm.in_mm.options.selectedIndex;
	var inday = document.resForm.in_dd.options.selectedIndex;
	set_date(inmonth, inday);
}

function set_date (thismonth, thisday)
{
	if(thisday>=28)
	{
		if(thismonth==11)
		{
			document.resForm.out_mm.options.selectedIndex = 0;
			document.resForm.out_dd.options.selectedIndex = 0;
			document.resForm.out_yy.options.selectedIndex = document.resForm.in_yy.options.selectedIndex+1;
		}
		else{
			document.resForm.out_mm.options.selectedIndex = thismonth+1;
			document.resForm.out_dd.options.selectedIndex = 0;
			document.resForm.out_yy.options.selectedIndex =document.resForm.in_yy.options.selectedIndex;
		}
	}else
	{
		document.resForm.out_mm.options.selectedIndex = thismonth;
		document.resForm.out_dd.options.selectedIndex = thisday+1;
		document.resForm.out_yy.options.selectedIndex =document.resForm.in_yy.options.selectedIndex;
	}

}
