function hbxStrip(a) {
   a = a.split("|").join("");
   a = a.split("&").join("");
   a = a.split("'").join("");
   a = a.split("#").join("");
   a = a.split("$").join("");
   a = a.split("%").join("");
   a = a.split("^").join("");
   a = a.split("*").join("");
   a = a.split(":").join("");
   a = a.split("~").join("");
   a = a.split(";").join("");
   a = a.split(" ").join("+");
   return a;
}

