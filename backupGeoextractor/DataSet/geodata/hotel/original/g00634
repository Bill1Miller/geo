<script language="javascript">
function loadDefaultSearchDates() {

        var hotelArrival = new Date();
        var hotelDeparture = new Date();

        var hotelAdvanceArrival = 21;
        var hotelAdvanceDeparture = 23;

        hotelArrival.setDate( hotelArrival.getDate() + hotelAdvanceArrival );
        hotelDeparture.setDate( hotelDeparture.getDate() + hotelAdvanceDeparture );

        document.frm_Search.arrivalMonth.value = hotelArrival.getMonth();
        document.frm_Search.departureMonth.value = hotelDeparture.getMonth();
        document.frm_Search.arrivalDay.value = hotelArrival.getDate();
        document.frm_Search.departureDay.value = hotelDeparture.getDate();

        amadChange(document.frm_Search.arrivalMonth, document.frm_Search.arrivalDay, document.frm_Search.departureMonth, document.frm_Search.departureDay);
    }

loadDefaultSearchDates();
</script>
