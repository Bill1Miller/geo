function frmsubmit() {
var frm = document.forms.frmsearch;
var mon = frm.selMonth.options[frm.selMonth.selectedIndex].value;
var day = frm.selDay.options[frm.selDay.selectedIndex].value;
var year = frm.selYear.options[frm.selYear.selectedIndex].value;
var cki = mon + '/' + day + '/' + year;

var nit = frm.selNights.options[frm.selNights.selectedIndex].text;
var rms = frm.selNights.options[frm.selRooms.selectedIndex].text;
var sUrl = 'http://www.hotelsanddeals.com/geo.php?LBL=Seattle Convention Center&ST=WA&CT=US&LON=-122.3316219&LAT=47.61106897&RAD=5' + '&cki=' + cki + '&nit=' + nit + '&rms=' + rms + '&cid=q1026';

document.location.href = sUrl;
return false;
}
