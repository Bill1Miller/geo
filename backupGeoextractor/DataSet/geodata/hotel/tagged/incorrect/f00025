<table cellspacing="0" width="100%" cellpadding="0" border="0"><tr><td><img src="http://images.myareaguide.com/images/blank.gif" width="1" height="12"></td></tr></table><table cellspacing="0" width="100%" cellpadding="0" border="0" xmlns:myObj="urn:myObj"><tr><td><img src="http://images.myareaguide.com/images/blank.gif" width="1" height="12"></td></tr></table><img width="1" height="1" src="http://track.myareaguide.com/agsusacallsnlsngl/10/go.img?s=514F6EAAF705EC2AD23C32B51ED4D028" xmlns:myObj="urn:myObj"><script language="JavaScript" src="http://content.myareaguide.com/js/simplecalendar.js" type="text/javascript" xmlns:myObj="urn:myObj"></script><script language="javascript" xmlns:myObj="urn:myObj">

<!--
function SetCookie(sName, sValue)
{
  date = new Date();
  document.cookie = sName + "=" + escape(sValue);
}

function GetCookie(sName)
{
  // cookies are separated by semicolons
  var aCookie = document.cookie.split("; ");
  for (var i=0; i < aCookie.length; i++)
  {
    // a name/value pair (a crumb) is separated by an equal sign
    var aCrumb = aCookie[i].split("=");
    if (sName == aCrumb[0]) 
      return unescape(aCrumb[1]);
  }

  // a cookie with the requested name does not exist
  return null;
}

var months = new Array('Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec');
function assigndefaults()
{
	var checkInDate = GetCookie('CheckInDate');
	var checkOutDate = GetCookie('CheckOutDate');
	if (checkInDate && checkOutDate)
	{
		AssignToAll(checkInDate,checkOutDate)
	}
}
function AssignToAll(checkInDate, checkOutDate)
{
	indates = document.all['InDate'];
	outdates = document.all['OutDate'];
	for(i=0; i<indates.length; i++)
	{
		indates[i].value = checkInDate;
	}
	for(i=0; i<outdates.length; i++)
	{
		outdates[i].value = checkOutDate;
	}
}
function validate(form)
{
	//debugger;
	var indate = Date.parse(form.InDate.value);
	var outdate = Date.parse(form.OutDate.value);
	if (indate && outdate)
	{
		SetCookie('CheckInDate',form.InDate.value);
		SetCookie('CheckOutDate',form.OutDate.value);
		indate = new Date(indate);
		outdate = new Date(outdate);
		AssignToAll(form.InDate.value,form.OutDate.value);
		return true;
	}
	else
	{
		alert('You must provide a check-in date and check-out date');
		return false;
	}
}
function MM_openBrWindow(theURL) { //v2.0
  window.open(theURL.replace('%Time%',new Date().getTime()),'_blank','toolbar=yes,location=yes,status=yes,menubar=yes,scrollbars=yes,resizable=yes,width=800,height=600');
}
