var doneLoad = false;
var trySwap  = 0;

tab0on      = new Image();
tab0on.src  = "/images/tabs/hotel_information_on.png";
tab0off     = new Image();
tab0off.src = "/images/tabs/hotel_information_off.png";
tab1on      = new Image();
tab1on.src  = "/images/tabs/hotel_map_on.png";
tab1off     = new Image();
tab1off.src = "/images/tabs/hotel_map_off.png";
tab2on      = new Image();
tab2on.src  = "/images/tabs/amenities_on.png";
tab2off     = new Image();
tab2off.src = "/images/tabs/amenities_off.png";
tab3on      = new Image();
tab3on.src  = "/images/tabs/hotel_policies_on.png";
tab3off     = new Image();
tab3off.src = "/images/tabs/hotel_policies_off.png";
tab4on      = new Image();
tab4on.src  = "/images/tabs/hotel_reviews_on.png";
tab4off     = new Image();
tab4off.src = "/images/tabs/hotel_reviews_off.png";

var currentTab = 0;

function swapTab(num){
    if (doneLoad){
        eval('document.images.tab' + currentTab + '.src = tab' + currentTab + 'off.src');
        currentTab = num;
        eval('document.images.tab' + num + '.src = tab' + num + 'on.src');
        for(l=0;l<5;l++) {
            if(document.getElementById('tabContent' + l)!=null)
                document.getElementById('tabContent' + l).style.display = 'none';
        }
        if(document.getElementById('tabContent' + num)){
            document.getElementById('tabContent' + num).style.display = 'inline';
        }
    } else {
        setTimeout('swapTab('+num+')',1000);
    }
}


function format_date(adate) {
	var d = adate.getDate();
	var dLength = d.toString().length;
	if (dLength < 2) d = '0' + d;
	var m = adate.getMonth()+1;
	var mLength = m.toString().length;
	if (mLength < 2) m = '0' + m;
	var y = adate.getYear().toString().substr(2,2);
    return m + '/' + d + '/' + y;
}

var rightNow = new Date();
var defaultCheckInDate = new Date(rightNow.getTime() + (14 * 86400000));
defaultCheckInDate = format_date(defaultCheckInDate);  // format date
var defaultCheckOutDate = new Date(rightNow.getTime() + (16 * 86400000));
defaultCheckOutDate = format_date(defaultCheckOutDate); // format date

function set_dates() {
    var getCheckInDate = document.HotelSearchAddressForm.checkinDate.value;
    var getCheckOutDate = document.HotelSearchAddressForm.checkoutDate.value
    if(getCheckInDate=='mm/dd/yy') document.HotelSearchAddressForm.checkinDate.value = defaultCheckInDate;
    if(getCheckOutDate=='mm/dd/yy') document.HotelSearchAddressForm.checkoutDate.value = defaultCheckOutDate;
}

var err = 0;
var loadFlag = false;
var gallery  = new Array("22682_0"  , "22682_1"  , "22682_2"  , "22682_3"  , "22682_4"  , "22682_5"  , "22682_6"  , "22682_7");

function preLoad(what){
	var myPic = new Image();
	myPic.src = what;
	return myPic;
};

var imgNum = 0;
function swap(clicked){
    var imgLink = document.getElementById('imgLink' + imgNum);
    imgLink.style.color = '';
    imgLink.style.textDecoration = 'underline';
    imgNum = clicked;
    imgLink = document.getElementById('imgLink' + imgNum);
    imgLink.style.color = '#ff6600';
    imgLink.style.textDecoration = 'none';
    if (loadFlag && document.images){
        document.images.bigpic.src = pic[clicked].src;
	}
};

var numImgs = 7;

function nextSlide() {
var nextNum = imgNum + 1;
if (nextNum == numImgs || nextNum == 6) nextNum = 0;
swap(nextNum);
}

// Preload Slide Show Control Icons
var playBtnOn = new Image();
playBtnOn.src = '/images/buttons/play_on.png';
var playBtnOff = new Image();
playBtnOff.src = '/images/buttons/play_off.png';
var stopBtnOn = new Image();
stopBtnOn.src = '/images/buttons/stop_on.png';
var stopBtnOff = new Image();
stopBtnOff.src = '/images/buttons/stop_off.png';

var currentAction = 'stop';

function slideShow(cmd) {
    var action = cmd;

    if (action=='play') {
        if (currentAction=='play') return false;
        currentAction = action;
        show = setInterval("nextSlide()", 4000);
        document.images.play.src = playBtnOn.src;
        document.images.stop.src = stopBtnOff.src;
        } else {
        if (currentAction=='stop') return false;
        currentAction = action;
        clearInterval(show);
        document.images.play.src = playBtnOff.src;
        document.images.stop.src = stopBtnOn.src ;
        }
}

function getLrg(supp){
	window.open('/hotel/lrg_' + supp + '.html','lrg','width=300px; height=250px; scrollbars=1;')
};

function loadPics(){
	pic    = new Array();
	for (p=0; p<gallery.length; p++)
	{
	    pic[p] = preLoad('http://images.sidestep.com/c/img/website/hotels/suppliers/GTP/GT/' + gallery[p] + '.jpg');
	}
    loadFlag = true;
};

function errPic(){
    if (((gallery.length - 1 <= 6 && err == gallery.length - 1) || (gallery.length - 1 > 6 && err >= 6))) {
        if(document.getElementById("picInf")!=null)
            document.getElementById("picInf").style.display = "none";

    }
}
function switchTab(tabname){
    var loc = window.location.href;
    if(loc.indexOf('?') > 0){
        if(loc.indexOf('tab=') > 0){
            var params = loc.substring(loc.indexOf('?')+1,loc.length).split('&');
            for(var i=0;i<params.length;i++){
                var p = params[i];
                if(p.indexOf('tab=')==0){
                    params[i] = 'tab='+tabname;
                    break;
                }
            }
            var newparams = params.join('&');
            loc = loc.substring(0,loc.indexOf('?')+1) + newparams;
        }
        else
            loc += '&tab=' + tabname;
    }
    else
        loc += '?tab=' + tabname;

    window.location.href = loc;

}

function initPage(){
    loadPics();
    errPic();
    doneLoad = true
}



</script>
