<script type="text/javascript">

function paddZero(s)
{
	if(s >= 0 && s <= 9){
	  s = '0' + s;
	}
	return s;
}

function setDefaults() {

    /*
	if ( document.multiForm.arrivalDate.value == '')
		document.multiForm.arrivalDate.value = formatDate(new Date());

	if ( document.multiForm.departureDate.value == '')
		document.multiForm.departureDate.value = formatDate(getNextDate(new Date()));
    */
    //SS 03.11.2005 CQ34930
    // make the checkin date one day in the future
    var loCheckinDate = getNextDate(new Date());

    if ( document.multiForm.arrivalDate.value == ''){
        document.multiForm.arrivalDate.value = formatDate(loCheckinDate);
    } else {
        loCheckinDate = createDate(document.multiForm.arrivalDate.value);
    }

    if ( document.multiForm.departureDate.value == ''){
        // make the checkout date 1 day after the check in date
        var loCheckoutDate = (getNextDate(loCheckinDate));
        document.multiForm.departureDate.value = formatDate(loCheckoutDate);
    }

	/* BEGIN FIX FOR CQ 28196 */
	if ( document.tab1Form != null ) {
		ftemp = f;
		f = document.multiForm;
		getLengthOfStay(document.tab1Form.ciDate.value, document.tab1Form.coDate.value);
		f = ftemp;
	}
	/* END FIX FOR CQ 28196 */
	else if ( document.multiForm.lengthOfStay.value == '' || document.multiForm.lengthOfStay.value == '0')
		document.multiForm.lengthOfStay.value = "01";

	if ( document.multiForm.numberOfRooms.value == '' || document.multiForm.numberOfRooms.value == '0')
		document.multiForm.numberOfRooms.value = "01";

	if ( document.multiForm.numberOfAdults.value == '' || document.multiForm.numberOfRooms.value == '0')
		document.multiForm.numberOfAdults.value = "01";

	if ( document.multiForm.roomOccupancyTotal.value == ''  || document.multiForm.numberOfRooms.value == '0')  // the same as numberOfAdults
		document.multiForm.roomOccupancyTotal.value = '01';

	if ( document.multiForm.iATANumber.value == '')
		document.multiForm.iATANumber.value = "";

	if ( document.multiForm.corporateAccountNumber.value == '')
		document.multiForm.corporateAccountNumber.value = "";

}

function formatDate(date_obj ) {
	formattedDate = date_obj.getFullYear() + '-' + paddZero(date_obj.getMonth() + 1) + '-' + paddZero(date_obj.getDate());
	return formattedDate;
}

function createDate(dateString) {
    var valueArray = dateString.split("-");
    return new Date(valueArray[0], valueArray[1]-1, valueArray[2]);
}

function getNextDate( currentDate_obj ) {
		oneDayInMillis =  86400000; //24 * 60 * 60 * 1000;
		nextDateMillis = currentDate_obj.getTime() + oneDayInMillis;

		return new Date( nextDateMillis );
}

// diff in days between two dates
function getDiffDaysNumber(indate,outdate)
{
  		// this will give us a float i.e something like 7.9583333.. for 8 days
  		var df =  ( outdate.getTime()-indate.getTime() ) / (1000 * 60 * 60 * 24);

  		// convert to a whole number
  		return Math.round(df);

}

</script>


