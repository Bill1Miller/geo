<!-- begin property page javascript functions -->
<script type="text/javascript">
var fromSearch = '';
var cat = 'HTLOV';
var locat = 'search';

function gotoPage(page) {
	document.mainForm.action = page;
	document.mainForm.submit();
	return true;
}

function goToRooms(page) {
	document.mainForm.action = page;
	document.mainForm.submit();
	return true;
}

//go to the preference page
function gotoPref(page) {
	if (fromSearch == '78XX') document.mainForm.action = "/westin/meetings/pre_decider_all.html";
	else {
		document.mainForm.action = page;
		if (fromSearch == '22XX' || fromSearch == '78XX' || fromSearch == '22SPG') {
			document.mainForm.requestedAffiliationCode.value = '';
			document.mainForm.requestedChainCode.value = '';
		}
	}
	if (locat == 'starpoints') document.mainForm.action = "/westin/starpoints/redeem_dates_prefs.html";

	document.mainForm.submit();
	return true;
}

function nextFloor() {
	document.mainForm.action = "floor_capacity.html";

	if (document.mainForm.floorVal == null || document.mainForm.floorVal.value == "") {
		document.mainForm.floorVal.value = 1;
		document.mainForm.floorVal.value = parseInt(document.mainForm.floorVal.value) + 1;

		document.mainForm.submit();
		return true;
	} else {
		document.mainForm.floorVal.value = parseInt(document.mainForm.floorVal.value) + 1;
   		document.mainForm.submit();
   		return true;
	}
}

function prevFloor() {
	document.mainForm.action = "floor_capacity.html";
	document.mainForm.floorVal.value = parseInt(document.mainForm.floorVal.value) -1;
	document.mainForm.submit();
	return true;
}

function getRateList(propid, brand, chainCode, affiliationCode) {
    document.multiForm.propertyID.value = propid;
    document.multiForm.requestedChainCode.value = chainCode;
    document.multiForm.requestedAffiliationCode.value = affiliationCode;
}

function getPrefsAll(propid, chain, affl, cat, minPts, maxPts) {
	document.multiForm.propertyID.value = propid;
	document.multiForm.requestedChainCode.value = chain;
	document.multiForm.requestedAffiliationCode.value = affl;
	document.multiForm.categoryNumber.value = cat;
	document.multiForm.minPoints.value = minPts;
	document.multiForm.maxPoints.value = maxPts;
	document.multiForm.action = "/preferredguest/starpoints/redeem_dates_prefs.html";
	document.multiForm.submit();
	return true;
}
</script>


<!-- end property page javascript functions -->

