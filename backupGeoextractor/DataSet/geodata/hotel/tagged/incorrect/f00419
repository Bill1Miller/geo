<script language="Javascript">

var now = new Date();
var month = now.getMonth();
var day = now.getDate();
var year = now.getYear();
function setDrops(theForm) {
   // I assume Day and months are all there and start with "Select a Day and Select a month
   HotelSearch.doa_mm.selectedIndex = month;
   HotelSearch.doa_dd.selectedIndex = day;
   HotelSearch.dod_mm.selectedIndex = month;
   HotelSearch.dod_dd.selectedIndex = day;
   
   HotelSearch.doa_yy.selectedIndex = 0;
   HotelSearch.dod_yy.selectedIndex = 0;
   
}


</script>

