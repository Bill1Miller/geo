<script language="javascript">

formName='availSearch';

var tab_avail_0 = new Image();
var tab_avail_1 = new Image();
var tab_reward_0 = new Image();
var tab_reward_1 = new Image();

tab_avail_0.src = "/en/common/media/images/tabs/tab_hotel_avail.gif";
tab_avail_1.src = "/en/common/media/images/tabs/tab_hotel_avail_on.gif";
tab_reward_0.src = "/en/common/media/images/tabs/tab_hotel_reward.gif";
tab_reward_1.src = "/en/common/media/images/tabs/tab_hotel_reward_on.gif";

function switchAvailDates()
{
document.availSearch['checkInDay'].selectedIndex = document.rewardSearch['checkInDay'].selectedIndex;
document.availSearch['checkOutDay'].selectedIndex = document.rewardSearch['checkOutDay'].selectedIndex;
document.availSearch['checkInMonthYr'].selectedIndex = document.rewardSearch['checkInMonthYr'].selectedIndex;
document.availSearch['checkOutMonthYr'].selectedIndex = document.rewardSearch['checkOutMonthYr'].selectedIndex;
}

function switchRewardDates()
{
document.rewardSearch['checkInDay'].selectedIndex = document.availSearch['checkInDay'].selectedIndex;
document.rewardSearch['checkOutDay'].selectedIndex = document.availSearch['checkOutDay'].selectedIndex;
document.rewardSearch['checkInMonthYr'].selectedIndex = document.availSearch['checkInMonthYr'].selectedIndex;
document.rewardSearch['checkOutMonthYr'].selectedIndex = document.availSearch['checkOutMonthYr'].selectedIndex;
}
</script>
