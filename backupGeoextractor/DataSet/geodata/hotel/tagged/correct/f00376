<link rel="stylesheet" href="/en/hi/hotels/search/hhonors/hh_search.css" type="text/css">



<script language="javascript">

function setSearchForm(form)
{
	formName = form;
}

function setSearchType(type)
{
	document.forms[formName].searchType = type;
}

function showStatic()
{
	toggleDiv("table_flex_dates_", 0);
	toggleDiv("table_static_dates_", 1);
	document.forms[formName].fixedSearch.value = "true";
	document.forms[formName].flexibleSearch.value = "false";
}

function showFlex()
{
	toggleDiv("table_flex_dates_", 1);
	toggleDiv("table_static_dates_", 0);
	document.forms[formName].flexibleSearch.value = "true";
	document.forms[formName].fixedSearch.value = "false";
}

function setStatic() {
	document.forms[formName].fixedSearch.value = "true";
	document.forms[formName].flexibleSearch.value = "false";
	if (document.forms[formName].searchType2) {
	document.forms[formName].searchType2.value = "fixed";
	}
}

function setFlex() {
	document.forms[formName].flexibleSearch.value = "true";
	document.forms[formName].fixedSearch.value = "false";
	if (document.forms[formName].searchType2) {
	document.forms[formName].searchType2.value = "flexible";
	}
}

</script>

