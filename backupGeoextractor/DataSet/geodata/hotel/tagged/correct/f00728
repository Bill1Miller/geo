<script language="JavaScript">

function ValidForm() {

  // validate name
  if (document.newsletter.uname.value == "Add your name") {
    alert('Please enter a name!');
    document.newsletter.uname.focus();
    return false;
  }

  // validate email
  else if (document.newsletter.uemail.value == "") {
    alert("Please enter an email address!");
    document.newsletter.uemail.focus();
    return false;
  }
  else if (! document.newsletter.uemail.value.match("^.+@.+\..+$")) {
    alert("Please enter a valid email address!");
    document.newsletter.uemail.focus();
    return false;
  }      
// all is well
  else {
    return true;
  }
}
</script>
