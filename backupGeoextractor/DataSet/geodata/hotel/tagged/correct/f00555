function BNBSearch() {
	state = document.bnbstate.state.value;
	if (state != '') {
		location.href = "http://www.yankeemagazine.com/travel/search/index.php?bnb=Y&internal=Y&state=" + state;
	}
	else {
		alert('Select a New England state from the list.');
	}
}
</script>
