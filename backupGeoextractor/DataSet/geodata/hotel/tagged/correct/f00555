	function Definition(term) {
	
		var explanation = '';
		
		if (term == 'A/C') {
			explanation = 'Air conditioning';
		}
		else if (term == 'SF') {
			explanation = 'Smoke free';
		}
		else if (term == 'NP') {
			explanation = 'No pets allowed';
		}
		else if (term == 'HA') {
			explanation = 'Some handicapped accessibility available.  The historic architecture of many New England buildings makes it hard to ascertain if handicap accessibility fully meets the guidelines of the Americans with Disabilities Act.  Please call ahead for information.';
		}
		else if (term == 'L') {
			explanation = 'Inspected and/or licensed by the appropriate local or municipal association or organization.';
		}
		else if (term == 'EP') {
			explanation = 'European Plan; rate include no meals.';
		}
		else if (term == 'B&B') {
			explanation = 'Bed-and-Breakfast; rate includes breakfast.';
		}
		else if (term == 'MAP') {
			explanation = 'Modified American Plan; rate includes breakfast and dinner.';
		}
		else if (term == 'VISA') {
			explanation = 'Visa cards accepted.';
		}
		else if (term == 'MC') {
			explanation = 'MasterCard cards accepted.';
		}
		else if (term == 'AMEX') {
			explanation = 'American Express cards accepted.';
		}
		else if (term == 'CB') {
			explanation = 'Carte Blanche cards accepted.';
		}
		else if (term == 'DISC') {
			explanation = 'Discover cards accepted.';
		}
		else if (term == 'DINERS') {
			explanation = 'Diners Club cards accepted.';
		}	
		else if (term == 'NOTE') {
			explanation = 'If your event takes place on numerous dates that are not continuous, please enter the first date as the event start date and the last date as the event end date. In the listing text, you may list the complete schedule for the event, but please only enter one record for the full event, not a separate submission for each date.';
		}
		alert(term + ": " + explanation);
	}

