var gImages=new Array;
var gIndex=0;
var DCS=new Object();
var WT=new Object();
var DCSext=new Object();

var gDomain="statse.webtrendslive.com";
var gDcsId="dcsuy22seau4fibba68ia3qp2_7e5d";
// Add customizations here
//WT.sp="@@SPLITVALUE@@";
