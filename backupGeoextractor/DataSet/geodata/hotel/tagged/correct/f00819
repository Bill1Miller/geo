<script type="text/javascript" >
var imageUrlArray = new Array("/props/48128/48128_b1.jpg","/props/48128/48128_pbimg3.jpg","/props/48128/48128_pbimg5.jpg","/props/48128/48128_pbimg6.jpg","/props/48128/48128_pbimg7.jpg","/props/48128/48128_pbimg8.jpg","/props/48128/48128_pbimg9.jpg","/props/48128/48128_pbimg10.jpg","/props/48128/48128_pbimg11.jpg","/props/48128/48128_pbimg12.jpg","/props/48128/48128_pbimg13.jpg","/props/48128/48128_pbimg15.jpg","/props/48128/48128_pbimg17.jpg","/props/48128/48128_pbimg18.jpg","/props/48128/48128_pbimg19.jpg","/props/48128/48128_pbimg20.jpg","/props/48128/48128_pbimg21.jpg","/props/48128/48128_pbimg22.jpg","/props/48128/48128_pbimg23.jpg","/props/48128/48128_pbimg25.jpg","/props/48128/48128_pbimg40.jpg","/props/48128/48128_pbimg41.jpg","/props/48128/48128_pbimg42.jpg","/props/48128/48128_pbimg43.jpg","/props/48128/48128_pbimg44.jpg");
var descArray = new Array("Best Western Pioneer Square Hotel","","","","","","","","","","","","","","","","","","","","Front of Al Boccalino&#039;s","Al Boccalino&#039;s","Private Parties","","");
var currentImageIndex = 0;

function getUrl(id)
{
    try {
        var listSize = 25;
        var lastIndex = listSize - 1;
        if (id == 'next' && currentImageIndex != lastIndex) {
            currentImageIndex++;
        }
        else if (id == 'prev' && currentImageIndex != 0) {
            currentImageIndex--;
        }
        else if (id == 'prev' && currentImageIndex == 0) {
            currentImageIndex = lastIndex;
        }
        else {
            currentImageIndex = 0;
        }

        document.getElementById('urlError').innerText='';
        document.getElementById('propertyPhoto').style.height = '';
        document.getElementById('propertyPhoto').style.width = '';
        document.getElementById('propertyPhoto').src=imageUrlArray[currentImageIndex];
        document.getElementById('propertyPhoto').alt=descArray[currentImageIndex];
        var oneRelativeIndex = currentImageIndex + 1;
        if (listSize != 0)
        {
            var child=document.getElementById('pgInfo').childNodes;
            var chdNodeLength=child.length;
            for(i=0;i<chdNodeLength;i++) {
                document.getElementById('pgInfo').removeChild(child[i]);
            }
            document.getElementById('pgInfo').appendChild(document.createTextNode(oneRelativeIndex + ' of ' + listSize));

            child=document.getElementById('pgText').childNodes;
            chdNodeLength=child.length;
            for(i=0;i<chdNodeLength;i++) {
                document.getElementById('pgText').removeChild(child[i]);
            }
            document.getElementById('pgText').appendChild(document.createTextNode(descArray[currentImageIndex]));
        }
    }
    catch(e) {
        currentImageIndex = 0;
    }
}

function nextPhoto() { getUrl('next'); }
function prevPhoto() { getUrl('prev'); }

getUrl(null);
</script>



