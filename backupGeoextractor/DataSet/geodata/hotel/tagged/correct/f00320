<script language="JavaScript">

function resetMenu(){
	// swap images
	var x = document.images;
	for (var i=0;i<x.length;i++)
	{
		if (x[i].className == 'leftmenuimg' ){

			x[i].src = 'plus.gif';
		} 
	}

	// close divs
	var divs = document.getElementsByTagName('div');
	if (divs) {
				
		
		for (var j = 0; j < divs.length; j++) {
			//alert(j+""+divs[j].style.display);
			if (divs[j].className == 'destmenu') {
				divs[j].style.display = 'none';
			}
		}
	}

}

function ackapa(img,mdiv) {

 	
	if (mdiv.style.display != 'block'){
	resetMenu();
	
		document.images["im"+img].src="minus.gif";
		mdiv.style.display = 'block';
	} 
	else
	{
	resetMenu();
	}
}


</script>


