function scroll_status (seed)
{
    var msg = "Welcome to the King George Hotel in San Francisco, conveniently located by the world famous cable cars in the heart of Union Square.         For reservations please call (800) 288-6005 or check our availability online.        ";
    var out = " ";
    var c = 1;
    if (150 < seed) {
            seed--;
            var cmd="scroll_status(" + seed + ")";
            timerTwo = window.setTimeout(cmd, 50);
    }
    else if (seed <= 150 && 0 < seed) {
            for (c=0 ; c < seed ; c++) {
                    out+=" ";
            }
            out+=msg;
            seed--;
            var cmd="scroll_status(" + seed + ")";
                window.status=out;
            timerTwo=window.setTimeout(cmd,50);
    }
    else if (seed <= 0) {
            if (-seed < msg.length) {
                    out+=msg.substring(-seed,msg.length);
                    seed--;
            var cmd="scroll_status(" + seed + ")";
                    window.status=out;
                    timerTwo=window.setTimeout(cmd,50);
            }
            else {
                    window.status=" ";
                    timerTwo=window.setTimeout("scroll_status(150)",50);
            }
    }
}
// -- End of JavaScript code -------------- 
// --></script> 
