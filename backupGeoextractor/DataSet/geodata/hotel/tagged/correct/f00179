function updateDate(fName, mField, dField, yField, dateField)
{
    var tempMonth, tempDate, tempYear, theDate;
    var iTemp;

    iTemp = document.forms[fName].elements[mField].selectedIndex;
    tempMonth = document.forms[fName].elements[mField].options[iTemp].value;

    iTemp = document.forms[fName].elements[dField].selectedIndex;
    tempDate = document.forms[fName].elements[dField].options[iTemp].value;

    iTemp = document.forms[fName].elements[yField].selectedIndex;
    tempYear = document.forms[fName].elements[yField].options[iTemp].value;

    theDate = tempMonth + "/" + tempDate + "/" + tempYear;
    document.forms[fName].elements[dateField].value= theDate;
} 
function updateArrival() 
{
updateDate("ipxres", "arrivemonth", "arriveday", "arriveyear", "pChkIn");
}

function updateDeparture() 
{
updateDate("ipxres", "departmonth", "departday", "departyear", "pChkOut");
}
//-->
