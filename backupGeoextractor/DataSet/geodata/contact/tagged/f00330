<!--

// **** This section contains code that will run when the page is loaded ****


// Build the netui_names table to map the tagId attributes
// to the real id written into the HTML
if (netui_names == null)
   var netui_names = new Object();
netui_names.fName="client_ccap_1{actionForm.firstName}"
netui_names.MemberID="client_ccap_1{actionForm.memberID}"
netui_names.lName="client_ccap_1{actionForm.lastName}"
netui_names.salutation="client_ccap_1wlw-select_key:{actionForm.salutation}"
netui_names.sessionID="client_ccap_1{actionForm.sessionID}"
netui_names.CAPNumber="client_ccap_1{actionForm.CAPnumber}"


// **** This section contains functions typically run as events ****


// method which will return a real id for a tagId
function getNetuiTagName(id) {
   return netui_names[id];
}

// method which will return a real id for a tagId,
// the tag parameter will be used to find the scopeId for
// containers that may scope their ids
function getNetuiTagName(id, tag)
{
   var scopeId = getScopeId(tag);
   if (scopeId == "")
      return netui_names[id];
   else
      return netui_names[scopeId  + "__" + id];
}

// method which get a tag will find any scopeId that,
// was inserted by the containers
function getScopeId(tag)
{
   if (tag == null)
      return "";
   if (tag.scopeId != null)
      return tag.scopeId;
   return getScopeId(tag.parentElement);
}
-->
</script>

