<script language="JavaScript">
function validate(){
	if(check_blank('firstName','Please enter the First Name') == false) return false;
	if(check_blank('lastName','Please enter the Last Name') == false) return false;
	if(check_email('email','Please enter a valid email address') == false) return false;
	/* commented for now
	if(check_blank('address1','Please enter the address') == false) return false;
	if(document.forms[1].strState.options.selectedIndex == 0 && document.forms[1].strStateOther.value == ""){
		alert('Please select a state or use the other option');
		document.forms[1].strState.focus();
		return false;
	}
	*/
	if(document.forms[1].strCountry.options.selectedIndex == 0){
		alert('Please select a country');
		document.forms[1].strCountry.focus();
		return false;
	}
	/* commented for now
	if(check_blank('zip','Please enter the Zip/Postal Code') == false) return false;
	if(check_blank('phone','Please enter the telephone number') == false) return false;
	*/
	document.forms[1].action = 'contact_act.cfm';
	document.forms[1].submit();
	return true;
}
</script>
