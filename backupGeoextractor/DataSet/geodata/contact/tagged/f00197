function validate()
 {
	var serr = "";
	
if (document.signup.fromaddress.value == "")
	 serr= serr + " Please type in your email.";
	else
	{
	if (!validateEmail(document.signup.fromaddress.value))
	 serr= serr + "Email address is invalid. Please retype it.";
	}

if (serr == "")
  {popUp_small("thank_you.htm");
  return true;
   
   }
  if (serr != "")
