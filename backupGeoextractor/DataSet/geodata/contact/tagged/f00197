function popUp(url) {
sealWin=window.open(url,"win",'toolbar=1,location=0,directories=0,menubar=1,status=1,scrollbars=1,resizable=1,left = 0, top = 0,width=450,height=200');
self.name = "mainWindow";
}
function popUp_small(url) {
sealWin=window.open(url,"win",'toolbar=0,location=0,directories=0,menubar=0,status=1,scrollbars=0,resizable=1,left = 0, top = 0,width=270,height=150');
self.name = "mainWindow";
}
</SCRIPT>
