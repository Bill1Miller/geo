function RightslinkPopUp (title, date, author, contentID) 

{
	var url = "https://s100.copyright.com/servlet/DispatchServlet";
	var location = url
		+ "?PublisherName=" + escape("VNU eMedia")
		+ "&publication=" + escape("Back Stage")
		+ "&ContentID=" + escape(contentID)
		+ "&Title=" + escape(title)
		+ "&Author=" + escape(author)
		+ "&PublicationDate=" + escape(date)
		+ "&Install=S";
		window.open( location, 'Rightslink','location=no,toolbar=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=650,height=550');
}





</script>


