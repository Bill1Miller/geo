function swapIt(imgName,imgType,imgAction) {
      if (document.images) {
        if (imgAction == 1) {
          switch(imgType){
            case "Home": newSrc="/Graphics/ACFE071.gif"; break
          case "Education_and_Training": newSrc="/Graphics/educationtraining_on.gif"; break
          case "Resources": newSrc="/Graphics/resources_on.gif"; break
