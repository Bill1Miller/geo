<script>
function stripChars(formfield){
	var thevalue = formfield.value;
	// first srip off the comma's
	thevalue = ReplaceString(thevalue, ",", "");
	// create arrays, loop thru and add to the tolist 
	charList = "(),)(,((,))";  // this will be an ever evolving list of characters...
	var charArray = charList.split(",");
	for (var i=0; i < charArray.length; i++) {
		theChar = charArray[i];
		thevalue = ReplaceString(thevalue, theChar, "");
		// recursively call itself
		test = Find(theChar,thevalue);
		if (test > -1){
			stripChars(formfield);
		}
	}
	return thevalue;
} 
// if advanced search, the contains() function will throw an error if a non-boolean, multiple word search without either being surrounded by "" or separated by booleans	
function checkbooleans(thefield){
	fail_var = false;
	searchstr = thefield.value; 
	// searchstr
	if (searchstr.length > 0) {
		// how many parenthesis does this contain?  remember these should be at least 1 boolean per set of () 
		paren_num = OccursNoCase("(", searchstr) + OccursNoCase(")", searchstr);
		paren_num = paren_num/2;
		bool_num = 0;
		bool_num = bool_num + OccursNoCase(" or ", searchstr);
		bool_num = bool_num + OccursNoCase(")or(", searchstr);
		bool_num = bool_num + OccursNoCase(")or ", searchstr);
		bool_num = bool_num + OccursNoCase(" or(", searchstr);
		bool_num = bool_num + OccursNoCase(" and ", searchstr);
		bool_num = bool_num + OccursNoCase(")and(", searchstr);
		bool_num = bool_num + OccursNoCase(")and ", searchstr);
		bool_num = bool_num + OccursNoCase(" and(", searchstr);
		bool_num = bool_num + OccursNoCase(" near ", searchstr);
		bool_num = bool_num + OccursNoCase(")near(", searchstr);
		bool_num = bool_num + OccursNoCase(")near ", searchstr);
		bool_num = bool_num + OccursNoCase(" near(", searchstr);
		bool_num = bool_num + OccursNoCase(" not ", searchstr);
		bool_num = bool_num + OccursNoCase(")not(", searchstr);
		bool_num = bool_num + OccursNoCase(")not ", searchstr);
		bool_num = bool_num + OccursNoCase(" not(", searchstr);
		//alert("paren_num: " + paren_num + " bool_num: " + bool_num);
		if (paren_num !=1 && paren_num > bool_num){
			fail_var =  true;
		}
		return fail_var;
	}// for (searchstr.length > 0)
}

function checkandsubmit(theform){
	var missing = false;
	var boolean_fail = false;
	var missing_str = "You have not provided the following:\n\n";
	var boolean_fail_str = "A multiple word Advanced Search containing ()'s must use one or more operators such as AND or OR or NEAR \nPlease correct and search again.";
	// strip off the illegal chars
	theform.criteria.value = stripChars(theform.criteria);
	// test boolean logic of search 
	boolean_fail = checkbooleans(theform.criteria);
	
	// test checkboxes
	if( theform.search_abs.checked || theform.search_nabs.checked || theform.search_cs.checked ) {
		dummy = 0;
	}
	else {
		missing = true;
		missing_str += "Search Location\n";
	}
	if (!theform.criteria.value){
		missing = true;
		missing_str += "Search String\n";
	}
	if (missing){
		alert(missing_str);
		return false;
	}
	else if (boolean_fail){
		alert(boolean_fail_str);
		return false;
	}	
	else{
		return true;
	}
}
function search_tips(){
	var thepage = "/customcf/verity_search_tips.cfm";
	//pleasewait();
	var features = "height=450,width=500,left=350,top=150,resizable,scrollbars";
	popupWinAll(thepage,features,'popupwindow');
}

