bName = navigator.appName;
bVer = parseInt(navigator.appVersion);

if ((bName == "Netscape" && bVer >= 3) || (bName == "Microsoft Internet Explorer" && bVer >= 4)) {
	br = "n3"; 
}
else {
	br = "n2";
}

function imgRoll(imgName,row,mode,type){
        if(br == "n3" && document.images) {
                if(!type) { type = 'GIF'; }
        imgFile= '/images/data/' + mode + '/' + row + '.' + type;
        document[imgName].src=imgFile;
        }
}

function openBridge(params) {
        window.open('/cgi-bin/iowa/bridge.html?' + params, 'bridge', 'scrollbars=yes,toolbar=no,location=no,directories=no,menubar=no,width=380,height=200');
}
function openNote(params) {
        window.open('/cgi-bin/iowa/note.html?' + params, 'note', 'scrollbars=yes,toolbar=no,location=no,directories=no,menubar=no,width=380,height=200');
}
