	function jumpPage(newLoc) {
			newPage = newLoc.options[newLoc.selectedIndex].value
	
			if (newPage != "") {
				self.location = newPage
			}
		}

function newwindow(newLoc) {
			 {
				window.open(newLoc, "", "width=600,height=600,scrollbars=yes,menubar=yes,toolbar=yes,resizable=yes,location=yes, status=yes")
			}
		}

