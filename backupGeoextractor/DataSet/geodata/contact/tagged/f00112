<script language="javascript">
function Popup_Description(n_id)
{
	var strURL;
	
	strURL = "";
	strURL = "http://www.eweek.com/newsletter_description/0,,,n=";
	strURL = strURL + n_id;
	strURL = strURL + ",00.asp";
	
	newsletter_popup = window.open(strURL, "NewsletterWindow", "HEIGHT=300,WIDTH=300,RESIZABLE=yes");
	
}
</script>

