			var isMemberCookie = getCookie("zm%5Fuser");
			var signInUrl = "http://www.eweek.com/join/0,2475,,00.asp" + "?success_page=" + getCookie("successpage")  
			var signOutUrl = "http://www.eweek.com/data_process_log_out/0,1207,,00.asp" + "?success_page=" + getCookie("successpage")  
			var registerUrl = "http://www.eweek.com/join/0,2475,,00.asp" + "?success_page=" + getCookie("successpage")  
			
			if (isMemberCookie != null) {
				document.write("<a href=\"" + signOutUrl + "\" target=\"_top\" class=\"nav_text\">Sign Out</a>&nbsp;");
			} else {
				document.write("<a href=\"" + signInUrl + "\" target=\"_top\" class=\"nav_text\">Sign In</a>&nbsp;");
			}
		</script>		
		
		
