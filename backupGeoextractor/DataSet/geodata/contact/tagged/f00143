function ShowNewIcon(sExpire)
{
	var dToday = new Date();
	var dExpire = new Date(sExpire);
	if ( dToday <= dExpire )
	{
		document.write( "<img src='/images/icons/icon_new.gif' alt='new'>" );
	}
}

