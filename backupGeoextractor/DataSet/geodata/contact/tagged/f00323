function search_size(){
	// Windows Netscape 4.7
	if(is_ns4 && is_win32)
	{
		document.write(' <td valign="top" width="128"><p><input name="qt" type="text" size="10" class="formSearch" value="Search cadence.com" onFocus="javascript:this.value=\'\';"></p></td>');
	}
	// All other browsers
	else
	{
		document.write(' <td valign="top" width="128"><p><input name="qt" type="text" class="formSearch" value="Search cadence.com" onFocus="javascript:this.value=\'\';"></p></td>');
	}

}

function conproSearch_size(){
	// Windows Netscape 4.7
	if(is_ns4 && is_win32)
	{
		document.write(' <td valign="top" width="155"><p><input name="qt" type="text" size="12" class="formSearch" value="Search Connections Program" onFocus="javascript:this.value=\'\';"></p></td>');
	}
	// All other browsers
	else
	{
		document.write(' <td valign="top" width="155"><p><input name="qt" type="text" class="formSearch" value="Search Connections Program" onFocus="javascript:this.value=\'\';"></p></td>');
	}

}

function drop_sizeIndex(){
if(is_ns4 && is_win32)
	{
		document.write('<select class="form" name="foofoo" onChange="MM_jumpMenu(\'parent\',this,0)" width="140px">');
					
	}
// All other browsers
else
	{
		document.write('<select class="form" name="foofoo" onChange="MM_jumpMenu(\'parent\',this,0)">');
							
	}

}		

function drop_size(){
if(is_ns4 && is_win32)
	{
		document.write('<select class="dropDown" name="foofoo" onChange="MM_jumpMenu(\'parent\',this,0)" size="1" width="176">');
					
	}
// All other browsers
else
	{
		document.write('<select class="dropDown" name="foofoo" onChange="MM_jumpMenu(\'parent\',this,0)" size="1">');
							
	}

}							
