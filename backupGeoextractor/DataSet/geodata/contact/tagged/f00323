// Mac Netscape 4.7
if (is_mac && is_ns4up) {
document.write('<link href="/includes/style_mac_n4.css" rel="stylesheet" type="text/css">');
}
// SunOS Netscape 4.7
else if(is_sun && is_ns4) {
document.write('<link href="/includes/style_sun_n4.css" rel="stylesheet" type="text/css">');
}
// Mac Netscape 5.0 and up
//else if (is_mac & is_ns5up) {
//document.write('<link href="/includes/style_mac_n7.css" rel="stylesheet" type="text/css">');
//}
// Mac IE and all other browsers
else if (is_mac) {
document.write('<link href="/includes/style_mac.css" rel="stylesheet" type="text/css">');
}
// Netscape 4.7 and up
else if (is_ns4up){
document.write('<link href="/includes/style_n4.css" rel="stylesheet" type="text/css">');
}
// IE and all other browsers
else {
document.write('<link href="/includes/style.css" rel="stylesheet" type="text/css">');
}

