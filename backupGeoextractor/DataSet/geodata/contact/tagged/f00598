<script type="text/javascript">
<!--

function newTile(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeTile() {
	if (document.images && (mosaicFlag == true)) {
		document[changeTile.arguments[0]].src = changeTile.arguments[1];
		document[changeTile.arguments[2]].src = changeTile.arguments[3];
		showTile(changeTile.arguments[4]);
	}
}

var mosaicFlag = false;
function preloadMosaic() {
	if (document.images) {
		mosaic_01_over = newTile("/dynamic/images/widgets/image_over_1_47.jpg");
		mosaic_02_over = newTile("/dynamic/images/widgets/image_over_2_65.jpg");
		mosaic_04_over = newTile("/dynamic/images/widgets/image_over_3_65.jpg");
		mosaic_03_over = newTile("/dynamic/images/widgets/image_over_4_65.jpg");
		mosaicFlag = true;
	}
}

preloadMosaic();

var agt=navigator.userAgent.toLowerCase();
var use_gebi = (document.getElementById) ? true : false;
var ie4flag = (document.all) ? true : false;
var ns4flag = (document.layers) ? true : false;
var ns6flag = (use_gebi && !document.all) ? true : false;
var safariflag = ((agt.indexOf('safari')!=-1)&&(agt.indexOf('mac')!=-1))?true:false;

// layer hiding and showing
function showTileCap(lay) {
	if (ie4flag) {document.all[lay].style.visibility = "visible";}
	if (ns4flag) {document.layers[lay].visibility = "show";}
	if (ns6flag) {document.getElementById(lay).style.visibility="visible";}
	return true;
}

function hideTile(lay) {
	if (ie4flag) {document.all[lay].style.visibility = "hidden";}
	if (ns4flag) {document.layers[lay].visibility = "hide";}
	if (ns6flag) {document.getElementById(lay).style.visibility="hidden";}
	return true;
}

//subnav hiding and showing high-level mgmt
function showTile(lay) {
	hideTile("mosaic3Layer");
hideTile("mosaic4Layer");
hideTile("mosaic5Layer");
hideTile("mosaic6Layer");

	showTileCap(lay);
	return true;
}

// -->
</script>
