		menuItem["nyclife"]	= {section:"nyclife",id:"nav_nyclife",link:"/nyclife/"};
		menuItem["music"]	= {section:"music",id:"nav_music",link:"/2004/toc.php?section=music"};
		menuItem["film"]	= {section:"film",id:"nav_film",link:"/2004/toc.php?section=film"};
		menuItem["arts"]	= {section:"arts",id:"nav_arts",link:"/2004/toc.php?section=arts"};
		menuItem["people"]	= {section:"people",id:"nav_people",link:"/2004/toc.php?section=people"};
		menuItem["classifieds"]	= {section:"classifieds",id:"nav_classifieds",link:"/classifieds/"};
		menuItem["home"]	= {section:"home",id:"nav_home",link:"/2004/"};

		function hideAllSubsections()
		{
			for( var i in menuItem ) {
				hideSubsection("subsection_" + menuItem[i].id);
			}
		}
		function showSubsection(menuId) {
			getElem( "subsection_" + menuId ).style.display="block";
		}
		function hideSubsection(menuId) {
			if( getElem( "subsection_" + hideWhich ) ) {
				getElem( "subsection_" + hideWhich ).style.display="none";
			}
		}		
		function hiSectionMenu(menuId) {
			document.images[menuId].src = '/images/nav_top/' + menuId + '_over.gif';
			clearTimeout(menuTimeout);
			hideAllSubsections();
			showSubsection( menuId );
			return true;
		}
		function loSectionMenu(menuId) {
			document.images[menuId].src = '/images/nav_top/' + menuId + '.gif';
			hideWhich = menuId;
			menuTimeout=setTimeout('hideSubsection()',500)
			return true;
		}
		function hiSubsection(menuId) {
			clearTimeout(menuTimeout);
		}
		function getElem( idString ) {
			var obj = null;
			if ( document.getElementById ) {
				obj = document.getElementById( idString );
			} else if ( document.layers
						&& document.layers[idString] ) {
				obj = document.layers[idString]
			} else if ( document.all
						&& document.all[idString] ) {
				obj = document.all[idString]
			}
			return obj;
		}

		var preloadFlag = false;
		function preloadImages()
		{
			if (document.images)
			{
				preloadImgArray = new Array();
				for( var i in menuItem ) {
					preloadImgArray[menuItem[i].id]		= new Image();
					preloadImgArray[menuItem[i].id].src	= "/images/nav_top/" + menuItem[i].id + "_over.gif";
				}
				preloadFlag = true;
			}
		}
		preloadImages();

		// -->
