function highlight(name, idx) { 
   document.images[name].src = imgOn[idx]; 
   return; 
}

function unHighlight(name, idx) { 
   document.images[name].src = imgOff[idx]; 
   return; 
}

function popup(url, width, height) {
    var date = new Date();
    var title = date.getTime();
    var spec = "status,scrollbars,width=" + width + ",height=" + height + ",resizable";
    top.open(url, title, spec);
}

function print_window(url) {
   var title = "print";
   var spec  = "status,scrollbars,menubar,width=640,height=480,resizable";
   window.open(url, title, spec);
}

function popunder(url, width, height, focus) {
   var features = "height=" + height + ",width=" + width + ",scrollbars,resizable,status=no,toolbar=no,menubar=no,location=no";
   var windowName = "pop";
   var pop = window.open(url, windowName, features, false);

   if(! focus) { this.focus(); }
}
