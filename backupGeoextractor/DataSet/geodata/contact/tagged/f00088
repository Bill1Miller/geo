<script language="JavaScript">													// script defines array of items & randselect();
   var item=new Array("winter_commons.gif","winter_cuhockey.jpg","winter_deer.jpg","winter_girlsbball.jpg","winter_lake.gif","winter_schoolbus.jpg","winter_sciencenter.gif","winter_scraping.gif","winter_seniors.gif","winter_shoveling.jpg","winter_snowballfight.jpg","winter_tree.gif");

   function randselect() {
	do {							
	i=Math.floor(Math.random()*item.length); 	
        } while(typeof(item[i])=="undefined");

   return item[i];
   }
</script>


