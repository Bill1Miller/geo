function getChannel(){
	if(!window.BreadCrumbIDs) return ''

	if(window.location.pathname.indexOf("/patient_education/") != -1) return "Patient Education"

	var mskcc_channels = []
	mskcc_channels[mskcc_channels.length] =	{ID:15467, name:"When You Need a Transplant"}
	mskcc_channels[mskcc_channels.length] =	{ID:17589, name:"Newly Diagnosed - Lymphoma"}
	mskcc_channels[mskcc_channels.length] =	{ID:13910, name:"Newly Diagnosed - Leukemia"}
	mskcc_channels[mskcc_channels.length] =	{ID:14172, name:"Newly Diagnosed - Colorectal"}
	mskcc_channels[mskcc_channels.length] =	{ID:13016, name:"Newly Diagnosed - GYN"}
	mskcc_channels[mskcc_channels.length] =	{ID:9698, name:"Newly Diagnosed Pilot"}
	mskcc_channels[mskcc_channels.length] =	{ID:12112, name:"Newly Diagnosed - Lung"}

	mskcc_channels[mskcc_channels.length] =	{ID:11570, name:"About Herbs"}
	mskcc_channels[mskcc_channels.length] =	{ID:5804, name:"SKI"}
	mskcc_channels[mskcc_channels.length] =	{ID:5306, name:"For New Patients"}
	mskcc_channels[mskcc_channels.length] =	{ID:3226, name:"International Patients"}
	mskcc_channels[mskcc_channels.length] =	{ID:3151, name:"Physician Referral Guide"}
	mskcc_channels[mskcc_channels.length] =	{ID:2715, name:"Pediatrics"}
	mskcc_channels[mskcc_channels.length] =	{ID:361, name:"Types of Cancer"}
	mskcc_channels[mskcc_channels.length] =	{ID:600, name:"Prevention and Screening"}
	mskcc_channels[mskcc_channels.length] =	{ID:539, name:"Public Education"}
	mskcc_channels[mskcc_channels.length] =	{ID:3336, name:"Fred's Team"}

	for(var i=0;i<mskcc_channels.length;i++){
		for(var x=0;x<BreadCrumbIDs.length;x++){
			if(BreadCrumbIDs[x] == mskcc_channels[i].ID) 
				return mskcc_channels[i].name
		}
	}
	return ''
}
var s_channel = getChannel()

