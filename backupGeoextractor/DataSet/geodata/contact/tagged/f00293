var url = location.href;	// url you wish to have bookmarked
var who = document.title;	// caption to appear with bookmark

function BookMarkIt()
{
var ver = navigator.appName
var num = parseInt(navigator.appVersion)
	if ((ver == "Microsoft Internet Explorer")&&(num >= 4)) {
	   window.external.AddFavorite(url,who);
	}else{
	   alert("Sorry, the bookmark feature is only available in modern browsers.");
	} 
}
</script>

