<script type="text/javascript">
  function dataValidate()
  {
    if(document.globe.fname.value=="")
    {
      alert('You must enter a value for First Name field');
      return false;
    }
    if(document.globe.lname.value=="")
    {
      alert('You must enter a value for Last Name field');
      return false;
    }
    if(!emailValidate())
    {
      alert('You must enter a valide value for Email field: xxx@xxx.xxx');
      return false;
    }
    if(document.globe.related.value=="")
    {
      alert('You must select a topic in the "Is your e-mail related to:" box');
      return false;
    }

	var found_what;
	for (var i=0; i<document.globe.what.length; i++)  {
		if (document.globe.what[i].checked)  {
			found_what = document.globe.what[i].value;
		}
	}
    if(found_what==null)
    {
      alert('You must select whether this is a question, complaint or comment');
      return false;
    }

	var found_regarding;
	for (var i=0; i<document.globe.regarding.length; i++)  {
		if (document.globe.regarding[i].checked)  {
			found_regarding = document.globe.regarding[i].value;
		}
	}
    if(found_regarding==null)
    {
      alert('You must select whether this is regarding our Web site or our TV station');
      return false;
    }

    if(document.globe.comments.value=="")
    {
      alert('You must enter a value for comments field');
      return false;
    }
  }
  function emailValidate()
  {
    re = new RegExp("^[0-9a-zA-Z\\._-]+@[0-9a-zA-Z\\._-]+\\.([a-zA-Z]{2,3}|(name|aero|info|coop|museum))$");
    if((document.globe.email.value).search(re)==-1)
    {
       return false;
    }
    else
    {
       return true;
    }
  }
</script>


















