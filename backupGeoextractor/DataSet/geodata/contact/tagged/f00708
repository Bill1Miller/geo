	if(visits >=14){var segId = 4}//stores a segment of 4 if the user visits the more than 14 times.
	
	visits = eval(parseInt(visits)+1);//increments the visits number by one on each day
	setCookie("numVisits",visits);//sets the cookie with the number of total visits
	setCookie("visitDate",hb_currentDate);//sets the cookie with the current date
}
else
{
	if(visitDate != hb_currentDate){setCookie("numVisits", 1);}
