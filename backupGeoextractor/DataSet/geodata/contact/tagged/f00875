<script language="JavaScript">
				function MM_jumpMenuGo(){ 
				var auctionsDropDown = document.form1.menu1;
				if(auctionsDropDown.options[auctionsDropDown.selectedIndex].value=="x"){
					alert('You must select a valid auction before clicking on the Go button');
				}
				else{
					var auctionID = auctionsDropDown.options[auctionsDropDown.selectedIndex].value;
					document.location.href = "/auctions/default.htm" + "?job=auction_summary" + "&id=" + auctionID;
				}
			}
			</script>
