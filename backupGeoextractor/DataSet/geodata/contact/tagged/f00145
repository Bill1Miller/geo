        }
        else {
          switch(imgType){
            case "Materials_Info": newSrc="/Graphics/MaterialsInfo_off.gif"; break
          case "Membership": newSrc="/Graphics/MembersOnly_off.gif"; break
          case "MemberLogin": newSrc="/Graphics/sitelogin_off.gif"; break
          case "AffiliateSocieties": newSrc="/Graphics/affiliatesocieties_off.gif"; break
