function swapIt(imgName,imgType,imgAction) {
      if (document.images) {
        if (imgAction == 1) {
          switch(imgType){
            case "Materials_Info": newSrc="/Graphics/MaterialsInfo_on.gif"; break
          case "Membership": newSrc="/Graphics/MembersOnly_on.gif"; break
          case "MemberLogin": newSrc="/Graphics/sitelogin_on.gif"; break
          case "AffiliateSocieties": newSrc="/Graphics/affiliatesocieties_on.gif"; break
          case "Chapters_Committees": newSrc="/Graphics/ChaptersCommittees_on.gif"; break
          case "CareerCenter": newSrc="/Graphics/careercenter_on.gif"; break
          case "AskASM": newSrc="/Graphics/askasm_on.gif"; break
          case "News": newSrc="/Graphics/news_on.gif"; break
          case "Bookstore": newSrc="/Graphics/bookstore_on.gif"; break
          case "Standards": newSrc="/Graphics/standards_on.gif"; break
          case "Journals": newSrc="/Graphics/journals_on.gif"; break
          case "Training": newSrc="/Graphics/training_on.gif"; break
          case "Magazines": newSrc="/Graphics/magazines_on.gif"; break
          case "Events": newSrc="/Graphics/events_on.gif"; break
