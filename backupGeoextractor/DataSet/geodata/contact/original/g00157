   function checkFields()
   {
      missinginfo = "";
      if (document.form2.CITY.value == "")
      {
         missinginfo += "\n - Location";
      }
      if (document.form2.AWXURL.value == "http://") 
      {
         missinginfo += "\n - URL (Web Address)";
      }
      if (document.form2.AWXURL.value == "http://www.accuweather.com/adcbin/contact")
      {
         missinginfo += "\n - URL (Not This Contact Page!)";
      }
      if ((document.form2.EMAIL.value == "") ||
          (document.form2.EMAIL.value.indexOf('@') == -1) || 
          (document.form2.EMAIL.value.indexOf('.') == -1))
      {
         missinginfo += "\n - Email address";
      }
      if (document.form2.DETAILS.value == "")
      {
         missinginfo += "\n - Details";
      }
      if (document.form2.FIRSTNAME.value == "") 
      {
         missinginfo += "\n - First Name";
      }
      if (document.form2.LASTNAME.value == "") 
      {
         missinginfo += "\n - Last Name";
      }
      if (document.form2.Subject.selectedIndex == "0") 
      {
         missinginfo += "\n - Type Of Question";
      }
      if (missinginfo != "") 
      {
         missinginfo ="_____________________________________\n" +
         "The following items were not correctly filled in:\n" +
         missinginfo + "\n_____________________________________" +
         "\nPlease reenter those items.";
         alert(missinginfo);
         return false;
      }
      else return true;
      }
   // End -->
