   function display()
   {
      window.onerror=null;
      colors = window.screen.colorDepth;
      document.form2.auto_colors.value = Math.pow (2, colors);
      if (window.screen.fontSmoothingEnabled == true)
         document.form2.auto_fonts_smoothed.value = "Yes";
      else 
         document.form2.auto_fonts_smoothed.value = "No";
      document.form2.auto_web_browser.value = navigator.appName;
      document.form2.auto_web_browser_version.value = navigator.appVersion;
      document.form2.auto_color_bit_depth.value = window.screen.colorDepth;
      document.form2.auto_resolution_width.value = window.screen.width;
      document.form2.auto_resolution_height.value = window.screen.height;
      document.form2.auto_resolution_max_width.value = window.screen.availWidth;
      document.form2.auto_resolution_max_height.value = window.screen.availHeight;
