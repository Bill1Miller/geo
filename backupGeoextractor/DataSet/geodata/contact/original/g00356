<script language="javascript" type="text/javascript">
	<!--//


	//==========//
	// JavaScript Rollovers v1.4
	//==========//


	// ***** ROLLOVER CODE - START ***** //
	
	if (document.images) {
		var imageCache = new Array();
		var allCacheCalls = 0;
		var tempImage = new Image();
	}	


	function swapImage(names) {
		if (document.images) {
			imageList = names.split(",");
			for (i = 0 ; i < imageList.length ; i++) {
				imagePair = imageList[i].split("=");
				if (imagePair.length == 2) {
					if (document.images[imagePair[0]]) {
						document.images[imagePair[0]].src = imagePair[1];
					} 
				}
			}
		}
	}


	function cache(name) {
		if (document.images) {
			var current = imageCache.length;
			imageCache[current] = new Image();
			imageCache[current].src = name;
		}
	}


	function pageLoaded() {
		if (document.images) {
			cache("/images/nav/bt_are_about.gif");
			cache("/images/nav/bt_are_about_over.gif");
			cache("/images/nav/bt_are_about_on.gif");
			cache("/images/nav/bt_are_news.gif");
			cache("/images/nav/bt_are_news_over.gif");
			cache("/images/nav/bt_are_news_on.gif");
			cache("/images/nav/bt_are_careers.gif");
			cache("/images/nav/bt_are_careers_over.gif");
			cache("/images/nav/bt_are_careers_on.gif");
			cache("/images/nav/bt_are_investors.gif");
			cache("/images/nav/bt_are_investors_over.gif");
			cache("/images/nav/bt_are_investors_on.gif");
			cache("/images/nav/bt_do_medicines.gif");
			cache("/images/nav/bt_do_medicines_over.gif");
			cache("/images/nav/bt_do_health.gif");
			cache("/images/nav/bt_do_health_over.gif");
			cache("/images/nav/bt_do_animal.gif");
			cache("/images/nav/bt_do_animal_over.gif");
			cache("/images/nav/bt_do_business.gif");
			cache("/images/nav/bt_do_business_over.gif");
			cache("/images/nav/bt_help_research.gif");
			cache("/images/nav/bt_help_research_over.gif");
			cache("/images/nav/bt_help_caring.gif");
			cache("/images/nav/bt_help_caring_over.gif");
			cache("/images/nav/bt_help_access.gif");
			cache("/images/nav/bt_help_access_over.gif");
			cache("/images/nav/bt_help_citizen.gif");
			cache("/images/nav/bt_help_citizen_over.gif");
			cache("/images/nav/blt_square.gif");
			cache("/images/nav/blt_square_on.gif");
			cache("/images/nav/blt_arrow.gif");
			cache("/images/nav/blt_arrow_on.gif");
			
			allCacheCalls = 1;
		}
