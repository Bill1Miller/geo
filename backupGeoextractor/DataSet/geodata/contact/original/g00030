<script xmlns:fo="http://www.w3.org/1999/XSL/Format" language="JavaScript">

function openPrintBrWindow(winName,features) 
{ 
	var url = window.location.href;
	var urlFirstPart = "";
	var urlSecondPart ="";
	var targetURL = "";
	var urlAppend = "";
	if(url.indexOf(',00.html') != -1){
		urlFirstPart=url.substring(0,url.indexOf('0,,')+3);
		urlSecondPart=url.substring(url.indexOf('0,,')+3);
	  	urlAppend="print:Y";
	  	urlAppend=urlAppend+'~';
	  	urlAppend=urlAppend+"isCURL:Y";
	  	if(urlSecondPart !="")
			urlAppend=urlAppend+"~";
		targetURL=urlFirstPart+urlAppend+urlSecondPart;	
	}else{
		urlFirstPart=url.substring(0,url.indexOf('?')+1);
		urlSecondPart=url.substring(url.indexOf('?')+1);
		urlAppend="print=Y";				  
		urlAppend=urlAppend+"&amp;";
		if(urlSecondPart !="")
			urlAppend=urlAppend;
		targetURL=urlFirstPart+urlAppend+urlSecondPart;
	}
	//window.location.href=targetURL;
	window.open(targetURL,winName,features);
	//window.open(theURL,winName,features);
}

</script><table xmlns:fo="http://www.w3.org/1999/XSL/Format" summary="Print Version" class="Table100Per" cellspacing="0" cellpadding="0"><tr><td class="print"><a accesskey="p" href="javascript:openPrintBrWindow('mywindow','resizable=yes,scrollbars=yes,menubar=no,toolbar=no');"><img border="0" src="http://siteresources.worldbank.org/images/Print_plain.gif" alt="Print version"/></a></td></tr></table>


	
