//Paul Comtois Internoded Inc. jan 21, 2000

	function SubscriptionWin(winURL)
	{
	newwin = window.open(winURL, "Subscription")
	}
	
	function getCalendarDay(day)
	{
	//returns the weekday as a string//
	
	var dow = new Array(7);
	dow[0] = "Sunday";
	dow[1] = "Monday";
	dow[2] = "Tuesday";
	dow[3] = "Wednesday";
	dow[4] = "Thursday";
	dow[5] = "Friday";
	dow[6] = "Saturday";
	return dow[day]		
	}
	
	
	function getCalendarMonth(Month)
	{
	//Returns the month string//
	var md = new Array(12);
	md[0] = "January";
	md[1] = "February";
	md[2] = "March";
	md[3] = "April";
	md[4] = "May";
	md[5] = "June";
	md[6] = "July";
	md[7] = "August";
	md[8] = "September";
	md[9] = "October";
	md[10] = "November";
	md[11] = "December";
	
	return md[Month]	
	
	}
	

	dte = new Date()	
	result = dte.toGMTString()		
    Day= dte.getDate();
	Year = dte.getYear();
	Month = dte.getMonth()+ 1;
	MonthDay = getCalendarMonth(dte.getMonth());		
	DayStr = getCalendarDay(dte.getDay());
	fulldate = MonthDay + " " + Day
