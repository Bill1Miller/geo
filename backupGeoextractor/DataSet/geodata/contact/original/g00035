function ReplSubstr( sourceList, fromList, toList )
	{
	aStr=sourceList;
	s1="";
	s2="";
	s3=""
//	while (aStr.indexOf(fromList)>-1)
//		{
		s1=aStr.substring(0, aStr.indexOf(fromList)-0)
		s2=aStr.substring(aStr.indexOf(fromList),aStr.indexOf(fromList)+fromList.length)
		s3=aStr.substring(aStr.indexOf(fromList)+fromList.length, aStr.length)
		aStr=s1+toList+s3
//		}
		return aStr
	}



	function redirect()
	{
		ff2=window.location.href;

		if ((ff2.indexOf("https:")==-1) && (ff2.indexOf("dirpurchase.asp")!=-1) )
		{
			window.location.href=ReplSubstr(ff2,"http:","https:")
		}		
		if ((ff2.indexOf("https:")!=-1) && (ff2.indexOf("dirpurchase.asp")==-1) )
		{
			window.location.href=ReplSubstr(ff2,"https:","http:")
		}

