<SCRIPT Language=Javascript>
function Vote(questionvalue){
//This function is used for voting.
x = 0
id = 0
while( x < document.forms[0].Question.length ) {
     if (document.forms[0].Question[x].checked) {
      break}
     x ++
};
height = 290 + (document.forms[0].Question.length * 35);

window.open('chart.asp?Option=' + questionvalue + '&questionid=' + document.forms[0].questionid.value,'popup', 'width=400,HEIGHT=' + height)
}
function Search(qry) {
//This function is used to take the value from the textbox
//and open the search page.
word = ""

for (var i = 0; i < qry.length; i++) {
    letter = qry.substring(i,i+1);
    if ( letter  == " " ) 
		{word = word + "+"}
	else
		{word = word + letter};
};

searchurl = "search.asp?qrystr=" + word;
window.location.href=searchurl;
}
</script>


