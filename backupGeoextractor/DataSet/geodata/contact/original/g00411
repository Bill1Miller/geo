<script>
function Job(employer, title, id, idType)  {
  this.title = title;
  this.employer = employer;
  this.id = id;
  this.idType = idType;

  function getURL() {
    url = "http://www.careerbuilder.com/";
    if (this.idType == 1) {
      url += "JobSeeker/Jobs/JobDetails.aspx?Job_DID=";
    }
    else {
      url += "pli/webidv1.asp?webid=";
    }
    url += this.id + "&lr=cbcb_ba&siteid=ckrba066";
    return url;
  }
  function getListing() {
    return "<li style='padding-bottom:5px'>" +
    (this.employer.length > 0 ? "<span class='nav2'>" + this.employer + "</span><br />" : "") +
    "<a class='link' href='javascript:openTopJobsWin(\"" + this.url() + "\")'>" + this.title + "</a></li>\n";
  }
  this.url = getURL;
  this.listing = getListing;
}
function openTopJobsWin(url) {
  x=Math.max(1, screen.width-798);
  y=1;
  h=Math.min(640, screen.availHeight);
  windowName='topjobs';
  if (self.name=='topjobs') {
    x=1;
    y=40;
    windowName='topjobs2';
  }
  window.open(url, windowName,	 'width=788,height='+h+',top='+y+',left='+x+',scrollbars=yes,toolbar=yes,status=yes,menubar=yes,location=yes,resizable=yes').focus();
}
jobs = new Array(
new Job("Bascom Animal Hospital","Veterinary Tech","J3W4SS702LWK8NPGV2G",1),
new Job("SeeCommerce","Applications Engineer","J3X80C6H29C2B3WC0BF",1),
new Job("Crossroads Trading Co.","Assistant Manager","J3W3W76GK5W4H5K6MJ7",1),
new Job("WyoTech Of Fremont","Automotive Techs/Instructors","J3W7CH5YFVMXHC68Y9V",1),
new Job("Sylvan Learning Center","Director of Education","MN1219265",0),
new Job("California State University Stanislaus","Director of Network & Communication Services","J3W5BC6V1VL5MN54MYN",1),
new Job("Sigma Designs","Engineers","J3X29S72HHZQY9CS5ZK",1),
new Job("International Aviation Services Company","Human Resources Coordinator","J3X59P5W5JZT4H3K2HL",1),
new Job("East Bay Municipal Utilities","Industrial Electrician","J3X4NW6VDWZCQDQXBX7",1),
new Job("Marquez Brothers International, Inc.","Junior Accountant","J3W0LD6JX8CXL9LVX18",1),
new Job("","Programmer","J3X3L377FJVFYNQNHQ5",1),
new Job("Paver Depot","Project Manager/Engineer","J3X13S622CHX68ZFVDR",1),
new Job("Valley Yellow Pages","Sales","J3X619631L29RLY86JY",1),
new Job("Via Technologies","Software Engineer","J3X058775C7FWGTFFY4",1),
new Job("Pacific Motor Trucking Co.","Terminal Supervisor/Dispatch Supervisor","J3W23S61LTK3RVKB8FP",1),
new Job("Infortrend Corporation","Test Engineer","J3W55Q6DKKJMLHX6D10",1),
new Job("AgileTV Corporation","Computers","J3W3P565VYPYFFV8BNY",1),
new Job("Vezers Precision Industrial Contractors","Industrial Construction","J3W29C6PV5KBHT119NV",1),
new Job("Arista Corp.","Inside Sales/Industrial Computers","J3X4JP70SSBBRK20S5X",1)
);

jobsToShow = 6;
firstJob = new Date().getTime() % jobs.length;

if (document.location.pathname.indexOf("/top_jobs") >= 0) {
  jobsToShow = jobs.length;
  firstJob = 0;
  document.getElementById("all-jobs").innerHTML = "";
}

jobsList = new String();

for (count = 0; count < jobsToShow && count < jobs.length; count++) {
  index = (firstJob + count) % jobs.length;
  jobsList += jobs[index].listing();
}
document.getElementById("topJobs").innerHTML = jobsList;
</script>
