<SCRIPT LANGUAGE="JavaScript">

function openRemoteCustom(URL,xwidth,yheight,scrollbars){
	if (scrollbars != "yes")
		scrollbars = "no";
	var winParam="toolbar=no,location=0,directories=0,status=0,menubar=no,scrollbars="+scrollbars+",width="+xwidth+",height="+yheight+",resizable=yes";
	var remote = window.open(URL,"customwindow",winParam);
	if (remote.opener == null) {
		remote.opener = window;
	}	
}


</SCRIPT>

