<!-----------------------------------------------------------------------------------
TimerInterval: Initialize this JavaScript variable with the interval for polling the chat server to find if this surfer has been invited for a
      chat conversation by any CSR. Note that this is in number of seconds. Set this as the same as Polling Time option
      for Chat Media in BizAdmin.
sCheckURL: In this variable, replace the pattern :<TalismaChatWebServer> with your Chat Web Server's IP Address
                    and <TalismaChatApplication> with the name of the Web Application for Talisma Chat Web Component
sChatURL: In this variable, replace the pattern :<TalismaChatWebServer> with your Chat Web Server's IP Address
sContextProps: Fill in the context variables in this fashion - "Param3=value3&Param4=value4&Param5=Value5"
sTrackURL: This parameter contains the URL for the Talisma WebTrak Web Component. Give the URL with IP Address.
var sTimeURL: This parameter is for requesting the wait time and queue position without requesting for the chat.
DBSvr:    This parameter contains the Database Server machine name; This can be an IP address
DBName:  This parameter contains the WebTrack Database Name in the Database Server machine

All URLs - MUST give the complete URL - no relative paths;
Do not include the angle brackets "<" and ">" in the following variables as shown
IP addresses must be given where ever applicable for web server names
Provide the TrackURL, DBSvr and DBName values twice, once for the JavaScript variables and once for the applet parameters
Replace all  Single Words enclosed in angle brackets with approriate values

- This program sets visitorID, VisitID and ContactID in cookies on client;  If cookie already exists on client, get value
------------------------------------------------------------------------------------->
<SCRIPT LANGUAGE="javascript">
var TimerInterval = 7;    // Seconds   provide -1 if do not want to check
var sContextProps = '';

var sCheckURL = "http://chat.valueweb.com/Media/dlls/TLMediaU.dll";
var sChatURL = "http://chat.valueweb.com/Media/visitorchat/default.asp";
var sTimeURL = "http://chat.valueweb.com/Media/visitorchat/TimeCal.asp";
var sTrackURL = "http://chat.valueweb.com/WebTrak" ;

//DBSvr = "talisma1";
//DBSvr = "10.255.1.28";
DBSvr = "chat.valueweb.com";
//DBName = "tlWebTrak";
DBName = "tlWebTrak";

if ( top.frames.length == 0 || top.frames[0].name  != "CustPage" )
{
 StartChecking();
 if ( TimerInterval != -1 ) {
  vIntervalIDExists = false;
  IntervalID = window.setInterval("CheckOnTime()", TimerInterval*1000);
 }
}

function Is() {
   var agent = navigator.userAgent.toLowerCase();
    this.major = parseInt(navigator.appVersion);
    this.version=parseFloat(navigator.appVersion);
    this.ns  = ((agent.indexOf('mozilla')!=-1) && ((agent.indexOf('spoofer')==-1) && (agent.indexOf('compatible') == -1)));
    this.ns4 = (this.ns && (this.major >= 4));
    this.ns47 = (this.ns && (this.version == 4.7));
    this.ns61=(this.ns && (agent.indexOf("netscape6/6.1") != -1));
    this.ie   = (agent.indexOf("msie") != -1);
    this.ie4  = (this.ie && (this.major = 4));
 this.ie5 = (this.ie && (agent.indexOf("msie 5.") != -1));
 this.ie6= (this.ie && (agent.indexOf("msie 6.") != -1));
 this.windows = (agent.indexOf("win") != -1);
 this.AOL=(agent.indexOf("aol") != -1);
 this.mac = (agent.indexOf("mac") != -1);
 this.winXP=(agent.indexOf("XP") != -1);
}

var is = new Is();

function CheckBrowser(nonsales)
{
 //if nonsales is set that chat should be sent to different param3
 sUrl = location.href;
 if (nonsales){
	sContextProps = '&Custname=Affinity Customer&param3='+nonsales+'&param4='+sUrl+'&param5=valueweb';
 } else {
 	sContextProps = '&Custname=Affinity Customer&param3=VW_Sales&param4='+sUrl+'&param5=valueweb';
 }
 if(is.AOL)
 {
   //for browser not belonging to these categories
   window.open("/shared/pages//doesnotsupport.htm",null,"left=50,top=50,width=500,height=500,location=no,menubar=no,resizable=no,scrollbars=yes,status=yes,toolbar=no,titlebar=no");
                          return ;
  }



if(is.windows) //check for windows 95 , 98 and NT
{
 if(!is.winXP)

 if(is.ie5||is.ie6||is.ns47||is.ns61)
  return StartChatSession("false");
  //return StartChatSession("true");
   else
    {
    //for browser not belonging to these categories
    window.open("/shared/pages//doesnotsupport.htm",null,"left=50,top=50,width=500,height=500,location=no,menubar=no,resizable=no,scrollbars=yes,status=yes,toolbar=no,titlebar=no");
                                return ;
    }

 else

 if(is.ie6)
 return StartChatSession("false");
 //return StartChatSession("true");
    else
    {
  //for browser not belonging to these categories
            window.open("/shared/pages//doesnotsupport.htm",null,"left=50,top=50,width=500,height=500,location=no,menubar=no,resizable=no,scrollbars=yes,status=yes,toolbar=no,titlebar=no");
            return ;
    }
 }
 else

  if(is.mac)

  if(is.ns47||is.ns61)
  return StartChatSession("false");
  //return StartChatSession("true");
   else
  {
  //for browser not belonging to these categories
  window.open("/shared/pages//doesnotsupport.htm",null,"left=50,top=50,width=500,height=500,location=no,menubar=no,resizable=no,scrollbars=yes,status=yes,toolbar=no,titlebar=no");
  return ;
   }


  else   //if OS is neither mac nor Win then chat will not be initiated.
   {
  window.open("/shared/pages//doesnotsupport.htm",null,"left=50,top=50,width=500,height=500,location=no,menubar=no,resizable=no,scrollbars=yes,status=yes,toolbar=no,titlebar=no");
   return ;
     }
}


</script>
