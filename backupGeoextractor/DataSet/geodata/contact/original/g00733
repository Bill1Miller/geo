//	document.layer for NS4
//	document.getElementById for IE5 & NS6
//	document.all && !(document.getElementById) for IE4

function changeView(dir, id) {
	if (dir == 'visible') {dirNS4='show';}
	if (dir == 'hidden') {dirNS4='hide';}
	if (document.layers) {document.layers[id].visibility = dirNS4;}
	else if (document.getElementById) {document.getElementById(id).style.visibility = dir;}
	else if (document.all && !(document.getElementById)) {document.all(id).style.visibility = dir;}
}

function hideArray (myArray) {
	var thisArray=new Array;
	thisArray=eval(myArray);
	for (var i = 0; i <thisArray.length; i++) {
		changeView('hidden', thisArray[i]);
	}
}

function hideAll () {
	// Level 2 Menus
	hideArray('ary_level_two');
	// Level 3 Menus
	changeView('hidden','about_annual_reports');
	changeView('hidden','success_special_projects');
	hideArray('ary_offices');
	hideArray('ary_programs');
	hideArray('ary_programs');
	hideArray('ary_whats_new');
	hideArray('ary_top_offices');
	return true;
}

function protect () {
	var onLayer=true;
	return onLayer=true;
}

function hideIfOff () {

}

function MM_findObj(n, d) { //v4.01
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && d.getElementById) x=d.getElementById(n); return x;
}
//-->
</script>


