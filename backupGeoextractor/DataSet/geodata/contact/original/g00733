//initialize layer names
var ary_level_two = new Array("progs_services","about", "offices", "whats_new", "success_stories", "employment", "top_educational");
var ary_offices = new Array("offices_egypt","offices_iraq","offices_jordan","offices_kuwait", "offices_lebanon", "offices_morocco", "offices_oman", "offices_qatar", "offices_syria", "offices_tunisia", "offices_uae", "offices_westbank_gaza", "offices_yemen");
