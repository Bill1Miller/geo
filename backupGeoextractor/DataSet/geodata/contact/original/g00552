function launch(page) {
        var str = "height=450,";
        str += "width=800,";
        str += "resizable=0,scrollbars=0";
        var remote = open(page, 'toolWin', str);
        if ( remote != null ) {
                remote.focus();
                remote.opener = self;
        }
}

function popupWindow(url) {
  window.open("","g4","height=140,width=326,menubar=0,resizable=0,scrollbars=1,status=0,titlebar=0,toolbar=0,left=0,top=0");
  return true;
}

