			<script language=javascript>
			function nleFrmSubmit()
			{
				if(document.newsLetterEmail.txtEmail.value.indexOf(".")<2 ||document.newsLetterEmail.txtEmail.value.indexOf("@")<1)
				{
					alert("Please Enter Valid Email Address");
					document.getElementById("isValidEmail").value = "false";
					return;
				}
				else
				{
					document.getElementById("isValidEmail").value = "true";
					
						document.newsLetterEmail.submit();
				}
				
			}
		</script>
	
	
