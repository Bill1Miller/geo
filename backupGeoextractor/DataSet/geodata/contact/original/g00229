<script language = "javascript">

function ProcessForm(frmThis)
{
	val = frmThis.sub_email.value;
	if (val == "" || val == "Your email")
	{
		alert("Please provide your email address");
		frmThis.sub_email.focus();
		return false;
	}
	
	return true;
}

</script>

