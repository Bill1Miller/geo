<script language = "javascript">

function IsEmail(stEmail)
{
	var iAtPos, iDotPos;
	
	// validate email address
	iLength = stEmail.length;
	if (iLength == 0)
		return false;
	else
	{
		iAtPos = stEmail.indexOf("@")
		if (iAtPos == -1 || iAtPos == 0)
			return false;
		
		iDotPos = stEmail.lastIndexOf(".")
		if ((iDotPos == -1) || (iDotPos <= (iAtPos + 1)))
			return false;

		iPosDiff = (iLength - iDotPos);
		if ((iPosDiff < 3) || (iPosDiff > 5))
			return false;
	}

	return true;
}


function ProcessMainForm(frmThis)
{

	val = frmThis.sub_email.value;
	if (!IsEmail(val))
	{
		alert("Please enter a valid email address");
		frmThis.sub_emailfocus();
		return false;
	}

	val = frmThis.comments.value;
	if (val == "")
	{
		alert("Please enter your comment or question");
		frmThis.comments.focus();
		return false;
	}

	frmThis.submit.disabled = true;
	return true;
}

</script>






