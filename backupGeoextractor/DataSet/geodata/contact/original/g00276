		function removespace(thestring)
  		{
		for (i=0; i < thestring.length; i++)
	    {
	    thestring = thestring.replace(' ', '_')
		} return thestring;
		}

function searchOpen() {
	var a = window.document.search.keywords.value; 
	var kw = removespace(a);
	var d = window.document.search.surfer.selectedIndex;
	var e = window.document.search.surfer[d].value;
	var r = window.document.search.role.selectedIndex;
	var s = window.document.search.role[r].value;
	
	var qs = 'keywords=' + kw + '&surfer=' + e + '&role=' + s;
	var junior = open('/surf/search/photos?' + qs,'juniors','scrollbars,resizable,height=600,width=750,status=yes');
}
