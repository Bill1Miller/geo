//this is the image rollover function
function changeImage(image_name,image_src) {
	//alert(image_name);
	document.images[image_name].src = image_src;
}
</script>
<script language="JavaScript">
window.name= "daddy";
function popThis(url){
	var junior = open(url,"junior","scrollbars,resizable,height=375,width=500");
}
function open_window(url) {
	mywin = window.open(url,"win",'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=yes,resizable=yes,width=600,height=300');
}
function popMailWin() {
	var pageURL   = escape(self.document.URL);
    var pageTitle = escape(self.document.title);
    var formURL   = "/cgi-bin/mail/mailurl2friend.cgi?path=/popsci/email&url=" + pageURL + "&group=popsci&title=" + pageTitle ;
	window.open(formURL,"emailpop","height=475,width=350,resizable,scrollbars");
	return false;
}
</script>
<SCRIPT>
	function openTarget (form, features, windowName) {
	  if (!windowName)
	    windowName = 'formTarget' + (new Date().getTime());
	  form.target = windowName;
	  open ('', windowName, features);
	}
</SCRIPT>
