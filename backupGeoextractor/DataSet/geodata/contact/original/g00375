<script language="javascript">
function RH_url_decode(key){
	if (window.location.search.indexOf(key + '=') > -1){
		var temp = window.location.search;
		temp = temp.substr(temp.indexOf(key + '=') + key.length + 1);
		if (temp.indexOf('&') > -1){
			temp = temp.substr(0, temp.indexOf('&'));
		}
		return temp;
	}
	return '';
}

