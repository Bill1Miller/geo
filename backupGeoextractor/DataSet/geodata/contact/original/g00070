function ChangeImage(ImageName, NewImage, Title, Directory)
{
if (document.images)
	{
	if (String (Directory) != "undefined")
		document[ImageName].src = "/" + Directory + "/" + NewImage;
	else
		document[ImageName].src = "/images/" + NewImage;
	}
	
	if (String (Title) != "undefined")
	window.status=Title;
	else
	window.status=' ';
}

