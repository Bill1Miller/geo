		function isCompatible()
		{
		 var BrowserName = navigator.appName;
		 var BrowserVer  = parseInt(navigator.appVersion);
		 var Compatible  = (BrowserName == "Netscape" && BrowserVer == 3) || (BrowserName == "Netscape" || BrowserVer == 4) || (BrowserName == "Microsoft Internet Explorer" || BrowserVer == 4)
		 return Compatible;
		}
		
		function flipIt(img, tothis)
		{
		 if (isCompatible())
		 {
