<script language="JavaScript">




function validateForm()      {
          
 if (window.document.contactus.NAME.value == '') {
              alert("Please provide your name.");
              window.document.contactus.NAME.focus();
             return false;
}
	else if (window.document.contactus.EMAIL.value.length < 9) {
              alert("Please provide a valid email address.");
              window.document.contactus.EMAIL.focus();
			  return false;
          }
          else if (window.document.contactus.CATEGORY.selectedIndex < 0) {
              alert("Please select a category for your submission.");
              window.document.contactus.EMAIL.focus();
             return false;
}
          
           else if (window.document.contactus.COMMENT.value == '') {
              alert("You must enter your message.");
              window.document.contactus.COMMENT.focus();
             return false;
}
}
</script>








                </head>




