  function buttonSwap(buttonName,styleFlag) {
      if (document.images) {
        if (styleFlag == "on") {
          document[buttonName].src = eval(buttonName + "on.src");
        } else {
          document[buttonName].src = eval(buttonName + "off.src");
        }
      }
  }

  
  var varBrowser = "Other";
  if(navigator.appName.indexOf("Netscape",0) == 0)
    {varBrowser = "Netscape";
  }else{
    if((navigator.appName.indexOf("Microsoft",0) == 0) || (navigator.appName.indexOf("MS",0) == 0))
    {varBrowser = "MSIE";}
  }

  if(document.images) {
  this[1] = new Image();
  this[1].src = "/Graphics/takeaction_on.gif";
  this[2] = new Image();
  this[2].src = "/Graphics/family_on.gif";
  this[3] = new Image();
  this[3].src = "/Graphics/worklife_on.gif";
  this[4] = new Image();
  this[4].src = "/Graphics/yourcommunity_on.gif";
  this[5] = new Image();
  this[5].src = "/Graphics/comingout_on.gif";
}

function swapIt(imgName,imgType,imgAction) {
      if (document.images) {
        if (imgAction == 1) {
          switch(imgType){
            case "Take_Action": newSrc="/Graphics/takeaction_on.gif"; break
          case "Family": newSrc="/Graphics/family_on.gif"; break
          case "Work_Life": newSrc="/Graphics/worklife_on.gif"; break
          case "Your_Community": newSrc="/Graphics/yourcommunity_on.gif"; break
          case "Coming_Out": newSrc="/Graphics/comingout_on.gif"; break
        }
        }
        else {
          switch(imgType){
            case "Take_Action": newSrc="/Graphics/takeaction_off.gif"; break
          case "Family": newSrc="/Graphics/family_off.gif"; break
          case "Work_Life": newSrc="/Graphics/worklife_off.gif"; break
          case "Your_Community": newSrc="/Graphics/yourcommunity_off.gif"; break
          case "Coming_Out": newSrc="/Graphics/comingout_off.gif"; break
        }
        }
        document.images[imgName].src = newSrc;
      }
    }
//-->
