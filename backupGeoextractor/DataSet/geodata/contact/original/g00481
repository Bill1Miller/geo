<!-- Begin
// Set slideShowSpeed (milliseconds)
var slideShowSpeed = 6000;
// Duration of crossfade (seconds)
var crossFadeDuration = 7;
// Specify the image files
var Pic = new Array();
// to add more images, just continue
// the pattern, adding to the array below
var Links = new Array();

Pic[0] = '/images/banner/animalcontrol_BANNER.gif'
Pic[1] = '/images/banner/whoami_banner.gif'
Pic[2] = '/images/banner/video_library.gif'
Pic[3] = '/images/banner/mygov.gif'
Pic[4] = '/images/banner/vol_intern.gif'
Pic[5] = '/images/banner/esl.gif'
Pic[6] = '/images/banner/citystore.gif'
Pic[7] = '/images/banner/bwm.gif'


<!--Comment: all links have to go thru /banner.asp for redirection to the destination so that user's choice of a banner can be captured in IIS logs-->
Links[0]="/banner.asp?goto=http://www.cityofboston.gov/animalcontrol";
Links[1]="/banner.asp?goto=http://www.cityofboston.gov/whoami";
Links[2]="/banner.asp?goto=http://www.cityofboston.gov/cable/video_library.asp";
Links[3]="/banner.asp?goto=http://mygov.cityofboston.gov";
Links[4]="/banner.asp?goto=http://www.cityofboston.gov/vi_opportunities";
Links[5]="/banner.asp?goto=http://www.cityofboston.gov/newbostonians/search.asp";
Links[6]="/banner.asp?goto=http://www.cityofboston.gov/citystore";
Links[7]="http://www.cityofboston.gov/women/memorial.asp";


// do not edit anything below this line
var t;
var j = 0;
var p = Pic.length;
var preLoad = new Array();
for (i = 0; i < p; i++) {
preLoad[i] = new Image();
preLoad[i].src = Pic[i];
}
function runSlideShow() 
{
	if (document.all) 
	{
		document.images.SlideShow.style.filter="blendTrans(duration=2)";
		document.images.SlideShow.style.filter="blendTrans(duration=crossFadeDuration)";
		document.images.SlideShow.filters.blendTrans.Apply();
	}
	
	document.images.SlideShow.src = preLoad[j].src;
	document.getElementsByName("showlink").item(0).href=Links[j];
	
	if (document.all) 
	{
		document.images.SlideShow.filters.blendTrans.Play();
	}
	
	j = j + 1;
	
	if (j > (p - 1)) j = 0;
	
	t = setTimeout('runSlideShow()', slideShowSpeed);
}
//  End -->
</script>
