<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript"><!--

	/****** functions ******/
	
function begin_hiding ( menuNum ) {
	menuToHide = menuNum ;
	prepare_hide( menuNum, 200 ) ;
}

function hide_all_but ( menuNum ) {
	for ( i = 0; i < numMenus; i++ ) {
		if ( i != menuNum ) {
			hide_menu(i) ;	
		}
	}
}

function hide_menu( menuNum ) {
	if (use_5th_gen_dom) {
		var menuDiv = document.getElementById("menu" + menuNum);
		menuDiv.style.visibility = "hidden";
	} else {
		eval( doc + "menu" + menuNum + style + visible + hide ) ;
	}
}

function highlight_item( menuNum, itemNum ) {

	var objectNum = (menuNum * 100) + itemNum ;
	var whichItem = "item" + menuNum + itemNum;

	if (use_5th_gen_dom) {
	    eval ( "document." + whichItem + ".src = '" + itemHighlightImg[objectNum] +"'" );
	} else {
	 
	 	if (use_ns4_dom) {
			whichMenu = "menu" + menuNum;
			document.layers[whichMenu].document[whichItem].src = itemHighlightImg[objectNum] ;
	 	} else {
			document[whichItem].src = itemHighlightImg[objectNum] ;
		}
	
	}
	prevent_hide( menuNum ) ;
}

function onMouseOut_hide_menu( menuNum, delay ) {
	menuToHide = menuNum ;
	prepare_hide( menuNum, delay ) ;
}

function shift_menus_up() {															// 2nd level pages: if menu drops UP, we'll move the whole DIV up onMouseOver
	if (dropsDown=="no") {

		if (use_5th_gen_dom) {
																																	// count number of menu items, not including "information for/about", and multiply by 17px
				document.getElementById('menu0').style.top = 123 - 204 + "px";						// move menu, wrapped in the DIV, up the specified number of pixels
				document.getElementById('menu1').style.top = 123 - 102 + "px";						// menu0 = "Information About"; menu1 = "Information For"
			
		} else {
		
			if (use_ns4_dom) {
					document.menu0.top = document.menu0.top - 204;
					document.menu1.top = document.menu1.top - 102;
				
			} else {
					document.all.menu0.pixelTop = document.all.menu0.pixelTop - 204;
					document.all.menu1.pixelTop = document.all.menu1.pixelTop - 102;

			}
			
		}
	}
}

function preload_images(dropdown_array) {
	for (var loop = 0; loop < dropdown_array.length; loop++) {
		var menu_item_image = new Image();
		if ( dropdown_array[loop] ) {
			menu_item_image.src = dropdown_array[loop];
		}
   }
}



function prepare_hide( menuNum, timeLeft ) {
	if ( menuToHide == -1 ) {
			// the menu has been rescued
	} else {
			// countdown to extinction
		if ( timeLeft > 0 ) {
				// don't hide it yet...
			nextCall = "prepare_hide(" + menuNum + "," + (timeLeft-100) + ")" ;
			setTimeout(nextCall, 100) ;
		} else {
				// hide it
			hide_menu( menuNum ) ;
			menuToHide = -1 ;
		}
	}
}

function prevent_hide( menuNum ) {
	if ( menuToHide == menuNum ) {
		menuToHide = -1 ;
	}
}

function set_vars() {
  if (use_ns4_dom || use_5th_gen_dom) {	
    horiz     =  ".left=" 	 ;
    vert      =  ".top=" 	 ; 
    doc       =  "document." 	 ;
    style     =  "" 		 ;
    visible   =  ".visibility"	 ;
    hide      =  "='hidden'" 	 ;
    show      =  "='visible'" 	 ;
  } else {
  
    if (use_ie4_dom) {				// IE4 and similar
    horiz     =  ".pixelLeft=" 	 ;
    vert      =  ".pixelTop=" 	 ;
    doc       =  "document.all." ;
    style     =  ".style" 	 ;
    visible   =  ".visibility"	 ;
    hide      =  "='hidden'" 	 ;
    show      =  "='visible'" 	 ;
    }
  }
}

function show_menu( menuNum ) {

	hide_all_but ( menuNum );
		
	if (use_5th_gen_dom) {
		var menuDiv = document.getElementById("menu" + menuNum);
		menuDiv.style.visibility = "visible";
	} else {
		eval( doc + "menu" + menuNum + style + visible + show ) ;
	}
						
	//onMouseOut_hide_menu( menuNum, 600 );	

}

function unhighlight_item( menuNum, itemNum ) {
	var objectNum = (menuNum * 100) + itemNum ;
	var whichItem = "item" + menuNum + itemNum ;

	if (use_ns4_dom) {
		whichMenu = "menu" + menuNum ;
		document.layers[whichMenu].document[whichItem].src = itemImg[objectNum] ;
	} else {
		document[whichItem].src = itemImg[objectNum] ;
	}
	
	begin_hiding( menuNum ) ;
}

/****** main program ******/

set_vars() ;
shift_menus_up();
preload_images(itemHighlightImg) ;

//-->
</SCRIPT>


