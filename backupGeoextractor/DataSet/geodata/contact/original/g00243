			<script language="javascript">
			if(navigator.appName == 'Netscape')
				{
				if(navigator.platform.toLowerCase().substr(0,3) != 'win')
					{
					document.write('<input type=\"text\" name=\"searchstring\" size=\"18\" maxlength=\"50\">');
					}
				else
					{
					document.write('<input type=\"text\" name=\"searchstring\" size=\"12\" maxlength=\"50\">');
					}
				}
			else
				{
