function setCookie2(theName) {
var exp = new Date();  
var oneDayFromNow = exp.getTime() + 1*24*60*60*1000;
exp.setTime(oneDayFromNow);
document.cookie = theName + "=TRUE; expires=" + exp.toGMTString() + "; path=/";
exp = new Date();
var FiveMinutesFromNow = exp.getTime() + 4*60*1000;
exp.setTime(FiveMinutesFromNow);
