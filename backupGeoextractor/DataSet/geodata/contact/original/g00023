<!-- begin circ-exitPop script -->

<script language="JavaScript1.2">


function setExitCookie() {
                
                var exp = new Date();  
                var oneHourFromNow = exp.getTime() + 12*1*60*60*1000;
                exp.setTime(oneHourFromNow);
                document.cookie = "exit_pop=TRUE; expires=" + exp.toGMTString() + "; path=/";


                }



function ExitPopCheck (ev) {


        if (navigator.appName.indexOf("Microsoft") != -1  && (navigator.appVersion.indexOf('5.') != -1) || (navigator.appVersion.indexOf('6.') != -1)) {

                ev = window.event;
                var node = ev.srcElement;
                while (!(node.hostname) && node.parentNode) {
                        node = node.parentNode;
                }
                if (node.hostname) {
                        Obj.hostname = node.hostname;
                }
        }
}


function getCookie2 (name) {
	var cookiename = name + '=';
	if (document.cookie.indexOf(cookiename) == -1) {
		return false;
	} else {
		return true;
	}
}

function ExitPopShow (ev) {

        if (navigator.appName.indexOf("Microsoft") != -1  && (navigator.appVersion.indexOf('5.') != -1) || (navigator.appVersion.indexOf('6.') != -1)) {
			
			if ((Obj.hostname.indexOf('usnews') == -1) && (Obj.hostname.indexOf('palmcoastd.com') == -1) && (Obj.hostname.indexOf('clickTAG') == -1))  {

                        if (!(getCookie2('exit_pop'))) {

				window.open("/usnews/usinfo/popUp3.htm","PopUP3","width=400,height=400,titlebar=0,toolbar=0,location=0,menubar=0,scrollbars=0,resizable=1,channelmode=0,directories=0,status=0");
				setExitCookie();


						}
                }
        }
}

var Obj = new Object;
Obj.hostname = '';
document.onclick = ExitPopCheck;
window.onunload = ExitPopShow;

</script>
<!-- end circ-exitPop script -->

