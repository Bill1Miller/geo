} else { 
		document.write("<a href='/user_registration'>Log in<\/a>&nbsp;|&nbsp;<a href='/register-bin/interface.pl.cgi?mode=register'>Sign up!<\/a>"); 
} 

document.write("&nbsp;&nbsp;&nbsp;");

