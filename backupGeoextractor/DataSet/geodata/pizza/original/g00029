	<script language="javascript" type="text/javascript">
		function toggleHours(p_iCurrentDay){
					
			var iSubSet
				
			for(x=1; x<8; x++){
			
				//skip the current day				
				if(x != p_iCurrentDay){					
					g_oTR = document.getElementById("tr_" + x)					
					//call function to set the display...
					g_iDisplay = setDisplay(g_oTR)
					//alert(g_iDisplay)
					//now we have to check to see if there are extra hour sets for the same day.
					//max is 3 per day.					
					for(iSubSet = 1; iSubSet<4; iSubSet++){
						g_oTR = document.getElementById("tr_" + x + "_" + iSubSet)
						if(g_oTR){
							//if here, there is another entry for this day...
							//call function to set the display...
							setDisplay(g_oTR)
						}
					}				
				}
			}
							
			//now highlight the day of the week when we show all
			g_oArestHours = document.getElementById("resthours")
			g_oTR = document.getElementById("tr_" + p_iCurrentDay)
			if(g_iDisplay == 1){					
				g_sBG = "#ffc"
				g_oArestHours.innerHTML = "view today\'s"
			} else {
				g_sBG = "#fff"
				g_oArestHours.innerHTML = "view all"
			}
			
			//set the color...
			g_oTR.style.backgroundColor = g_sBG
			//now check for any extra sets....
			for(iSubSet = 1; iSubSet<4; iSubSet++){
				g_oTR = document.getElementById("tr_" + p_iCurrentDay + "_" + iSubSet)
				if(g_oTR){
					//if here, there is another entry for this day...
					//call function to set the display...
					g_oTR.style.backgroundColor = g_sBG
				}
			}
					
		}
		
		
	function setDisplay(p_oTR){
		if(p_oTR.style.display == 'none'){
			p_oTR.style.display = ''						
			l_iDisplay = 1
		} else {
			if(p_oTR.style.display == ''){
				p_oTR.style.display = 'none'	
				l_iDisplay = 0						
			}						
		}
		return l_iDisplay
	}
	</script>
