<script language="JavaScript" type="text/javascript">

var maintainMenu = false;
var activeSubMenu = "";
var menuConfigTimeout = 250;
var isOpera = false;

function ypInitMenus() {
	if (navigator.userAgent.indexOf("Opera")!=-1) {
		var versionindex = navigator.userAgent.indexOf("Opera") + 6
		if (parseInt(navigator.userAgent.charAt(versionindex)) >= 8) {
			isOpera = true;
		}
	}
}

function displaySubMenu(theSubMenu, theSubMenuHeight, theSubMenuWidth) {
	if (!isOpera) {
		hideSubMenu();
		activeSubMenu = theSubMenu;
		maintainMenu = true;
		container = document.getElementById(theSubMenu);
		subMenus = container.getElementsByTagName("div");
		for (i = 0; i < subMenus.length; i++) {
			subMenus[i].style.width = theSubMenuWidth + "px";
			subMenus[i].style.top = "-" + theSubMenuHeight + "px";
			subMenus[i].style.display = "block";
		}
		
		container.style.display = "inline";
		container.style.zIndex = "999";
		container.style.height = theSubMenuHeight+"px";
		container.style.width = theSubMenuWidth+"px";
		container.style.overflow = "hidden";
	}
}

function hideSubMenu() {
	theSubMenu = document.getElementById(activeSubMenu);
	if (theSubMenu != null && !maintainMenu) {
		theSubMenu.style.display = "none";
		activeSubMenu = "";
	}
}

function tryHideSubMenu() {
	if (!isOpera) {
		maintainMenu = false;
		if (document.getElementById(activeSubMenu) != null) {
			menuTimeOut = window.setTimeout("hideSubMenu()", menuConfigTimeout);
		}
	}
}
</script>

