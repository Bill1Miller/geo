function popup (t, l, st, m, s, r, w, h, url) {
	hide_popup();

	if (w==0 && h==0)
		popupWin = window.open(url,'offers','toolbar=yes,location=yes,directories=yes,status=yes,menubar=yes,scrollbars=yes,resizable=yes');
	else {
		var opt = "toolbar=" + t + ",location=" + l + ",directories=no" + ",status=" + st + ",menubar=" + m + ",scrollbars=" + s + ",resizable=" + r + ",width=" + w + ",height=" + h;
		popupWin = window.open(url,'offers',opt);
		popupWin.focus();
	}
}
</script>
