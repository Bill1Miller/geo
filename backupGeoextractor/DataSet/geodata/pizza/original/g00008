// Function for logging ULT clicks for Results page
var res_imgBeacon = new Image();
res_imgBeacon.src = "img/b.gif";

function log_res_beacon(slk) {
 if (typeof(res_cv_ult[slk]) != 'undefined' && res_cv_ult_oneflag[slk]==0) {
  res_imgBeacon.src = res_cv_ult[slk];
  res_cv_ult_oneflag[slk]=1;
 }
}

function tog_facet2(o,id,onoff) {
	var x = ygPos.getX(o.previousSibling);
	var y = ygPos.getY(o.previousSibling);
	a = Array(x,y);
	if( onoff == 1 ) {
	 	d.getElementById(id).style.display = "block";
		ygPos.setPos(d.getElementById(id),a);
	}
	else if( onoff == 0 ) {
		d.getElementById(id).style.display = "none";
	}
}

