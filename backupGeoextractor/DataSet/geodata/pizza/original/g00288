function google_ad_request_done(google_ads) {  

		var first_ad_unit 	= '';
		if (google_ads.length > 0) {
				
				first_ad_unit 	+= '<div class="googleads"><ul>';
	
			for (var i = 0; i < google_ads.length; ++i) {
								
			first_ad_unit  += '<li><a href=\"'
            + google_ads[i].url
            + '\"><b>'
            + google_ads[i].line1
            + '</b><div>'
            + google_ads[i].line2 + ' '
            + google_ads[i].line3
            + '</a></div>';

			}
				first_ad_unit 	+= '</ul><br /></div>';

		}

		document.write(first_ad_unit);
		//document.getElementById("first_ad_unit").innerHTML += first_ad_unit;
}



google_ad_client		= "pub-9464321798359467";
google_ad_output		= 'js';
google_max_num_ads	= '4'; 
google_feedback		= "on";
google_ad_type			= "text_image";

</script>

