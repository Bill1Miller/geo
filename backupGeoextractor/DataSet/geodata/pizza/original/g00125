<!--

function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		home_02_over = newImage("images/home_02-over.gif");
		home_03_over = newImage("images/home_03-over.gif");
		home_04_over = newImage("images/home_04-over.gif");
		home_05_over = newImage("images/home_05-over.gif");
		home_06_over = newImage("images/home_06-over.gif");
		home_07_over = newImage("images/home_07-over.gif");
		home_08_over = newImage("images/home_08-over.gif");
		preloadFlag = true;
	}
}

