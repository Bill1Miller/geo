//function toggles between facets
function tog_facet(o,isShw) {
	oDL = o.parentNode.parentNode;
	oDL.style.display="none";
	if(isShw)
		oDL.previousSibling.previousSibling.style.display="block";
	else
		oDL.nextSibling.nextSibling.style.display="block";
	ylspftr();
}

//toggles User Recommended full review and snippet
function tog_review(o,o2,onOff) {
  if( onOff == 1 ) {
	d.getElementById(o).style.display = "block";
    d.getElementById(o2).style.display = "none";
  }
  else if( onOff == 0 ) {
	d.getElementById(o).style.display = "none";
    d.getElementById(o2).style.display = "block";
  } 
}

//function positions footer
function ylspftr() {
	if(d.getElementById('yls_sdbr')) {
		ylss=d.getElementById('yls_sdbr');
		ylsc=d.getElementById('yls_cntnt');
		if(ylss.offsetHeight>ylsc.offsetHeight) 
			ylsc.style.height=20+ylss.offsetHeight +"px";
		else if(ylsc.offsetHeight>ylss.style.offsetHeight)
			return true;
	}
}
addEvent(window, 'load', ylspftr);addEvent(window, 'resize', ylspftr);
