function tog_facet2(o,id,onoff) {
	var x = ygPos.getX(o.previousSibling);
	var y = ygPos.getY(o.previousSibling);
	a = Array(x,y);
	if( onoff == 1 ) {
	 	d.getElementById(id).style.display = "block";
		ygPos.setPos(d.getElementById(id),a);
	}
	else if( onoff == 0 ) {
		d.getElementById(id).style.display = "none";
	}
}

