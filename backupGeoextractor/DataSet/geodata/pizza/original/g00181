<script>
var d = document;
var curW = 210;
var curH = 210;
var curX = 0;
var curY = 0;
var MAPSM = 210;
var MAPLG = 400;
var timerId = "";
var timerId2 = "";
var intMap = d.getElementById("mpoi_container");
var growMap = d.getElementById("lhs_image");
var thumb = d.getElementById("thumb");
var cState = 0;

function init() {
  if (!d.getElementById) return;
  yg_addEvt(thumb, "mouseover", genOpenMap(), false);
  yg_addEvt(thumb, "mouseout", genCloseMap(), false);
  curX = ygPos.getX(thumb);
  curY = ygPos.getY(thumb);
  a = Array(ygPos.getX(thumb),ygPos.getY(thumb));
  ygPos.setPos(growMap,a);
  ygPos.setPos(intMap,a);
  growMap.style.width = MAPSM+"px";
  growMap.style.height = MAPSM+"px";
}
 
function genOpenMap() {
  return function(e) {
    timerId2 = setTimeout("openMap()",500);
  }
}

function genCloseMap() {
  return function(e) {
    clearTimeout(timerId2);
  }
}

function openMap() {
    growMap.style.display = "block";
    var len = ( d.all ) ? 40 : 15;
    var oAnim = new ygAnim_Size(growMap, len, [ [MAPLG, MAPLG] ]);
    oAnim.animate();
    timerId = setTimeout("openPause()",430);
    return false;
}

function openPause() {
  intMap.style.display = "block";
  curW = MAPLG;
  curH = MAPLG;
  var trackC = trackIt();
  yg_addEvt(d, "mousemove", trackC, false);
  cState = 1;
  clearTimeout(timerId);
}

function closeMap() {
  intMap.style.display = "none";
  var len = ( d.all ) ? 20 : 10;
  var oAnim = new ygAnim_Size(growMap, len, [ [MAPSM, MAPSM] ]);
  oAnim.animate();
  timerId = setTimeout("closePause()",230);
  return false;
}

function closePause() {
  growMap.style.display = "none";
  curW = MAPSM;
  curH = MAPSM;
  curX = ygPos.getX(thumb);
  curY = ygPos.getY(thumb);
  cState = 0;  
  clearTimeout(timerId);
}

function trackIt() {
  return function(e) {
    var offSetY = (d.all) ? d.documentElement.scrollTop : window.pageYOffset;
    //d.getElementById('debug').innerHTML = "X = " + e.clientX + " curX = " + curX + " curW = " + curW +
    //  " #--# Y = " + e.clientY + " curY = " + curY + " curH = " + curH +
    //  " offset = " + offSetY; 
    if( cState == 1 ) {
      if( e.clientX > curX + curW || e.clientX < curX ||
          e.clientY > (curY + curH - offSetY) || 
          e.clientY < (curY - offSetY) ) {
          closeMap();
      }
    }
  } 
}

function yg_addEvt(o,e,f,c){ if(o.addEventListener)o.addEventListener(e,f,c);else if(o.attachEvent)o.attachEvent("on"+e,f);else eval("o.on"+e+"="+f)}

init();
</script>
</html>


