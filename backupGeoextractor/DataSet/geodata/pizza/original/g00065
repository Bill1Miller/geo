	<script language=javascript>
function showDetail(n)
{
	var el = document.getElementById('divSpace' + n);
	var myEl = eval('document.forms[0].page_btn_show'+n);
	if(myEl.src.indexOf('icon_plus') > -1)
	{
		document.getElementById('SpaceRowMain' + n).style.backgroundColor = '#d3d3d3';
		document.getElementById('SpaceRowAdditional' + n).style.backgroundColor = '#d3d3d3';
		el.style.display = "block";
		eval('document.images["page_btn_show' + n + '"].src = "http://images.loopnet.com/images/Listing/AddEdit/icon_minus.gif"');
	}
	else
	{
		document.getElementById('SpaceRowMain' + n).style.backgroundColor = '#ffffff';
		document.getElementById('SpaceRowAdditional' + n).style.backgroundColor = '#ffffff';
		el.style.display = "none";
		eval('document.images["page_btn_show' + n + '"].src = "http://images.loopnet.com/images/Listing/AddEdit/icon_plus.gif"');
	}
}
</script>


