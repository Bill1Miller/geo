	var aPathArray = new Array();
	var aOriginalPathArray = new Array();
	var aCaptionArray = new Array();
	var aFileSizeArray = new Array();
	var aFileColorArray = new Array();
	var aPageTitleArray = new Array();
 aPathArray[0] = 'http://www.loopnet.com/Attachments/3/B/9/3B958A14-81D5-4B31-9A3F-8CD2E7F05DAE.JPG';
 aOriginalPathArray[0] = 'http://www.loopnet.com/Attachments/3/B/9/3B958A14-81D5-4B31-9A3F-8CD2E7F05DAE_or.JPG';
 aPageTitleArray[0] = 'LoopNet - D\\\'Amore\\\'s Pizza Connection, Restaurant, Los Angeles, CA';
 aCaptionArray[0] = 'D\'Amore\'s Pizza Connection';
 aFileSizeArray[0] = '2 MB';
 aFileColorArray[0] = 'red';
	function ShowImage(index)
	{
		var img = new Image();
		img.src = aPathArray[index];
		htm = '<a href="javascript:makePopupProfileImage(\'ProfileImage.aspx?LID=14465617&STID=&Index=' + index + '&ImageTitle=' + aPageTitleArray[index] + '\', 480, 720);"><img border=0 src="' + aPathArray[index] + '" width=328 height=246></a>';
		var divImg = document.getElementById('divPhoto');
		divImg.innerHTML = htm;
		document.Profile.currentImage.value = index;
		var divCnt = document.getElementById('divCount');
		if (divCnt != null)
			divCnt.innerHTML = index + 1;
		var divEnl = document.getElementById('divEnlarge');
		var enlHtm = '';
		if (aCaptionArray[index] != "")
		{
			enlHtm += aCaptionArray[index] + ' - ';
		}
		enlHtm += '<a href="javascript:makePopupProfileImage(\'ProfileImage.aspx?LID=14465617&STID=&Index=' + index + '&ImageTitle=' + aPageTitleArray[index] + '\', 480, 720);">Click to Enlarge</a>';
		enlHtm += '&nbsp;(<font color=' + aFileColorArray[index] + '>' + aFileSizeArray[index] + '</font>)';
		divEnl.innerHTML = enlHtm;
	}
	function ShowNextImage()
	{
		var curImg = document.Profile.currentImage.value;
		curImg++;
		if (curImg == document.Profile.totalImages.value)
		{
			curImg = 0;
		}
		ShowImage(curImg);
	}
	function ShowPreviousImage()
	{
		var curImg = document.Profile.currentImage.value;
		curImg--;
		if (curImg < 0)
		{
			curImg = document.Profile.totalImages.value - 1;
		}
		ShowImage(curImg);
	}
ShowImage(0);
</script>


