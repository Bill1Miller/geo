</table><script language='javascript'>
var IE4 = (document.all && !document.getElementById)?1:0; //ver 4
var IE6 = (document.all&&document.getElementById)?1:0;    //ver 5 & 6
var NS4 = (document.layers)?1:0;						  //ver 4
var NS6 = (document.getElementById && !document.all)?1:0; //ver 6
var IE = (IE4||IE6)?1:0;
var NS = (NS4||NS6)?1:0;
if(IE){
	document.onkeydown = function(){
		if((window.event.ctrlKey)&&(window.event.shiftKey)){
			switch(event.keyCode){
				case 84://T
					gotoTrace();break;
				case 83://S
					setTrace();break;
			}
		}
	}
}else if(NS4){
	document.onkeydown = function(e){
		if((e.modifiers & Event.CONTROL_MASK)&&(e.modifiers & Event.SHIFT_MASK)){
			switch(parseInt(e.which)){
				case 20://T
					gotoTrace();break;
				case 19://S
					setTrace();break;
			}
		}
	}
}else if(NS6){
	document.onkeydown = function(e){
		if((e.ctrlKey)&&(e.shiftKey)){
			switch(e.keyCode){
				case 84://T
					gotoTrace();break;
				case 83://S
					setTrace();break;
			}
		}
	}
}
function gotoTrace(){
	top.window.location = '/trace.asp'
}
function setTrace(){
	if(typeof getCookie != 'function')return;
	toggle = (getCookie('Trace')!=null)?(getCookie('Trace').indexOf('f')>-1):true;
	var today = new Date(),expires = new Date();
	expires.setTime(today.getTime() + 3600000*24*30);
	setCookie('Trace', toggle, expires, '/');
	location.reload();
}
