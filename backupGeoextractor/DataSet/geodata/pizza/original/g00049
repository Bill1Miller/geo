<script language="javascript">
	function setDisplayInterestLevel(level) {
		for(i = 1; i <= 4; i++) {
			var starImg = "starnobg.gif";
			if( i <= level ) {
				starImg = "starbg.gif";
			}
			var imgName = 'starNum'+i;
			document.images[imgName].src="/images/06/places/"+starImg;
		}
	}
	function resetDisplayInterestLevel() {
		setDisplayInterestLevel(document.ReviewForm.interestLevel.value);
	}	
	function setFormInterestLevel(level) {
		document.ReviewForm.interestLevel.value = level;
	}
</script>

