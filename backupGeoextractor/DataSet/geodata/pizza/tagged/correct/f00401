</script>
<script>
function toggleVoteSection(reviewNum, voteUrl, yesVote, positive, total){
var img = new Image();
img.src = voteUrl;
document.getElementById('voteSection'+reviewNum).innerHTML = "<span>Thank you for your feedback!</span>";
document.getElementById('yesFeedback'+reviewNum).innerHTML = "Yes";
document.getElementById('noFeedback'+reviewNum).innerHTML = "No";
document.getElementById('feedbackSection'+reviewNum).style.color = "gray";
var totalFeedback = parseInt(total) + 1;
var positiveFeedback = yesVote ? parseInt(positive) + 1 : parseInt(positive);
if(yesVote)
document.getElementById('feedbackNumbers'+reviewNum).innerHTML = positiveFeedback + " of " + totalFeedback + " found this review helpful";
else
document.getElementById('feedbackNumbers'+reviewNum).innerHTML = positiveFeedback + " of " + totalFeedback + " found this review helpful";
}
</script>
