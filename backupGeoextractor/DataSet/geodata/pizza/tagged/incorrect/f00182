var a = Array(x+3,y+47);
ygPos.setPos(dd,a);
dd.id = "ylsmylocdd";
d.body.appendChild(dd);

c += '<div class="ylsmylocddborder">';
c += '<i>Use this menu to access recently used locations on this computer.</i>';
c += '<i>You can also <a href="http://login.yahoo.com/config/login?.src=lcl&.done=http%3A%2F%2Flocal.yahoo.com%2Fdetails%3Fid%3D29515408" onClick="log_myloc_beacon(\'signinloc\')">Sign In</a>';
