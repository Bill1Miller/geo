</div><div id="_ctl6_86c8d8ea-4023-426f-b686-2f9469e7224e"><SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript">
CallGGWithQS();

var GGRunning = false;
function CallGGWithQS() {
	var SiteID = '700';
	var AccID = '1';
    var ref = (parent==self?window.document.referrer:top.document.referrer);
    var qs = '*GGWINLOC=' + (window.location) + ('&*') + 'GGREF=' + (ref) + ('*&') + 'GGSiteID=' + (SiteID)  + ('&') + 'GGAccID=' + (AccID)+ ('&') + '&GGJavaON=true' + 'GGJava=' + (navigator.javaEnabled());
    if(typeof(screen)=="object")
		{
        qs += ('&') + 'GGWinDim='+screen.width+'x'+screen.height + ('&') + 'GGCD=' + screen.colorDepth;
        };

  
    var GGCallingImage = '<img name="GGCallingImage" border=0 height=1 width=1 onLoad="setImgSource(\'GGTrackerImg\',\'' + qs + '\')">';
    var GGTrackerImg = '<img name="GGTrackerImg" border=0 height=1 width=1>';
    document.write(GGCallingImage);
    document.write(GGTrackerImg);
    setImgSource('GGCallingImage', ''); 
}

function setImgSource(img, qs)
{

   if((!document.images) || GGRunning) return;
   GGRunning = true;
   
   if(img == "GGCallingImage")
   {
      var newSource = 'http'+(document.URL.indexOf('https:')==0?'s':'')+ '://www.GulliverGOLD.com/space.gif?' + qs;
   }
   else if(img == "GGTrackerImg")
   {
      var newSource = 'http'+(document.URL.indexOf('https:')==0?'s':'')+ '://www.GulliverGOLD.com/track.asp?' + qs;
   }
   if(document.images[img].src.indexOf(newSource)<0)
   {
      document.images[img].src = newSource;
   }
   GGRunning = false;
}

</SCRIPT>
