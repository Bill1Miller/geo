		<div class="body">
							<SCRIPT LANGUAGE="JavaScript">
					
					function validateInput(form) {
						document.all["noCity"].style.display = "none";
						document.all["noDate"].style.display = "none";
						document.all["pastDate"].style.display = "none";
						document.all["longStay"].style.display = "none";
						document.all["roomsPerPerson"].style.display = "none";
						var cont = true;
						
						if (form.city.value.length == 0 ) {
						
							document.all["noCity"].style.display = "block";
							cont = false;
						}

						if ((form.checkInDate.value == -1) || (form.checkInMonthYear.value == -1) || (form.checkOutDate.value == -1) || (form.checkOutMonthYear.value == -1)) {
							document.all["noDate"].style.display = "block";
							cont = false;
						}
						else {
							var today = new Date();
							var checkIn = new Date();
							var checkInDate = form.checkInDate.value;
							var checkInMonthYear = form.checkInMonthYear.value;
							var checkInMonth = checkInMonthYear.substring( 0 , checkInMonthYear.length - 4 );
							var checkInYear = checkInMonthYear.substring( checkInMonthYear.length - 4 , checkInMonthYear.length );
							checkIn.setYear(checkInYear);
							checkIn.setMonth(checkInMonth);
							checkIn.setDate(checkInDate);

							if ( checkIn < today ) {
								document.all["pastDate"].style.display = "block";
								cont = false;
							}
							else {
								var checkOut = new Date();
								var checkOutDate = form.checkOutDate.value;
								var checkOutMonthYear = form.checkOutMonthYear.value;
								var checkOutMonth = checkOutMonthYear.substring( 0 , checkOutMonthYear.length - 4 );
								var checkOutYear = checkOutMonthYear.substring( checkOutMonthYear.length - 4 , checkOutMonthYear.length );
								checkOut.setYear(checkOutYear);
								checkOut.setMonth(checkOutMonth);
								checkOut.setDate(checkOutDate);
								
								if ( checkOut - checkIn > 8553600000 ) {
									document.all["longStay"].style.display = "block";
									cont = false;
								}
							}
						}

						if ( ((form.numberOfAdults.value * 1 + form.numberOfChildren.value * 1) / ( form.numberOfRooms.value * 1 )) > 5 ) {
							document.all["roomsPerPerson"].style.display = "block";
							cont = false;
						}

						if ( cont == true ) {
							//dont show on these pages right now
							//showProcPage();
						}
						else {
							return false;
						}
					}
				</SCRIPT>

