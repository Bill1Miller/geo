 function changeArea(obj){
   storeWhere(obj.options[obj.selectedIndex].value);
   storeComparisonWhere();setChangeAreaValuesForHotels();
   if (typeof(document.forms['anythingTop']) != "undefined") {
      document.forms.anythingTop.submit(); 
      return true ;
    }
   return ;
 } 
//--></script>


