								<script type="text/javascript">
								function check_retention() {
								//HANH IS THE MAN
									// if not checked remove values
									if (!document.getElementById('retain').checked)	{
										document.getElementById('rating').value = '';
										document.getElementById('brand').value = '';

										//loop thru and remove amenities
										 var oform = document.getElementById('minisearch');
										 var iNumElems = oform.elements.length;
										 var oElem;

	 								     for (var i = 0; i < iNumElems; i++) {
											oElem = oform.elements[i];
											var amenity_check = new RegExp("amenity");
											if ((("HIDDEN" == oElem.type) || ("hidden" == oElem.type)) && (amenity_check.test(oElem.id))) {
												oElem.value = '';
											}
										 }
									}
									return true;
								}
								</script>

