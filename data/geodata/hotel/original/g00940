<SCRIPT type="text/javascript">

function searchLandmark()
	{
	//document.resrvForm.action = "http://www.res99.com/nexres/news/subscribe.cgi"
	var ldFieldName = 'selected_landmark_combo';
	var selIndex = document.landmarkForm.selected_landmark_combo.selectedIndex;
	document.landmarkForm.selected_polygon.value = '';
	if (document.landmarkForm.selected_landmark_combo.options[selIndex].value)
		{
		document.landmarkForm.submit();
		}
	}

function searchPoly()
	{
	var selIndex = document.landmarkForm.selected_polygon.selectedIndex;
	document.landmarkForm.selected_landmark_combo.value = '';
	if (document.landmarkForm.selected_polygon.options[selIndex].value)
		{
		document.landmarkForm.submit();
		}
	}

function displayForm(elementId)
	{
	var styleIs = document.getElementById(elementId).style.display;
	//alert('stulie id = ' + styleIs);

	if (styleIs == 'block')
		{
		styleIs = 'none';
		}
	else
		{
		styleIs = 'block';
		}
	document.getElementById(elementId).style.display = styleIs;
	return;
	}

function hideForm(elementId)
	{
	//alert('element id = ' + elementId);
	document.getElementById(elementId).style.display = 'none';
	return;
	}

function validateBox(type)
{
	var totalRadioButton = document.compareForm.hid.length;
	var checkedBox = 0;
	if (typeof document.compareForm.hid.length == "undefined")
		{
		if (document.compareForm.hid.checked){
			checkedBox++;
			}
		}
	else {
		for (var i=0; i<totalRadioButton; i++){
			if (document.compareForm.hid[i].checked){
				checkedBox++;
			}
		}
	}

	if(type=='compare'){
		if(checkedBox >=2 && checkedBox<=5 || getCookieValue("HID"))
		{
			document.compareForm.action = 'compare.cgi';
//			document.compareForm.method = 'GET';  // CR4011
			document.compareForm.target='_new';
			document.compareForm.submit();
		}
		else{
			alert("Please select at least two hotels to compare and no more than five");
		}
		return;
	}

	if (type=='map'){
		if(checkedBox > 0 && checkedBox<=5 || getCookieValue("HID")){
			document.compareForm.action = '/nexres/hotels/map_hotels.cgi';
			document.compareForm.target='_new';
			document.compareForm.submit();
		}
		else{
				alert("Please select at least one hotels to map and no more than five");
			}
		return;
	}

}

function hotelCheckBox(chkbox)
{
	if (chkbox.checked == true){
		if (!getCookieValue("HID")) {
			// no cookie present.  Set new cookie
			writeSessionCookie("HID",chkbox.value);
		}
		else{
			//append current value to cookie
			var cookieVal = getCookieValue("HID");
			if (window.RegExp) {
				var htlregstr = cookieVal;
				var reg1 = new RegExp(htlregstr);
				if (!reg1.test(chkbox.value)){
					cookieVal = cookieVal + "|"+chkbox.value;
					writeSessionCookie("HID",cookieVal);
				}
			}
		}
	}
	else{
	// remove hotel value from cookie
			var reg1str = getCookieValue("HID");
			if (reg1str){
				var newString = reg1str.replace(chkbox.value,'');
				if (window.RegExp) {
					newString = newString.replace(/(\|$)|(^\|)/g,'');
					newString = newString.replace(/(\|\|)/g,'|');
				}
				writeSessionCookie("HID",newString);
			}
	}
}

</SCRIPT>

