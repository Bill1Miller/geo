	// Pass in the form object from the pclub login
	function validateAndSubmitLogin(theForm) {
		var errMsg = '';
		var pcnStr = theForm.login.value;
		var pinStr = theForm.password.value;

		if(theForm.login.value == '' || theForm.password.value == '') {
