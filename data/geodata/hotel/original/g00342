	<SCRIPT type="text/javascript">
		function getRateList(){
			// All values set from variables declared at the Page Level
		    document.multiForm.propertyID.value = propertyID;
		    document.multiForm.requestedChainCode.value = requestedChainCode;
		    document.multiForm.requestedAffiliationCode.value = requestedAffiliationCode;
		    document.multiForm.theBrand.value = requestedAffiliationCode;
		    document.multiForm.language.value = LANG_CODE;
		}
	</SCRIPT>
