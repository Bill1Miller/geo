	<SCRIPT language="javascript">
	document.getElementById("txtDateCheckIn").value = '08/05/2006'
	document.getElementById("txtDateCheckOut").value = '08/08/2006'

	var clnRoom = '';
	var clnAdults = '';
	var clnChildren = '';

	if(clnRoom >= 1){
		document.getElementById(ctlName + "_ddlRoom").value = clnRoom
		doGenDDL(document.getElementById(ctlName + "_ddlRoom"),'ddlAdult','ddlChildren')
	}
	if(clnAdults != ""){
		document.getElementById(ctlName + "_ddlAdult").value = clnAdults
	}
	if(clnChildren != ""){
		document.getElementById(ctlName + "_ddlChildren").value = clnChildren
	}
	</SCRIPT>

