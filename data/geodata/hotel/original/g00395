		var calStartDate = new CalendarPopup();
		var calEndDate = new CalendarPopup();
		calStartDate.setReturnFunction("setEndDate");
		function setEndDate(y,m,d){
			var oEndDate = addDays(new Date(y,m-1,d),3)
			document.getElementById("txtDateCheckIn").value=doAddZeroLeading(m)+"/"+doAddZeroLeading(d)+"/"+y;
			document.getElementById("txtDateCheckOut").value=doAddZeroLeading(oEndDate.getMonth()+1)+"/"+doAddZeroLeading(oEndDate.getDate())+"/"+oEndDate.getYear();
		}
	</SCRIPT>
