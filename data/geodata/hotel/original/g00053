var winpops;
function openpopup(myurl,id){
    var myurl = myurl || null;
    var id = id || null;
    if(typeof(myurl) == 'string'){
      if (IE){
        if(id == null) return;
        winpops=window.open('',"customer_frameset","width=725,height=450,location=yes,addressbar=no,menubar=yes,toolbar=yes,statusbar=yes,scrollbars=yes,resizable,left=250,top=150");
