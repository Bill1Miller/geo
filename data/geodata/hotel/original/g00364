	<SCRIPT LANGUAGE="JavaScript">
		var cboMonth;
		var cboDay;
		var iYearOffset = 0;
		cal.setReturnFunction("setDropdowns");

		function setDropdowns(y,m,d) {
			//document.forms[0].date11_year.value=y;

			y=Number(y);
			m=Number(m);
			d=Number(d);

			var thisYear = new Date();
			thisYear = thisYear.getFullYear();
			var calYear = y;

			//document.write((m + ((calYear - thisYear) * 12)));

			cboMonth.selectedIndex = ((m + ((calYear - thisYear) * 12)) - iMonthOffset);
			cboDay.selectedIndex = d;

			setDates(cboMonth);
		}

