function submitSearchForm(){
   document.searchHotelsForm.checkInDate.value = document.tmpSearchForm.tmpCheckInDate.value;
   document.searchHotelsForm.checkOutDate.value = document.tmpSearchForm.tmpCheckOutDate.value;
   document.searchHotelsForm.numberOfRooms.value = document.tmpSearchForm.numberOfRooms.value;
   document.searchHotelsForm.submit();
