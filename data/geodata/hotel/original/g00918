<script language="JavaScript">


function getArgValue(arg) {
    var args = new Object();
    var query = location.search.substring(1);
    var pairs = query.split("&");
    for (var i=0; i < pairs.length; i++) {
        var pos = pairs[i].indexOf('=');
        if(pos == -1) continue;
        var argname = pairs[i].substring(0, pos);
        var value = pairs[i].substring(pos+1);
        //args[argname] = unescape(value);
        if(argname == arg) {
           return unescape(value);
        }
    }
    return "";
}

function getRefValue(arg) {
    var args = new Object();
    var query = '' + document.referrer;
    query = query.substring((query.indexOf('?')) +1);
    var pairs = query.split("&");
    for (var i=0; i < pairs.length; i++) {
        var pos = pairs[i].indexOf('=');
        if(pos == -1) continue;
        var argname = pairs[i].substring(0, pos);
        var value = pairs[i].substring(pos+1);
        //args[argname] = unescape(value);
        if(argname == arg) {
           return unescape(value);
        }
    }
    return "";
}


function print_breadcrumb(location_name, do_metro, city_name, state_abbr, country_abbr, category, ecity_name, estate_abbr, ecountry_abbr, ecategory)
{
   var refPage;
   var page;
   var citystr;
   var stestr;
    var bcct;

   if (getRefValue("pgt") != null) { 
      refPage = getRefValue("pgt"); 
      page = getArgValue("pgt");
   }
   if(getArgValue("pgt") != null) {
      page = getArgValue("pgt");
   }
   if(getArgValue("ctystr") != null) {
      citystr = getArgValue("ctystr");
   }
   if(getArgValue("stestr") != null) {
      stestr = getArgValue("stestr");
   }
   if(getArgValue("bcct") != null) {
      bcct = getArgValue("bcct");
   }
    
    
   var cte  = getArgValue("cte");


    var breadcrumb = "<a href=\"http://www.nextel.com/\">Home</a> ";
    var locations_link = "<a href=\"search.html\">Locations</a>&nbsp;";
    var locations_nonlink = "Locations";
    var arrow_img = "<img hspace=\"5\" height=\"5\" width=\"7\" src=\"img/nav-img/img_arrow.gif\">&nbsp;";
    var category_link = "<a href=\"search.html?pgt=cat_list&cte="+ecategory+"\">" + category + "</a>";
    var country_name;

    
    if (country_abbr.length) 
           country_name = country_names[country_abbr];
    var state_name;
    if (country_abbr.length && state_abbr.length)
           country_name = countries[country_abbr][state_abbr] + ', ' + country_names[country_abbr];
  
    var country_link="<a href=\"search.html?pgt=results&cnty="+ecountry_abbr+"&ste="+estate_abbr+"\">"+country_name+"</a>";
  
    var city_link = "<a href=\"search.html?pgt=results&cnty="+ecountry_abbr+"&ste="+estate_abbr+"&cty="+ecity_name+"\">"+city_name+"</a>";

    if((category != null && category.length) || (country_abbr != null && country_abbr.length))
    {
       breadcrumb += arrow_img + locations_link;

       if(category != null && category.length) { 
         if (country_name || state_name)
              if (getArgValue("lid"))
                breadcrumb += arrow_img + category_link;
              else 
                if (refPage != "cat_list" || page == "results" && bcct > 1)
                  breadcrumb += arrow_img + country_link + arrow_img + category;
                else 
                  breadcrumb += arrow_img + country_link + arrow_img + category_link;
         else 
           breadcrumb +=  arrow_img + 'Hot Spots' + ' (' + category + ')';      
      }
    else {
    if(country_abbr != null && country_abbr.length && state_abbr && city_name)  
              breadcrumb += arrow_img + country_link;
    else
       if (refPage != null && refPage != "cat_list")
             breadcrumb += arrow_img  + country_name;
    }

       //if(category != null && refPage != null && refPage == "cat_list" && country_abbr != null) 
         //    breadcrumb += arrow_img + country_name;
    
       if(city_name != null && city_name.length)
       {
            if(location_name)
               breadcrumb += arrow_img + city_link;
            else 
               breadcrumb += arrow_img + city_name;
         
            if(do_metro == 1 && country_abbr == "US")
                breadcrumb += " Metro";
             }
 
            if(location_name)
               breadcrumb += arrow_img + location_name;
       }
    else if (page != null && page == "cat_list" && cte != null) {
            breadcrumb += arrow_img + locations_link;
            breadcrumb += arrow_img + 'Hot Spots' + ' (' + cte + ')';    
    }
    else if (page != null && page == "category") {
            breadcrumb += arrow_img + locations_link + arrow_img + 'Hot Spot Categories';
    }
    else if (page == "results" && !country_abbr && !category && !state_abbr && !city_name) {
        breadcrumb += arrow_img + locations_link + arrow_img + 'Search Results';    
    }
    else {
       if (citystr || stestr) {
           breadcrumb += arrow_img + locations_link + arrow_img  + 'Select Location';
       }
      else {
          breadcrumb += arrow_img + locations_nonlink;
      }
   }
    document.write(breadcrumb);
}

print_breadcrumb("Seattle Downtown SpringHill Suites", "", "Seattle", "WA", "US", "Hotel", "Seattle", "WA", "US", "Hotel");
</script>

