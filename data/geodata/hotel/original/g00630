<script language="Javascript">

function OnclickFood()
{

var submitted = false;

if (window.document.frmheader.txtHeaderSearch.value=="" || window.document.frmheader.txtHeaderSearch.value==null)
		{
			//NewWindow=window.open('RestaurantSearchError.htm','mywindow','width=500,height=125,toolbar=no,menubar=no,top=150,left=250,resizable=false'); 
			alert("Please enter the name of a restaurant to search for.");
			return false;
		}
		
if(submitted == true) { return; }

//window.document.frmheader.submit();

window.document.frmheader.submitgo.disabled = true;

submitted = true;
}
</script>
