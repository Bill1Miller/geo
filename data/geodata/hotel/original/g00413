	function show(nr)
	{
		if (document.getElementById)
		{
			current = (document.getElementById(nr).style.display == 'block') ? 'none' : 'block';
			document.getElementById(nr).style.display = current;
		}
		else if (document.all)
		{
			current = (document.all[nr].style.display == 'block') ? 'none' : 'block'
			document.all[nr].style.display = current;
		}
	}
	</script>
	<table border="0" cellpadding="0" cellspacing="0" class="containerSize">	
	<tr>
		<td align="center">
		<div id="book_1" style="display:none;">
