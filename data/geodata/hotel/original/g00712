<script>
	function selectsearch()
	{
		if (document.search1.dsppsearch[0].checked) {
			document.search1.submit();
		}
		else {
			document.search1.action='/dspp/comn/comn_DirecttoHPSearch_IDX/1,4622,,00.html';
			document.search1.submit();
		}	
	}//end of the function selectsearch
</script>







<SCRIPT Language="JavaScript" title="Java Script to set page URL and delete cookie">
function setPageUrl( givenUrl )
{
    document.cookie = 'DSPPPageURLCookie=' + document.URL + ';path=/';
    document.location = givenUrl;
}

function deleteDSPPCookie(pageURL)
{
    document.signoutform.formPageURL.value=pageURL;
    document.signoutform.submit();
    // document.location = pageURL;
}

function deleteDSPPCookie1(pageURL)
{
	document.signoutform.edit.value=1;
    document.signoutform.formPageURL.value=pageURL;
    document.signoutform.submit();
    // document.location = pageURL;
}
</SCRIPT>



