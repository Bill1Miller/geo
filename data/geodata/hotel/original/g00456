<script language=javascript>
function validate_me()  {
	//document.write("<img src='' width=20 height=20>");
	var err_msg;
	err_msg = '';
	var date_first;
	var date_second;
	var nights;
	if (document.form1.checkInMonthYr.value == '') {err_msg = 'Month/Year is missing.\r\n';};
	if (document.form1.day.value == '') {err_msg += 'Day is missing.\r\n';};
	if (document.form1.nightstay.value == '') {err_msg += 'Nights is missing.\r\n';};	
	if (err_msg == '') {
		//hmw('teaser.aspx','C00152','H00062')
		//document.write("");
		nights = document.form1.nightstay.value;
		date_first = replaceChars(document.form1.checkInMonthYr.value, document.form1.day.value);
		date_second = DateAdd(date_first, 'D',nights);
		document.form1.action = "http://www.reservationpage.com/C00152/H00062/pickdateteaser.aspx";
		document.form1.method = "post";
		document.form1.date1.value = date_first;
		document.form1.date2.value = date_second;
		document.form1.mychainid.value = "C00152";
		document.form1.myhotelid.value = "H00062";
		document.form1.guidx.value = gd;
		document.form1.submit();
		//alert(document.form1.guidx.value);
	}
	else {
		alert(err_msg);
	}	
}
function replaceChars(entry, day) {
	out = " "; // replace this
	add = "/"+day+"/"; // with this
	temp = "" + entry; // temporary holder
	while (temp.indexOf(out)>-1) {
		pos= temp.indexOf(out);
		temp = "" + (temp.substring(0, pos) + add + temp.substring((pos + out.length), temp.length));
	}
	temp = temp.replace(" ","");    //remove the space.
	return temp;
}
function DateAdd(datDate, strInterval, lngOffset)
{
  var datMilliseconds = Date.parse(datDate);
  if (isNaN(datMilliseconds))
     {
     //alert('Invalid Date Format');
     return (datDate);
     }
  if (isNaN(lngOffset))
     {
     //alert('Invalid Offset');
     return (datDate);
     }
  strInterval = strInterval.charAt(0).toUpperCase();
  switch (strInterval)       {
       case 'D':
	    lngOffset *= 24;	  // days to hours
       case 'H':
	    lngOffset *= 60;	  // hours to minutes
       case 'M':
	    lngOffset *= 60;	  // minutes to seconds
       case 'S':
	    lngOffset *= 1000;	  // seconds to milliseconds
	    break;
       default:
	    //alert('Invalid Interval');
	    return (datDate);
  }
  var newdate;
  newdate = new Date(datMilliseconds + lngOffset);     
  var y=newdate.getYear()+"";
  var M=newdate.getMonth()+1;
  var d=newdate.getDate();
  var finaldate;
  finaldate = M+'/'+d+'/'+y;
  return finaldate;
}
</script>
