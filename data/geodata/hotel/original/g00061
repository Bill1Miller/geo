<script language="JavaScript" type="text/javascript">
function formvalidation(thisform)
{
	with (thisform)
	{
		if(document.all.groupVar.value=="true")
		{
			thisform.action="advSearch.do"+"?group=true";
		}else if(document.all.meetingVar.value=="true")
		{
			thisform.action="advSearch.do"+"?meetings=true";
		}else{
			thisform.action="advSearch.do";
		}
		thisform.submit();
	}
}

function clearcode(thisform) {
    thisform.code.value="";
}
</script>





