<script  type="text/javascript">
// When a user clicks on a location
// go to that anchor on the page
function goToLocation(locationValue) {
	if(locationValue!=null) {
 		window.location.hash =locationValue;
	}
}
function setupDisplay(number)
{
	if (!(number > 1))
	{
		number = 1;
	}
	for (i = 1 ; i <= number ; i++)
	{
		setStyle('room' + i + 'a', 'block');
		setStyle('room' + i + 'b', 'block');
		setStyle('room' + i + 'c', 'block');
		setStyle('room' + i + 'd', 'block');
	}
	for (i = 5 ; i > number ; i--)
	{
		setStyle('room' + i + 'a', 'none');
		setStyle('room' + i + 'b', 'none');
		setStyle('room' + i + 'c', 'none');
		setStyle('room' + i + 'd', 'none');
	}
}
function setPromo()
{
		 rx = /setpromo\=([A-Za-z0-9]+)/;
		 matches = rx.exec(location.search);
		 if (matches && matches[1])
			{
	        	document.forms.addBillingForm.iataNumber.value = matches[1];
			}
}

function secureUrl()
{
	window.name="parentWindow";
}

function reviewReservePopup(urlname, target)
{
    window.open(urlname,target,"toolbar=no,location=no,status=yes,menubar=no,scrollbars=yes,resizable=yes,width=800,height=600");
}

function reviewReserve(url)
{
	document.location.href=url;
}

function returnFromPopup(path)
{
    window.opener.location.href=path;
    window.close();
}
</script>

<!-- End: $Workfile: bodyStart.jsp$ -->
