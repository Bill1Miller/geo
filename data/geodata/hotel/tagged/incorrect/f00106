	<script language="javascript">
	
		var showPopup = true;
		var neverShown = true;
		var interceptedLocation = '';
		var TIMEOUT_INTERVAL = 90000;
		var INTERCEPT_WIDTH = 600;
		var INTERCEPT_HEIGHT = 370;
		
		
					window.onload = doOnLoad();
			//		window.onunload = doOnUnload();
					window.onresize = doOnResize()
		
		
		function goToHelp(relativeUrl, title, options) {
				
				
				var url = "http://www.oakwood.com";
				
				oak_setDisplay("interceptDiv", false);
				oak_setDisplay("interceptFrame", false);
				url = url + "/" + relativeUrl;
				//var child = window.open(url, title, options);
				var child = window.open(url);
				child.focus();
			}
			
		function doOnLoad() {
			
			
		}
		
		function doOnUnload(){
		
			
		}	
		
		function doOnResize() {
				
		}
		
		function centerDiv(divid){
			//get center left and top on the visible screen.
			var centerLeft = oak_getScrollLeft() + (oak_getClientWidth() / 2);	
			var centerTop = oak_getScrollTop() + (oak_getClientHeight() / 2);	
			
			if((centerTop - (oak_getHeight(divid) / 2)) < oak_getScrollTop())
				oak_setTop(divid, oak_getScrollTop());	//dont center div and display at top if it cant fit in the window
			else
				oak_setTop(divid, centerTop - (oak_getHeight(divid) / 2));
			
			if((centerLeft - (oak_getWidth(divid) / 2)) < oak_getScrollLeft())
				oak_setLeft(divid, oak_getScrollLeft() + 5); //add 5, otherwise part of it gets hidden
			else
				oak_setLeft(divid, centerLeft - (oak_getWidth(divid) / 2));
			
			setTimeout("centerDiv('" + divid + "')", 3000);
		}
		
		function intercept(href){	
								
		}		
		
		function doNotIntercept(href) {
			showPopup = false;
			window.location = href;
		}
		
		function showIntercept() {
				
		}
		
		function deactivateIntercept(){

			oak_setDisplay("interceptDiv", false);
			oak_setDisplay("interceptFrame", false);
	
			oak_SetCookie("interceptCookie", "false", null, null, null, false);
			
			
			if(interceptedLocation != ''){
				window.location = interceptedLocation;
			} else {
				//setTimeout('oak_setDisplay("interceptDiv", true)', 5000)
			}
		}
		
		function closeIntercept(){
			oak_setDisplay("interceptDiv", false);
			oak_setDisplay("interceptFrame", false);
			if(interceptedLocation != ''){
				window.location = interceptedLocation;
			} else {
				//setTimeout('oak_setDisplay("interceptDiv", true)', 5000)
			}
		}
		
		function noThankYou(){
			var form = document.forms.checkSessionForm;
			
			oak_setDisplay("interceptDiv", false);
			oak_setDisplay("interceptFrame", false);	
			
			oak_SetCookie("interceptCookie", "false", null, null, null, false);
			
			if(interceptedLocation != ''){
				window.location = interceptedLocation;
			} else {
				//setTimeout('oak_setDisplay("interceptDiv", true)', 5000)
			}		
		}		
		
		function resetIntercept(){
			showPopup = false;
		}
		
	</script>	



