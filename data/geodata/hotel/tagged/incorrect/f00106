<script language="javascript">

	function setCityAndZip()
	{
		setCity();
		setZip();
	}
	
	function resetCityAndZip()
	{
		resetCity();
		resetZip();
	}
	
	function setCity()
	{
		var form = document.forms.propertySearchForm;
		
			if (form.city.value == 'City')
			{
				form.city.value = "";
			}	
		
	}
	
	function resetCity()
	{
		var form = document.forms.propertySearchForm;
		
			if (form.city.value == "")
			{
				form.city.value = 'City';
			}	
		
	}
	
	function setZip()
	{	
		var form = document.forms.propertySearchForm;
		
			if (form.zipcode.value == 'Zip/Postal Code')
			{
				form.zipcode.value = "";
			}
			form.zipcode.maxLength = "5";		
		
	}
	
	function resetZip()
	{
		var form = document.forms.propertySearchForm;
		
			if (form.zipcode.value == "")
			{	
				
				form.zipcode.maxLength = "20";
				form.zipcode.value = 'Zip/Postal Code';
			}		
		
	}

	function processEvent(eventName)
	{	
		setCityAndZip();
		var form = document.forms.propertySearchForm;
		form.event.value=eventName;
		form.currentPage.value=1;
		
		if (validate())
		{
			oak_setDisplay('searchDisplay', true);
			form.submit();
		}
		else
		{
			resetCityAndZip();
		}
	}
		
	function selectLocation(id)
	{
		setCityAndZip();
		
		var form = document.forms.propertySearchForm;
		form.event.value="chooseLocation";
		form.chosenLocationId.value=id
		oak_setDisplay('searchDisplay', true);
		form.submit();
	}	
	
	function validate()
	{
		var form = document.forms.propertySearchForm;
		var msg = "";
				
		if (form.event.value != null && form.event.value == "search")
        {
            
	            if ( form.nationAbbr.value == "US" 
	            	&& ((form.city.value == null || "" == form.city.value)
	                    && (form.zipcode.value == null || "" == form.zipcode.value)) )
	            {
	            	msg = msg + "Please enter a city";
	            }
	            
            
            
            

            msg = validateDates(msg, form);
        }
        
        if (msg != "")
        {
        	alert(msg);
        	return false;
        }
        else
        {
        	return true;
        }
	}
	
	function showFields()
	{
		oak_toggleDisplay('altLocations');
		oak_toggleDisplay('searchFields');
		oak_toggleDisplay('searchButton');
	}
	
	function resetAptTypes(showAll)
	{	
		var bedrooms = document.forms.propertySearchForm.bedrooms;
		var selected = bedrooms.value;
		
		if (!showAll && selected == -1)
			selected = 1;
		
		var aptTypeCount = 6;	
		var offset = 0.0;
		var isSelected = false;

		if (showAll)
		{
			bedrooms.length = aptTypeCount + 1;
			
			isSelected = false;			
			if (selected == "-1")
				isSelected = true;
				
			bedrooms.options[0] = 
				new Option('Any Size', "-1", false, isSelected);	
				
			offset = 1;	
		}	
		else
		{
			bedrooms.length = aptTypeCount;
		}		
						
		
				isSelected = false;
				if (selected == "0")
					isSelected = true;
			
				bedrooms.options[0 + offset] = 
					new Option("Studio", "0", false, isSelected);
	 	 	
				isSelected = false;
				if (selected == "1")
					isSelected = true;
			
				bedrooms.options[1 + offset] = 
					new Option("1 Bedroom", "1", false, isSelected);
	 	 	
				isSelected = false;
				if (selected == "2")
					isSelected = true;
			
				bedrooms.options[2 + offset] = 
					new Option("2 Bedroom", "2", false, isSelected);
	 	 	
				isSelected = false;
				if (selected == "3")
					isSelected = true;
			
				bedrooms.options[3 + offset] = 
					new Option("3 Bedroom", "3", false, isSelected);
	 	 	
				isSelected = false;
				if (selected == "4")
					isSelected = true;
			
				bedrooms.options[4 + offset] = 
					new Option("4 Bedroom", "4", false, isSelected);
	 	 	
				isSelected = false;
				if (selected == "5")
					isSelected = true;
			
				bedrooms.options[5 + offset] = 
					new Option("5 Bedrooms", "5", false, isSelected);
	 	 		  
	}
	
	function checkEnter(event)
	{  	   
	   NS4 = (document.layers) ? true : false;
   
	   var code = 0;
	   if (NS4)
	     code = event.which;
	   else
	     code = event.keyCode;
	   
	   if (code==13)
	   {
		 processEvent('search');	
	     return false;
	   }	   
	}
</script>
