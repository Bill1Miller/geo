    function fixwidth() {
        var ads_content = document.getElementById("ads_content");
        if(!ads_content) return;
        var tw = ads_content.offsetWidth;
        var th = ads_content.offsetHeight;
        var rightframe = parent.document.getElementById("rightframe");
        if(!rightframe) return;
        rightframe.width = tw + "px";
        rightframe.height = th + "px";
    }
