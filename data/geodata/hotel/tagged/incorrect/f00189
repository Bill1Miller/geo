<script>
function updateDateInput(oValue,oInput) {
if (oInput=='checkinDate') checkDateRange(oValue,document.HotelSearchAddressForm,document.HotelSearchAddressForm.checkinDate);
if (oInput=='checkoutDate') checkDateRange(oValue,document.HotelSearchAddressForm,document.HotelSearchAddressForm.checkoutDate);
}

function openCalendar(oInput,y,x) {
if (oInput=='checkinDate') popCalendar(document.HotelSearchAddressForm,document.HotelSearchAddressForm.checkinDate,'',y,x);
if (oInput=='checkoutDate') popCalendar(document.HotelSearchAddressForm,document.HotelSearchAddressForm.checkoutDate,'',y,x);
}

function checkEnter(e) {
	if (e.keyCode == 13) {
        runSearch();
		document.HotelSearchAddressForm.submit();
	}
}
</script>
