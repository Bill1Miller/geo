function SearchCtrl_calArrivalDate_Up_SetClick(addClickTo)
{
	if(addClickTo != '') document.getElementById(addClickTo).onclick = SearchCtrl_calArrivalDate_Up_CallClick;
	document.onmousedown = CalendarPopup_Up_LostFocus;
	document.getElementById('SearchCtrl_calArrivalDate').onclick = SearchCtrl_calArrivalDate_Up_CallClick;
}

function SearchCtrl_calArrivalDate_Up_CallClick(e)
{
	var monthnames = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
	var daynames = new Array('M','T','W','T','F','S','S');
