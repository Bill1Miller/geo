<!--

			function fOnRange(rng)
			{ // call back from within the calendar engine.
				var sd=new Date(rng[0]), ed=new Date(rng[1]);
				if(gfPop.gdCtrl.name=="hs_arrival"||gfPop.gdCtrl.name=="hs_departure")
				{
					var which=sd.getTime()==ed.getTime()?gfPop.gdCtrl.name=="hs_arrival"?1:2:1+2;
					if(which&1)document.search_start.hs_arrival.value=gfPop.fFormatInput(sd.getUTCFullYear(),sd.getUTCMonth()+1,sd.getUTCDate());
					if (document.search_start.hs_departure.value=='') {document.search_start.hs_departure.value = AddDay(document.search_start.hs_arrival);}
					if(which&2)document.search_start.hs_departure.value=gfPop.fFormatInput(ed.getUTCFullYear(),ed.getUTCMonth()+1,ed.getUTCDate());
				}

			}

			function AddDay(currentdate)
			{
				if (currentdate.value != '')
				{
					var datum = currentdate.value.split('.');
					switch(datum[1]) {
						case "2":
						case "02":
							if (datum[0] == "28" || datum[0] == "29")
							{	datum[0] = "01";
								if (datum[1] == "12")
								{	datum[1] = "01";
									datum[2] = parseInt(datum[2]) + 1;
								}
								else
								{	datum[1] = parseInt(datum[1]) + 1; }
							}
							else
							{	datum[0] = parseInt(datum[0]) + 1;}
							break;
						case "1":
						case "01":
						case "3":
						case "03":
						case "5":
						case "05":
						case "7":
						case "07":
						case "8":
						case "08":
						case "10":
						case "12":
							if (datum[0] == "31")
							{	datum[0] = "01";
								if (datum[1] == "12")
								{	datum[1] = "01";
									datum[2] = parseInt(datum[2]) + 1;
								}
								else
								{	if (datum[1] == "08") datum[1] = "8";
									datum[1] = parseInt(datum[1]) + 1; }
							}
							else
							{	datum[0] = parseInt(datum[0]) + 1; }
							break;
						case "4":
						case "04":
						case "6":
						case "06":
						case "9":
						case "09":
						case "11":
							if (datum[0] == "30")
							{	datum[0] = "01";
								if (datum[1] == "12")
								{	datum[1] = "01";
									datum[2] = parseInt(datum[2]) + 1;
								}
								else
								{	if (datum[1] == "09") datum[1] = "9";
									datum[1] = parseInt(datum[1]) + 1; }
							}
							else
							{	datum[0] = parseInt(datum[0]) + 1; }
							break;
					}
					return datum[0] + "." + datum[1] + "." + datum[2];
				}
				else
				{
					return '';
				}

			}

			// The following script is used to hide the calendar whenever you click the document.
			document.onmousedown = function(e)
			{
				  if (!self.gfPop) return;
				  var n=!e?self.event.srcElement.name:e.target.name;
				  if (document.layers)
				  {
				    with (gfPop) var l=pageX, t=pageY, r=l+clip.width, b=t+clip.height;
				    if (n!="popcal"&&(e.pageX>r||e.pageX<l||e.pageY>b||e.pageY<t)) gfPop.fHideCal();
				    return routeEvent(e); // must use return here.
				  }
				  else if (n!="popcal") gfPop.fHideCal();
			}

			if (document.layers) document.captureEvents(Event.MOUSEDOWN);
			//-->
				</script>
