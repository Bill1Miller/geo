SearchCtrl_calDepartureDate_outer_EnableHideDropDownFlag = false;
SearchCtrl_calDepartureDate_outer_VisibleDate = '07/28/2006';

function SearchCtrl_calDepartureDate_Up_SetClick(addClickTo)
{
	if(addClickTo != '') document.getElementById(addClickTo).onclick = SearchCtrl_calDepartureDate_Up_CallClick;
	document.onmousedown = CalendarPopup_Up_LostFocus;
	document.getElementById('SearchCtrl_calDepartureDate').onclick = SearchCtrl_calDepartureDate_Up_CallClick;
}

function SearchCtrl_calDepartureDate_Up_CallClick(e)
{
	var monthnames = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
	var daynames = new Array('M','T','W','T','F','S','S');
	CalendarPopup_Up_DisplayCalendar("SearchCtrl_calDepartureDate_outer_EnableHideDropDownFlag", "SearchCtrl_calDepartureDate","","","SearchCtrl_calDepartureDate_div", "SearchCtrl_calDepartureDate_monthYear", "SearchCtrl_calDepartureDate_Up_PreDisplayCalendar", "SearchCtrl_calDepartureDate_Up_PreMonthYear", "class='dateweekday'","class='dateweekend'","class='dateoffmonth'","class='dateselecteddate'","class='datemonthheader'","class='datedayheader'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "class='datetoday'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", 2, monthnames, daynames, 1, 6, 5, false, false, '07/29/2006', '12/31/9999','calDepartureDate_image',3, true, 'SearchCtrl_calDepartureDate_Up_PostBack', -165, 0, false, 'Clear Date', false, 'Today\'s Date:', '', '', -1, "SearchCtrl_calDepartureDate_outer_VisibleDate", "Select a Date", CalendarPopup_Array_SearchCtrl_calDepartureDate_outer, '', '', '', '');
}

