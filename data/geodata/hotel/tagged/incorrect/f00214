      <TD ALIGN="right" >
        <span id="SearchCtrl_calArrivalDate_outer" class="formstyle" NAME="calArrivalDate"><input name="SearchCtrl:calArrivalDate" type="text" value="29/07/2006" readonly="readonly" id="SearchCtrl_calArrivalDate" class="formstyle" style="width:75px;" /><a href="javascript:SearchCtrl_calArrivalDate_Up_CallClick();"><img src="/graphics/calendar.gif" border="0" /></a></span><script language="javascript">
SearchCtrl_calArrivalDate_outer_EnableHideDropDownFlag = false;
SearchCtrl_calArrivalDate_outer_VisibleDate = '07/28/2006';

function SearchCtrl_calArrivalDate_Up_SetClick(addClickTo)
{
	if(addClickTo != '') document.getElementById(addClickTo).onclick = SearchCtrl_calArrivalDate_Up_CallClick;
	document.onmousedown = CalendarPopup_Up_LostFocus;
	document.getElementById('SearchCtrl_calArrivalDate').onclick = SearchCtrl_calArrivalDate_Up_CallClick;
}

function SearchCtrl_calArrivalDate_Up_CallClick(e)
{
	var monthnames = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
	var daynames = new Array('M','T','W','T','F','S','S');
	CalendarPopup_Up_DisplayCalendar("SearchCtrl_calArrivalDate_outer_EnableHideDropDownFlag", "SearchCtrl_calArrivalDate","","","SearchCtrl_calArrivalDate_div", "SearchCtrl_calArrivalDate_monthYear", "SearchCtrl_calArrivalDate_Up_PreDisplayCalendar", "SearchCtrl_calArrivalDate_Up_PreMonthYear", "class='dateweekday'","class='dateweekend'","class='dateoffmonth'","class='dateselecteddate'","class='datemonthheader'","class='datedayheader'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "class='datetoday'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", 2, monthnames, daynames, 1, 6, 5, false, false, '07/28/2006', '12/31/9999','calArrivalDate_image',3, true, 'SearchCtrl_calArrivalDate_Up_PostBack', -165, 0, false, 'Clear Date', false, 'Today\'s Date:', '', 'DateChange', -1, "SearchCtrl_calArrivalDate_outer_VisibleDate", "Select a Date", CalendarPopup_Array_SearchCtrl_calArrivalDate_outer, '', '', '', '');
}

function SearchCtrl_calArrivalDate_Up_PreDisplayCalendar(theDate)
{
	var monthnames = new Array('January','February','March','April','May','June','July','August','September','October','November','December');
	var daynames = new Array('M','T','W','T','F','S','S');
	CalendarPopup_Up_DisplayCalendarByDate("SearchCtrl_calArrivalDate","","SearchCtrl_calArrivalDate_div", "SearchCtrl_calArrivalDate_monthYear", "SearchCtrl_calArrivalDate_Up_PreDisplayCalendar", "SearchCtrl_calArrivalDate_Up_PreMonthYear", theDate, "class='dateweekday'","class='dateweekend'","class='dateoffmonth'","class='dateselecteddate'","class='datemonthheader'","class='datedayheader'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "class='datetoday'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", "style='color:Black;background-color:White;font-family:Verdana,Helvetica,Tahoma,Arial;font-size:XX-Small;'", 2, monthnames, daynames, 1, 6, 5, false, false, '07/28/2006', '12/31/9999', true, 'SearchCtrl_calArrivalDate_Up_PostBack', false, 'Clear Date', false, 'Today\'s Date:', '', 'DateChange', -1, "SearchCtrl_calArrivalDate_outer_VisibleDate", "Select a Date", CalendarPopup_Array_SearchCtrl_calArrivalDate_outer, '', '', '', '');
}

SearchCtrl_calArrivalDate_Up_SetClick('');

function SearchCtrl_calArrivalDate_Up_PreMonthYear(theDate)
{
	var monthnames = new Array('Jan','Feb','Mar','Apr','May','Jun','Jul','Aug','Sep','Oct','Nov','Dec');
	CalendarPopup_Up_DisplayMonthYear("SearchCtrl_calArrivalDate_div", "SearchCtrl_calArrivalDate_monthYear", "SearchCtrl_calArrivalDate_Up_PreDisplayCalendar", "SearchCtrl_calArrivalDate_Up_PreMonthYear", monthnames, theDate, "Apply", "Cancel", "07/28/2006", "12/31/9999");
}

