 '_CHECKBOX'){if ( elementName.length ){for (i=0; i < elementName.length; i++){if (elementName[i].checked)return true;}return false;}else{return elementName.checked;}}}function tf_validate_date( elementName ){var theDate = elementName.value;if (theDate.length == 0)return true;separator = theDate.indexOf('/');if (separator == -1 || separator == theDate.length) return false;month = theDate.substring(0, separator);separator = theDate.indexOf('/', separator + 1);if (separator == -1 || (separator + 1 ) == theDate.length) return false;day = theDate.substring((month.length + 1), separator);year = theDate.substring(separator + 1);if (!tf_integer(month))return false;else if (!tf_checkrange(month, 1, 12))return false;else if (!tf_integer(year))return false;else if (!tf_checkrange(year, 0, 9999))return false;else if (!tf_integer(day))return false;else if (!tf_checkday(year, month, day))return false;elsereturn true;}function tf_checkday(year, month, day){var maximum = 31;if (month == 4 || month == 6 || month == 9 || month == 11)maximum = 30;else if (month == 2){if (year % 4 > 0)maximum =28;else if (year % 100 == 0 && year % 400 > 0)maximum = 28;elsemaximum = 29;}return tf_checkrange(day, 1, maximum);}function check_TF_Main_Search( theForm ){ if ( !tf_element_has_value( theForm.CheckInDate, "_TEXT") ){if (!tf_on_error( theForm, theForm.CheckInDate, theForm.CheckInDate.value, "Check-In Date Required And Must Be A Valid Date In MM/DD/YYYY Format" ) ){return false;}}if( !tf_validate_date( theForm.CheckInDate ) ) { alert("Check-In Date Required And Must Be A Valid Date In MM/DD/YYYY Format"); return false; }if ( !tf_validate_date( theForm.CheckInDate ) ){if (!tf_on_error( theForm, theForm.CheckInDate, theForm.CheckInDate.value, "Check-In Date Required And Must Be A Valid Date In MM/DD/YYYY Format" ) ){return false;}}if ( !tf_element_has_value( theForm.CheckOutDate, "_TEXT") ){if (!tf_on_error( theForm, theForm.CheckOutDate, theForm.CheckOutDate.value, "Check-Out Date Required And Must Be A Valid Date In MM/DD/YYYY Format" ) ){return false;}}if( !tf_validate_date( theForm.CheckOutDate ) ) { alert("Check-Out Date Required And Must Be A Valid Date In MM/DD/YYYY Format"); return false; }if ( !tf_validate_date( theForm.CheckOutDate ) ){if (!tf_on_error( theForm, theForm.CheckOutDate, theForm.CheckOutDate.value, "Check-Out Date Required And Must Be A Valid Date In MM/DD/YYYY Format" ) ){return false;}}return true;}//--></SCRIPT><SCRIPT LANGUAGE="JAVASCRIPT"><!--function check_TF_Show_Images( theForm ){ return true;}//--></SCRIPT></head>
