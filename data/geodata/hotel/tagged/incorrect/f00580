        <td align="left">
<script language="javascript">
    tagDefault = 'Separate each tag with a comma.';
    shoutDefault = 'To protect your voice, we limit shouts to a maximum of 120 characters!';
    
	// check if tagtext contains only alphabets, numbers and commas
	function validate()
	{
		var objregexp = /^[\sa-zA-Z0-9,]*$/
		
		var tagtext = trim(document.getElementById("tagTextTd").getElementsByTagName("input")[0].value);
		
		if (!tagtext.match(objregexp) || tagtext == tagDefault)
		{
			alert("Please tag with simple keyword(s) contain only alphabets and numerics!");
			return false;
		}
		return true;
	}
	
	// check if shouttext is empty
	function validateShout()
	{
		var shouttext = trim(document.getElementById("shoutTextTd").getElementsByTagName("textarea")[0].value);
		
		return (shouttext != "" && shouttext != shoutDefault);
	}
	
	// limit textarea to the maximum length defined
	function limitLength(txtArea, max)
	{
		if (txtArea.value.length > max)
		{
		    txtArea.value = txtArea.value.substring(0, max);
		}
	}
	
	function favValidate() 
	{
	    var select = document.getElementById("listSelectTd").getElementsByTagName("select")[0];
	    var textbox = document.getElementById("listNameTd").getElementsByTagName("input")[0]
		if (select.value == -1 && textbox.value == "") {
			alert("Please enter the name of the new list.");
			textbox.focus();
			return false;
		}
		return true;
	}
	
	function showTagForm(isShow)
	{
		if (isShow)
		{
			document.getElementById("yourTag").style.display = "none";
			document.getElementById("tagForm").style.display = "";
			document.getElementById("listForm").style.display = "none";
			document.getElementById("tagtext").value = tagDefault;
		}
		else
		{
			document.getElementById("yourTag").style.display = "";
			document.getElementById("tagForm").style.display = "none";
			document.getElementById("listForm").style.display = "none";
			showWhatsTag(false);
		}
		return;
	}
	
	function showShoutForm(isShow)
	{
		if (isShow)
		{
			document.getElementById("yourTag").style.display = "none";
			document.getElementById("shoutForm").style.display = "";
			document.getElementById("shoutForm").value = shoutDefault;
			document.getElementById("listForm").style.display = "none";
		}
		else
		{
			document.getElementById("yourTag").style.display = "";
			document.getElementById("shoutForm").style.display = "none";
			document.getElementById("listForm").style.display = "none";
			showWhatsShout(false);
		}
		return;
	}
	
	function showListForm(isShow)
	{
		if (isShow)
		{
			document.getElementById("yourTag").style.display = "none";
			document.getElementById("shoutForm").style.display = "none";
			document.getElementById("listForm").style.display = "";
		}
		else
		{
			document.getElementById("yourTag").style.display = "";
			document.getElementById("shoutForm").style.display = "none";
			document.getElementById("listForm").style.display = "none";
		}
		return;
	}
	
	function showWhatsShout(isShow)
	{
		if (isShow)
		{
			document.getElementById('whatsShout').style.display="";
			document.getElementById('whatsShoutClose').style.display="";
			document.getElementById('whatsShoutLink').style.display="none";
		}
		else
		{
			document.getElementById('whatsShout').style.display="none";
			document.getElementById('whatsShoutClose').style.display="none";
			document.getElementById('whatsShoutLink').style.display="";
		}
	}
	
	function showWhatsTag(isShow)
	{
		if (isShow)
		{
			document.getElementById('whatsTag').style.display="";
			document.getElementById('whatsTagClose').style.display="";
			document.getElementById('whatsTagLink').style.display="none";
		}
		else
		{
			document.getElementById('whatsTag').style.display="none";
			document.getElementById('whatsTagClose').style.display="none";
			document.getElementById('whatsTagLink').style.display="";
		}
	}
	
	function showLogInText(isShow)
	{
		if (isShow)
		{
			document.getElementById('logInText').style.display="";
		}
		else
		{
			document.getElementById('logInText').style.display="none";
		}
	}
</script>

