			                function GoRecord(select)
			                {
			                    var baseUrl = "/restaurants/";
			                    var pageName = '/editorial.aspx';
			                    var val = select.options[select.selectedIndex].value;
			                    
			                    if ( val == "" )
			                        return "";
			                    
			                    if (false)
			                        baseUrl = "/movies/title/";
			                    else if (false)
			                        baseUrl = "/movies/theater/";
			                            
			                    window.location = baseUrl + alphanumeric(select.options[select.selectedIndex].text) 
			                                + "." + val + pageName;
			                }
			                
