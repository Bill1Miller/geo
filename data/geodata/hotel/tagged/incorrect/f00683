<script type="text/javascript"><!--

	var Rightnav1_DatePicker1_closeOnPick = true;
	var Rightnav1_DatePicker1_opusCalVisible = false;

	function Rightnav1_DatePicker1_toggleCal() {

		var opusCal = document.getElementById('Rightnav1_DatePicker1_CalendarIFrameDiv');

		if (Rightnav1_DatePicker1_opusCalVisible) {
			//opusCal.style.visibility = 'hidden';
			opusCal.style.display = 'none';
			Rightnav1_DatePicker1_opusCalVisible = false;
		}

		else {
			//opusCal.style.visibility = 'visible';
			opusCal.style.display = 'block';
			Rightnav1_DatePicker1_opusCalVisible = true;
		}

	}

	function Rightnav1_DatePicker1_updateCal() {
		var calFrame = document.getElementById('Rightnav1_DatePicker1_CalendarIFrame');
		var ddlDayPicked = document.getElementById('Rightnav1_DatePicker1_ddlDayPicked');
		var dayPicked = ddlDayPicked.selectedIndex;
		var ddlYearMonthPicked = document.getElementById('Rightnav1_DatePicker1_ddlYearMonthPicked');
		var strYearMonthPicked = ddlYearMonthPicked.selectedIndex;
		var MonthPicked = ddlYearMonthPicked.options[strYearMonthPicked].value.substring(4,6);
		var yearPicked = ddlYearMonthPicked.options[strYearMonthPicked].value.substring(0,4);
		var datePicked = MonthPicked + '/' + ddlDayPicked.options[dayPicked].value + '/' + yearPicked;
		calFrame.setAttribute('src', 'OpusDatePickerCalendar.aspx?date=' + datePicked + '&cname=Rightnav1_DatePicker1');
	}

	//-->
</script>

