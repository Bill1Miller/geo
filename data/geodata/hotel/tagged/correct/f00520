	<script language="JavaScript">
		var sub_base = "/cms_content/hotels/images/";
		var sub_offSuffix = "_off.gif";
		var sub_overSuffix = "_over.gif";
		var sub_onSuffix = "_on.gif";

		var sub_precacheKeys = new Array("rr_hotel","Go");

		var sub_IMGOFF = "0";
		var sub_IMGOVER = "1";
		var sub_IMGON = "2";

		var sub_precacheImages = new Array();


		if (document.images) {
		for (var i=0; i < sub_precacheKeys.length; i++) {
		    sub_precacheImages[sub_precacheKeys[i]+sub_IMGOFF] = new Image();
		    sub_precacheImages[sub_precacheKeys[i]+sub_IMGOVER] = new Image();
		    sub_precacheImages[sub_precacheKeys[i]+sub_IMGOFF].src = sub_base + sub_precacheKeys[i] + sub_offSuffix;
		    sub_precacheImages[sub_precacheKeys[i]+sub_IMGOVER].src = sub_base + sub_precacheKeys[i] + sub_overSuffix;

		}
		}

		// key,status=0/1
		function setRRHotelImg()
		{
		if (document.images) {
		    for (var i=0; i < setRRHotelImg.arguments.length; i+=2) {
			var key = setRRHotelImg.arguments[i];
			var status = setRRHotelImg.arguments[i+1];
			document.images[key].src = sub_precacheImages[key+status].src;
		    }
		}
		}
	</script>

	<script language="JavaScript">
		function addIsis(form) {
			form.searchWord.value = form.isiscode.value+form.searchWord.value;
			//alert(form.searchWord.value);
			form.submit();
    	}

		// Code  for  key pressed
		function fnPressEnterPart(form)
		{
		if(navigator.appName != "Netscape"){
			var k = window.event.keyCode;
			if (k==13) // ENTER KEY IS PRESSED
			  {
			   if (validateForm(form))
				 {
				 addIsis(form);
			 	 }
			   else
			     {
			     return false;
			     }
			  }
			 }
		}
	</script>

	<script language="JavaScript" type="text/javascript">

		//	@@@@@@@@@@@@@@@ for Netscape
			function PressEnter(anEvent)
			{
				var WhichKey = anEvent.which;
				var eventTarget = anEvent ? anEvent.target : event.srcElement;
				var textField = eventTarget.type == 'text' ;

				if(WhichKey ==13  && textField)
				{
					if (validateForm(document.hotelSearch))
				 	{
				 		addIsis(document.hotelSearch);
				 	}
					else
					{
					 return false;
					}



				}
			}

			if(navigator.appName=="Netscape")
			{
			//var x = new Array(10);
			//alert("Inside Netscape");
			document.captureEvents(Event.KEYPRESS);
			document.onkeypress = PressEnter;


			}

			//End of key pressed
//	@@@@@@@@@@@@@@@@@@@@@@
	</script>

