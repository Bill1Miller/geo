//Begin function for setting default option in dropdown context sensitive
function dropDownDefault() {

  var path = this.window.location.pathname;
  var arrpath=path.split("/");
  var cur=arrpath[1];
//  window.alert("inside selectit and cur is : " + cur);
  switch (cur){

     case "locator": cur="site";break;
     case "wbr": cur="wbr";break;
     case "meetingsandweddings": cur="meetings";break;
     case "destinationsandinterests": cur="destinationsandinterests";break;
     case "wotw": cur = "wotw";break;
     //case "hotels": cur = "thishotel"; break;
     case "corporate": cur = "corporate";break;
     //case "help": cur = "help";  break;
     default: cur="site";
 }


