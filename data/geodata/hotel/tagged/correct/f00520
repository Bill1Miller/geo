<script language="JavaScript" type="text/javascript">

var versionNumber = parseFloat(navigator.appVersion);

// Begin form validation
function validateForm(form) {
	form.searchWord.value=form.hotelSearchWord.value;

if(versionNumber >= 5 || navigator.appName != "Netscape"){

	form.hotelSearchWord.style.backgroundColor = '#FFFFFF';

}

	var output = "";

	if (!validateAlphaNum(form.hotelSearchWord.value,1,30)) {
		output += "* Search word or phrase\n";
	}

	//final validation error message
	if (output != "") {
		alert("The following information was missing or incorrect:\n\n"+output);


if(versionNumber >= 5 || navigator.appName != "Netscape"){


		form.hotelSearchWord.focus();
		form.hotelSearchWord.style.backgroundColor = '#FFFFCC';

}



		return false;

	}
	else {

		return true;

	}
}
//  End form validation


</script>

<script language="JavaScript" TYPE="text/javascript">


// Begin visual tour
/*
inputsArray is an external input to this page and includes the path to the large image,  off thumbnail and on thumbnail and it includes the caption text (in that order) for the first through the nth image in the visual tour
 */
//var inputsArray = new Array ("images/h_image_1_1_lrg.gif","images/h_image_1_1_off.gif","images/h_image_1_1_on.gif","This is the 1st caption in set 1.","images/h_image_2_1_lrg.gif","images/h_image_2_1_off.gif","images/h_image_2_1_on.gif","This is the 2nd caption in set 1.","images/h_image_3_1_lrg.gif","images/h_image_3_1_off.gif","images/h_image_3_1_on.gif","This is the 3rd caption in set 1.","images/h_image_4_1_lrg.gif","images/h_image_4_1_off.gif","images/h_image_4_1_on.gif","This is the 4th caption in set 1.","images/h_image_5_1_lrg.gif","images/h_image_5_1_off.gif","images/h_image_5_1_on.gif","This is the 5th caption in set 1.","images/h_image_1_2_lrg.gif","images/h_image_1_2_off.gif","images/h_image_1_2_on.gif","This is the 1st caption in set 2.","images/h_image_2_2_lrg.gif","images/h_image_2_2_off.gif","images/h_image_2_2_on.gif","This is the 2nd caption in set 2.","images/h_image_3_2_lrg.gif","images/h_image_3_2_off.gif","images/h_image_3_2_on.gif","This is the third caption in set 2.","images/h_image_4_2_lrg.gif","images/h_image_4_2_off.gif","images/h_image_4_2_on.gif","This is the 4th caption in set 2.","images/h_image_5_2_lrg.gif","images/h_image_5_2_off.gif","images/h_image_5_2_on.gif","This is the 5th caption in set 2.","images/h_image_1_3_lrg.gif","images/h_image_1_3_off.gif","images/h_image_1_3_on.gif","This is the 1st caption in set 3.","images/h_image_2_3_lrg.gif","images/h_image_2_3_off.gif","images/h_image_2_3_on.gif","This is the 2nd caption in set 3.");

var inputsArray = new Array ( 
"/cms_content/hotels/SYRSH/images/hero.jpg","/cms_content/hotels/SYRSH/images/Dhero.jpg","/cms_content/hotels/SYRSH/images/Shero.jpg","","/cms_content/hotels/SYRSH/images/Mpool.jpg","/cms_content/hotels/SYRSH/images/Dpool.jpg","/cms_content/hotels/SYRSH/images/Spool.jpg","","/cms_content/hotels/SYRSH/images/Mdining.jpg","/cms_content/hotels/SYRSH/images/Ddining.jpg","/cms_content/hotels/SYRSH/images/Sdining.jpg","","/cms_content/hotels/SYRSH/images/Mguestroom.jpg","/cms_content/hotels/SYRSH/images/Dguestroom.jpg","/cms_content/hotels/SYRSH/images/Sguestroom.jpg",""

 );

// end external input
var setsAllowed = 10;
var slidesPerSet = 5;
var currentSet = null;
var numSlides = inputsArray.length/4;

var divThumbnailsPrefix = "thumbnails";

var numSlidesSet = new Array;
    for (i = 0; i < setsAllowed; i++) {
        if (numSlides < slidesPerSet *(i+1))  {
            numSlidesSet[i]=  numSlides - (slidesPerSet *(i));
        }
        else {
            numSlidesSet[i]=  slidesPerSet;
        }
        if (numSlidesSet[i] < 0 ) numSlidesSet[i] = "null"
}


var captionArray = new Array ();

//Preload Visual Tour images
var precacheSets = 1;

var keys = new Array("h_image_1_","h_image_2_","h_image_3_","h_image_4_","h_image_5_");

var IMGOFF = "0";
var IMGON = "1";
var IMGLRG = "2";
var num = 0;
var captionNum = 0;
var precacheTourImages = new Array();

if (document.images) {
    for (var i=0; i < inputsArray.length; i+=4) {

        precacheTourImages[keys[num]+precacheSets+IMGLRG] = new Image();
        precacheTourImages[keys[num]+precacheSets+IMGOFF] = new Image();
        precacheTourImages[keys[num]+precacheSets+IMGON] = new Image();
        precacheTourImages[keys[num]+precacheSets+IMGLRG].src = inputsArray[i];
        precacheTourImages[keys[num]+precacheSets+IMGOFF].src = inputsArray[i+1];
        precacheTourImages[keys[num]+precacheSets+IMGON].src = inputsArray[i+2];
        captionArray[captionNum] = inputsArray[i+3];
        num += 1;
        captionNum += 1;
        if (num == 5) {
            precacheSets += 1;
            num = 0;
        }
    }
}
//End Preload

