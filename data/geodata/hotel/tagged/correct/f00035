}

function showCascadeSearch(sHotelID, bFromPop, sLinkType)
{
    if(sHotelID == null || sHotelID == '')
    {
        return;
    }
    var tgtUrl = "/HotelDateSearch?d=" + sHotelID + "&returnTo=" + "__2F__Hotels__2D__g32655__2D__Los_Angeles_California__2D__Hotels__2E__html" + "&fromPop=" + bFromPop;
    var sFromSource = 'Hotels';
    if(sFromSource !="")
    {
        
        if(sLinkType && sLinkType != "")
        {
          sFromSource += sLinkType;
        }

        tgtUrl += "&from=" + sFromSource;
    }


        var w = window.open ( tgtUrl, "CascadeSearch", "toolbar=0,resizable=1,menubar=0,location=0,status=0,scrollbars=1,width=245,height=610,screenX=5,screenY=5,left=5,top=5");
  

    if (w != null)
    {
        w.opener = self;
        w.focus();
    }
}

