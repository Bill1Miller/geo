function doTellYourFriends()
{
    var tgtUrl = "/TellFriends-xLos+Angeles+Hotels%3A+Read+Los+Angeles+Hotel+Reviews+and+Compare+Prices+__2D__+TripAdvisor-w__2F__Hotels__2D__g32655__2D__Los_Angeles_California__2D__Hotels__2E__html";

