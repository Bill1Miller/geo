<script>
var doc
var name = navigator.appName; var vers = navigator.appVersion;
((name == "Netscape") ? doc = window.document : doc = document.all)
//if (name == "Netscape"){doc = window.document}else{doc = document.all}
vers = vers.substring(0,1); // or 0,4  could return 4.5 instead of just 4

function version(){
if (vers > 3){return vers;}else{vers = "-1"}; return vers;
}

var arrImg = new Array("ammenities","services","rooms","dining","reserve","slideshow");
if (vers <= version()) {
	for(var i = 0; i < arrImg.length; i++){
		arrImg[i] = new Image(); arrImg[i].src = "http://www.marqueen.com/images/"+arrImg[i]+"h.gif"
    }
}
function swapImage2(item){
	var strImage = doc[item].src
	if (strImage.indexOf("-n") != -1){strImage = strImage.replace(/-n/i, "-h");}else{strImage = strImage.replace(/-h/i, "-n");}
	doc[item].src = strImage
}
function swapImage(item){
	if(doc[item].src == "http://www.marqueen.com/images/"+item+"n.gif"){doc[item].src = "http://www.marqueen.com/images/"+item+"h.gif"}else{doc[item].src = "http://www.marqueen.com/images/"+item+"n.gif"}
}
function swapPhoto(item){
	doc.homeimage.src = "/images/" + item
}
</script>
