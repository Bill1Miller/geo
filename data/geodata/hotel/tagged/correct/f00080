<script language="JavaScript">
function onSearchFormSubmit(string) {
    result = searchStringIsValid(string);
    if (result == true) {
        updateSD();
    }
    return result;
}

function searchStringIsValid(string) {
    if (string == '' || string == 'Search LA.com') {
        result = false;
    } else {
        result = true;
    }
    if (result == false) {
        alert("You must enter a search word or words");
    }
    return result;
}

function updateSD() {
    frm = document.searchform;
    document.searchform.sd.value = 'Results for "' + frm.stxt.value + '"';
    //alert("updateSD document.searchform.sd.value = " + document.searchform.sd.value);
    return true;
}
</script>
