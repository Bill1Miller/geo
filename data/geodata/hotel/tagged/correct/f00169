		<script type="text/javascript">
		var fromSearch = '';
		var locat = 'search';

		function gotoPage(page) {
			document.mainForm.action = page;
			document.mainForm.submit();
			return true;
		}

		function goToRooms(page){

			document.mainForm.action = page;
			document.mainForm.submit();
			return true;
		}

		//go to the preference page
		function gotoPref(page) {
			if(fromSearch == '78XX'){
				document.mainForm.action = "/whotels/meetings/pre_decider_all.html";
			}
			else{
				document.mainForm.action = page;
				if(fromSearch == '22XX' || fromSearch == '78XX' || fromSearch == '22SPG'){
					document.mainForm.requestedAffiliationCode.value = '';
					document.mainForm.requestedChainCode.value = '';
				}
			}
			if (locat == 'starpoints'){
				document.mainForm.action = "/whotels/starpoints/redeem_dates_prefs.html";
			}
			document.mainForm.submit();
			return true;
		}

		function getPrefsAll(propid, chain, affl, cat, minPts, maxPts){

			document.multiForm.propertyID.value = propid;
			document.multiForm.requestedChainCode.value = chain;
			document.multiForm.requestedAffiliationCode.value = affl;
			document.multiForm.categoryNumber.value = cat;
			document.multiForm.minPoints.value = minPts;
			document.multiForm.maxPoints.value = maxPts;
			//document.multiForm.action = "/preferredguest/starpoints/redeem_nodates_decider.html";
			document.multiForm.action = "/preferredguest/starpoints/redeem_dates_prefs.html";

			document.multiForm.submit();
			return true;
		}


		function nextFloor(){
			document.floorForm.action ="meetings_events_facilities.html";

			if(document.floorForm.floorVal == null || document.floorForm.floorVal.value == ""){
				document.floorForm.floorVal.value = 1;
				document.floorForm.floorVal.value = parseInt(document.floorForm.floorVal.value) + 1;

				document.floorForm.submit();
				return true;
			} else {
				document.floorForm.floorVal.value = parseInt(document.floorForm.floorVal.value) + 1;
				document.floorForm.submit();
				return true;
			}
		}

		function prevFloor() {
			document.floorForm.action = "meetings_events_facilities.html";
			document.floorForm.floorVal.value = parseInt(document.floorForm.floorVal.value) -1;
			document.floorForm.submit();
			return true;
		}
		</script>
