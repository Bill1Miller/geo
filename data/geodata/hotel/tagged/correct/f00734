		function elevenPlus(){
			if (document.obo.tickets.value==100){
				document.obo.tickets.selectedIndex=0;
				groupSales=window.open('groupSales.aspx?prodID=791','groupSales','width=320,height=160,toolbar=0,menubar=0,status=0,scrollbar=0');
				groupSales.focus();
			}
			changeFindState();
		}
		function changeFindState(){
			grayed=(document.obo.tickets.value>0 && document.obo.performance.value!='none')?'':'Grayed';
			document.findButton.src='images/findHome'+grayed+'.gif';
		}
		function bestAvailable(){
				ba=window.open('bestAvailable.aspx','bestAvailable','width=320,height=160,toolbar=0,menubar=0,status=0,scrollbar=0');
				ba.focus();
		}

		function find(){
			if(document.obo.tickets.value>0 && document.obo.performance.value!='none'){
				theDate='';
				theDate+=document.obo.year.value;
				if(document.obo.month.value<10){theDate+='0';}
				theDate+=document.obo.month.value;
				if(document.obo.day.value<10){theDate+='0';}
				theDate+=document.obo.day.value;
address='onlineBoxOffice/captcha.aspx?reset=1&prodID=791&tickets='+
document.obo.tickets.value+
'&single='+theDate+'&dateRadio=1&t='+document.obo.performance.value+'&seatingRadio=1';
				launchOnlineBoxOffice(0,address);
			}
		}
		function advancedFind(){
			page='';
			vars='reset=1&prodID=791&';
			if(document.obo.tickets.value>0){
				page='dateTime.aspx?';

				theDate='';
				theDate+=document.obo.year.value;
				if(document.obo.month.value<10){theDate+='0';}
				theDate+=document.obo.month.value;
				if(document.obo.day.value<10){theDate+='0';}
				theDate+=document.obo.day.value;

				vars+='tickets='+document.obo.tickets.value+'&single='+theDate+'&t='+document.obo.performance.value+'&dateRadio=1&';
			}else{
				page='show.aspx?';
			}
			launchOnlineBoxOffice(0,'onlineBoxOffice/'+page+vars.substr(0,vars.length-1));
		}
