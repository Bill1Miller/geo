var dteSecondValidDate = new Date(dteFirstValidDate.getTime() + (1000*60*60*24*14));
var dteToMonth=parseInt(dteSecondValidDate.getMonth());
var dteToYear= dteSecondValidDate.getFullYear();
calendar.initialViewsToCreate=1;
calendar.initialize();
