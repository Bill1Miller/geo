<script type="text/javascript">
function onerror_show_callstack(func, stackDepth) {
  var result = "";
  var num = 0;
  if (stackDepth == null) stackDepth  = 3;
  var ANON_FUNC_LENGTH_MAX_TO_SHOW = 300;
   while (func != null && num++ < stackDepth ) { // just print out the top
    try {
      var pattern = / [^ (]+/; //The func name will have a space before it.
      var name = pattern.exec(func.toString())[0];
      if (name == ' anonymous' || name == ' command' || name == ' commandOne' || name == ' commandTwo') {
        var funcString = func.toString();
        var code =    funcString.substring(funcString .indexOf("{"), funcString.length);
        var label = " ANON: ";
        if (name == ' command' || name == ' commandOne' || name == ' commandTwo')label = " CMD: ";
        if (code.length < ANON_FUNC_LENGTH_MAX_TO_SHOW) {
          result += label + code ;
        } else {
          result += label + code.substring(0, ANON_FUNC_LENGTH_MAX_TO_SHOW) + "...";
        }
      } else {
        result += " FUNC:" + name;
      }
    } catch (e) {
      result += " ???";
    }
    try {
      func = func.caller;
    } catch (e) {
    }
   }
   if (func == null) result += " FUNC: window"; // not in a function
   return result;
}
function handleJSError(msg, url, linenumber, stack)
{
    var callstack = 'not retrieved';
    if (document.getElementById && document.createElement) {
  // Advanced enough browser to merit reporting details
  try{
      callstack = (stack != null) ? stack : onerror_show_callstack(handleJSError.caller);
  } catch (e){
       //
  }

    } else {
  // 2nd tier functional browser

    }
    // Event lookup, might only be in IE
    var evt = (window.event) ? window.event: null;
    var evttext = 'No event source.';
    if (evt) {
       if (evt.srcElement) {
    if (window.event.srcElement.id != '') {
      evttext = window.event.srcElement.id;
    } else if (window.event.srcElement.name != '') {
      evttext = window.event.srcElement.name;
    }
       } else if (evt.target) {
    if (evt.srcElement.id != '') {
      evttext = evt.srcElement.id;
    } else if (evt.srcElement.name != '') {
      evttext = evt.srcElement.name;
    }
       }
       evttext += ':' + (evt.type) ? evt.type : 'notype';
    }
    if (msg != null) msg = msg.replace(/(.*)\r\n(.*)/g, "$1$2");
    if (callstack != null) callstack = callstack.replace(/(.*)\n(.*)/g, "$1 $2");
    var errorurl = 'url=' + escape(url) + '&line=' + escape(linenumber)  + '&msg=' + escape(msg) + '&evt=' + escape(evttext) + '&cstack=' + escape(callstack);
    if (document.images) {
  var errorImg = new Image(1,1);
  errorImg.src = '/r9/logjs/images?' + errorurl ;
    }
    // Show error message to user
    return false;
}
window.onerror = handleJSError;
</script>
