  for (i = 0; i < menuArray.length; i++) {
    if (getElement(menuArray[i] + "SubMenu")) {
      getElement(menuArray[i] + "SubMenu").style.top='116px';
    }
  }
  bufferTop = 95;
  bufferBottom = 142;
} else {
  for (i = 0; i < menuArray.length; i++) {
    if (getElement(menuArray[i] + "SubMenu")) {
      getElement(menuArray[i] + "SubMenu").style.top='205px';
    }
  }
  bufferTop = 185;
  bufferBottom = 232;
}
</script>

</body>
