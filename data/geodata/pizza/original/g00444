function hnav_pageset(page) {
  for (x = 0; x < menuArray.length; x++) {
    if (page == menuArray[x]) { navitem = true;break;} else { navitem = false;}
  }
      currMenuItem = 'none';
    onMenuItem = 'nada';
  if (navitem) {
          getElement(page + "Image").src= eval("hnav_" + page + "_on");
          currMenuItem = page;
          onMenuItem = page;
  }
}


function initMouse() {
	document.onmousemove = mouseMove
        currMenuItem = 'none'
	if (is.ns) document.captureEvents(Event.MOUSEMOVE)
}

function mouseMove(e) {
	var x = (is.ns)? e.pageX : event.x+document.body.scrollLeft
	var y = (is.ns)? e.pageY : event.y+document.body.scrollTop
	//status = "x:"+x+" y:"+y

	if (currMenuItem != 'none') {
		if ((y > bufferBottom) || (y < bufferTop) ||  ( x > 725) || (x < 8)) {hnav_out();}
	}
	return true
}

function hnav_over(id) {
if(currMenuItem != id){
  getElement(id + "Image").src= eval("hnav_" + id + "_over");
if (getElement(id + "SubMenu")){
  getElement(id + "SubMenu").style.visibility="visible";
} 
  
  if (id != currMenuItem && currMenuItem != 'none'){getElement(currMenuItem + "Image").src= eval("hnav_" + currMenuItem);}
  if (currMenuItem == onMenuItem){getElement(currMenuItem + "Image").src= eval("hnav_" + currMenuItem + "_on");}
  if (getElement(currMenuItem + "SubMenu")){getElement(currMenuItem + "SubMenu").style.visibility="hidden" } 
  currMenuItem = id;
}
}

function hnav_out() {
  if (currMenuItem != 'none'){
    if (getElement(currMenuItem + "SubMenu")){getElement(currMenuItem + "SubMenu").style.visibility="hidden" } 
  
    if (currMenuItem == onMenuItem){getElement(currMenuItem + "Image").src= eval("hnav_" + currMenuItem + "_on");}
    else {  getElement(currMenuItem + "Image").src= eval("hnav_" + currMenuItem);}
  }
  currMenuItem = "none";
}

function BrowserCheck() {
	var b = navigator.appName
	if (b=="Netscape") this.b = "ns"
	else if (b=="Microsoft Internet Explorer") this.b = "ie"
	else this.b = b
	this.v = parseInt(navigator.appVersion)
	this.ns = (this.b=="ns" && this.v>=4)
	this.ns4 = (this.b=="ns" && this.v==4)
	this.ns5 = (this.b=="ns" && this.v==5)
	this.ie = (this.b=="ie" && this.v>=4)
	this.ie4 = (navigator.userAgent.indexOf('MSIE 4')>0)
	this.ie5 = (navigator.userAgent.indexOf('MSIE 5')>0)
	if (this.ie5) this.v = 5
	this.min = (this.ns||this.ie)
}

// automatically create the "is" object
is = new BrowserCheck()

function getElement(id) {
  var element=document.getElementById?document.getElementById(id):(document.all?document.all(id):null)
 return element;
}
</script>


