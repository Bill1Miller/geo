<script language=javascript>
function changeSortBy (select) {
   var form = document.forms['searchForm'];
   if (select.value != 'relevancy') {
      if (select.value == 'rating') {
         form.order.value = 'desc';
      } else {
         form.order.value = 'asc';
      }
   } else {
      form.order.value = 'desc';
   }

   form.submit();
}

function changeSortOrder (select) {
   var form = document.forms['searchForm'];
   if (select.value == 'descending') {
      form.order.value = 'desc';
   } else {
   
      form.order.value = 'asc';
     
   }

   form.submit();
}

</script>
<input type="hidden" name="sort_order" value="asc">

