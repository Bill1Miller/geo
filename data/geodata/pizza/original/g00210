<script type="text/javascript">
function google_ad_request_done( google_ads ) {
	if (google_ads.length < 1 ) {
		document.getElementById('bottom_ad_block').style.display = 'none';
		return;
	}
	var ads_length;
	if (google_ads.length > 3) {
		ads_length = 3;
	} else {
		ads_length = google_ads.length;
	}
	for(i = 0; i < ads_length; ++i) {
document.write("<TR>");
document.write(" <TD  CLASS=\"network\" valign=\"top\" width=\"85\">");
document.write("  <TABLE BORDER=\"0\" CELLPADDING=\"0\" CELLSPACING=\"0\" width=\"85\">   ");
document.write("   <tr><td align=\"right\" style=\"padding-top:8px;\"><span style=\"color:#698cd3;font-family: arial;font-size:9px\">Local Sponsor</span></td></tr>     ");
document.write("  </TABLE>");
document.write(" </TD>");
document.write(" <TD  CLASS=\"network\" VALIGN=\"top\" WIDTH=\"327\">");
document.write("  <TABLE BORDER=\"0\" CELLPADDING=\"8\" CELLSPACING=\"0\" width=\"327\">");
document.write("	<TR VALIGN=\"top\">");
document.write("     <TD> ");
document.write("      <a href=\"" + google_ads[i].url + "\" class=\"noline_lg\"><B>" + google_ads[i].line1 + "</B></a>");
document.write("      <BR>");
document.write("	  <span class=\"light_blue_eleven\"></span>");
document.write("	  <TABLE BORDER=\"0\" cellpadding=\"0\" CELLSPACING=\"0\">");
document.write("		<TR VALIGN=\"top\">");
document.write("		 <TD class=\"grey_sm2\">");
document.write("			" + google_ads[i].line2 + " " + google_ads[i].line3 + "          ");
document.write("		 </TD>");
document.write("		</TR>");
document.write("	   </TABLE>");
document.write("	 </TD>");
document.write("	</TR>");
document.write("  </TABLE>");
