<script>
function checkMailForm() {
    //alert(document.signUpForm.customVal2.checked);
    if (document.signUpForm.customVal1.checked == true)
    {
        document.signUpForm.customVal1.value = "Yes";
        if (document.signUpForm.customVal2.value == "")
        {
            alert("Please fill out the Presentation Topic field.");
            return false;
        }
    }
    else if (!document.signUpForm.customVal1.checked);
    {
        //alert(document.signUpForm.customVal1.checked);
        document.signUpForm.customVal1.value = "Yes, Please";
    }
    if ( document.signUpForm.fullName.value == '' ||
        document.signUpForm.email.value == '') 
    {
        alert("Please fill out all the fields in bold.");
        return false;
    } else {
        return true;
    }
}
</script>

