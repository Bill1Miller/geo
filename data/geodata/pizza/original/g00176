<script language="javascript">

var winpops;
function openpopup(myurl,id){
    var myurl = myurl || null;
    var id = id || null;
    if(typeof(myurl) == 'string'){
      if (IE){
        if(id == null) return;
        winpops=window.open('',"customer_frameset","width=725,height=450,location=yes,addressbar=no,menubar=yes,toolbar=yes,statusbar=yes,scrollbars=yes,resizable,left=250,top=150");
        var pWin = (document.getElementById) ? document.getElementById(id) : null;
        if(pWin == null) return;
        pWin.href=myurl;
        pWin.target="customer_frameset";
        pWin.click();
      } else {
        winpops=window.open(myurl,"customer_frameset","width=725,height=450,location=yes,addressbar=no,menubar=yes,toolbar=yes,statusbar=yes,scrollbars=yes,resizable,left=250,top=150");
      }
      /* Setting opener of daughter win to null to remove opener control
         from customer javascript. GM */
      winpops.opener = null;
    }
}

function mypopup(new_location,id,parent_location){
    var parent_location = parent_location || null;
    var id = id || null;
    openpopup(new_location,id);
    winpops.focus();
    if(parent_location != null && id != null){
      if (IE){
        var pWin = (document.getElementById) ? document.getElementById(id) : null;
        if(pWin == null) return;
        document.getElementById(id).target="_top";
        document.getElementById(id).href=parent_location;
        document.getElementById(id).click();
      } else {
        window.location=parent_location;
      }
    }
}

