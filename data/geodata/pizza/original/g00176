  function setChangeAreaValuesForHotels() {
    document.anythingTop['lat'].value = '';
    document.anythingTop['long'].value = '';


    if (document.anythingTop.pre_geo_id1.options[document.anythingTop.pre_geo_id1.selectedIndex].value !='' &&
         ( document.getElementById('h_airport_block').style.display == 'block' || 
           document.getElementById('h_neighborhood_block').style.display == 'block') 
       ) {
      document.anythingTop.geo_id.value =
        document.anythingTop.pre_geo_id1.options[document.anythingTop.pre_geo_id1.selectedIndex].value;
      document.anythingTop.where.value='';
    }else
    if (document.anythingTop.pre_geo_id2.options[document.anythingTop.pre_geo_id2.selectedIndex].value !='' &&
         ( document.getElementById('h_airport_block').style.display == 'block' ||
           document.getElementById('h_neighborhood_block').style.display == 'block') 
       ) {
      document.anythingTop.geo_id.value =
        document.anythingTop.pre_geo_id2.options[document.anythingTop.pre_geo_id2.selectedIndex].value;
      document.anythingTop.where.value='';
    }else
    if (document.anythingTop.pre_csz.options[document.anythingTop.pre_csz.selectedIndex].value !='' &&
        document.getElementById('h_attraction_block').style.display == 'block' ) {
      document.anythingTop.city_state_zip.value =
        document.anythingTop.pre_csz.options[document.anythingTop.pre_csz.selectedIndex].value;
      document.anythingTop.where.value='';
      document.anythingTop.hotelAttraction.value = 
        document.anythingTop.pre_csz.options[document.anythingTop.pre_csz.selectedIndex].text;
    }
  }

var initialtab=[1, "sc1"] ;
var previoustab="sc1" ;

function expandcontent(cid, aobject){
    if(cid == 'sc2') 
      document.getElementById('q_askj').value = document.anythingTop.query.value ;
    else if(cid == 'sc1')
      document.anythingTop.query.value = document.asb.q_askj.value ;
    
   if (document.getElementById){
       highlighttab(aobject)
       if (previoustab!="")
          document.getElementById(previoustab).style.display="none"
          document.getElementById(cid).style.display="block"
          previoustab=cid
      }
}

function highlighttab(aobject){
    if (typeof tabobjlinks=="undefined")
       collecttablinks()
       for (i=0; i<tabobjlinks.length; i++)
           tabobjlinks[i].className=""
           aobject.className="current"
}

function collecttablinks(){
    var tabobj=document.getElementById("tablist")
    tabobjlinks=tabobj.getElementsByTagName("input")
}

function do_onload(){
    collecttablinks()
    expandcontent(initialtab[1], tabobjlinks[initialtab[0]-1])
}



