  eval("var HelpWindow = window.open('/files/help/" + Topic + "','Help','width=500,height=400,scrollbars=yes');");
  }


//var url = '/geo/usa/states/california/metros/mountain_view/cities/mountain_view/home/srch_web/';

function testForYP() {
  var match1 = '^.*yellow_pages.*$';
  var re1 = new RegExp(match1);
  if(location.href.match(re1)) {

    return '/geo/usa/states/california/metros/mountain_view/cities/mountain_view/home/srch_local/';

  } else {
      return '/geo/usa/states/california/metros/mountain_view/cities/mountain_view/home/srch_web/';
  }

}

