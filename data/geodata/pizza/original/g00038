<script language="javascript">
function getcookie(cookiename) {
 var cookiestring=""+document.cookie;
 var index1=cookiestring.indexOf(cookiename);
 if (index1==-1 || cookiename=="") return "";
 var index2=cookiestring.indexOf(';',index1);
 if (index2==-1) index2=cookiestring.length;
 return unescape(cookiestring.substring(index1+cookiename.length+1,index2));
}

var cookie      = getcookie("mycityNanoCookie");

if (cookie != ''){
//	alert("cookie found");
        document.write ('<a href="http://cgi.mycity.com/nanoGiveAway/nanoForm.html"><b><font type="verdana" size="4pt" color="red">click here to complete ipod nano sweepstakes survey</font></b></a>');
}else{
//	alert("cookie not found");	
}


</script>

