<script>
var domain = '';
var hrefdomain='';
if(document.referrer){
  var lead_slashes = document.referrer.indexOf("//");
  var domain_start = lead_slashes + 2;
  var without_resource = document.referrer.substring(domain_start, document.referrer.length);
  var next_slash = without_resource.indexOf("/");
  domain = without_resource.substring(0, next_slash);
  lead_slashes = window.location.href.indexOf("//");
  domain_start = lead_slashes + 2;
  without_resource = window.location.href.substring(domain_start, window.location.href.length);
  next_slash = without_resource.indexOf("/");
  hrefdomain = without_resource.substring(0, next_slash);
  // added by kevin cuxil 12.6.2005 to accomodate for subdomain re-directs
  var leader = hrefdomain.substring(0,3);
  if (!/^www/.test(leader)) {
    hrefdomain = hrefdomain.replace(/^(.)*?\./, 'www.');
  }
  // end addition
}
else {
  //if(location.search.indexOf("CAMP") != -1) { alert(location); }
}
if(!document.referrer || (domain && (domain != hrefdomain))){
document.write("<IMG src='http://hits.411web.com/cgi-bin/hit?saleclient=228' width=1 height=1 border=0>");
}
</script>

