function addFilmstripData(data,id,selected,left,right, tableId) {
  var fs = new YpFilmstrip(id,selected,left,right);
  for (var i=0;i<data.length;i++) {
	fs.addImage(data[i]);
  }
  ypSnapCtrl.addFilmstrip(fs,tableId);
}

function panLoaderReady() {

  ypSnapCtrl.initCount++;
  if (ypSnapCtrl.initCount == 2) {
    ypSnapCtrl.preloadPan();
  }
}


ypSnapCtrl = new YpSnapshotControl(ypSettings);

