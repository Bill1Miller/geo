<script language="Javascript" type="text/javascript">
<!--


/*js yp */


/* YP COMBO-BOX */

function YpComboBox(inputId,iframeId, buttonId, width, boxId, onchangeListener) {
  this.boxId = boxId;
  eval('window.'+boxId+' = this');
  this.inputId = inputId;
  this.onchangeListener = onchangeListener;
  this.buttonId = buttonId;
  this.iframeId = iframeId;
  this.width = width;
  this.isOpen = false;
  this.options = [];
  this.selectedIndex = -1;
  this.curHiLite = -1;
  this.initialized = false;
  this.dom = {};
  this.optionHeight = 18;
  this.hrHeight = 9;
  this.isIE = ((navigator.userAgent.toLowerCase()).indexOf('msie') > 0);
  this.isSafari = ((navigator.userAgent.toLowerCase()).indexOf('safari') > 0);
  this.isOpera = ((navigator.userAgent.toLowerCase()).indexOf('opera') > 0);
  this.style = {
  	over:'font-family:Verdana, sans serif;font-size:12px;padding:1px 1px 1px 10px;margin:0px;'+(this.isSafari||this.isOpera ? 'background-color:#DDDDFF;' : 'background-color:highlight;color:highlighttext'),
	norm:'background-color:#FFFFFF;font-family:Verdana, sans serif;font-size:12px;padding:1px 1px 1px 10px;color:#000000;margin:0px;',
	hr:'font-family:Verdana, sans serif;font-size:12px;border-bottom:1px solid #000000;margin:3px 3px 0px 3px;',
	title:'font-weight:bold;font-style:italic;font-family:Verdana, sans serif;font-size:12px;padding:1px;',
	required:'cursor:default;overflow:hidden;width:'+this.width+'px;height:'+this.optionHeight+'px;background-color:#FFFFFF;'
  };

  this.doClose = function(evt) {
    evt = (evt) ? evt : window.event;
	if (evt) evt.cancelBubble = true;
  	this.dom.iframe.style.visibility = 'hidden';
	//this.dom.iframe.style.display = 'none';
	this.isOpen = false;
	document.onclick = null;
  }
  this.doOpen = function (evt) {
    evt = (evt) ? evt : window.event;
	if (evt) evt.cancelBubble = true;
    if (!this.initialized) this.init(); 
	var iframe = document.getElementById(this.iframeId);
	var input = document.getElementById(this.inputId);
 	iframe.style.left = this.getClientX(input) + 'px';

	this.dom.iframe.style.visibility = 'visible';
	//this.dom.iframe.style.display = 'block';
	this.isOpen = true;
	document.onclick = new Function('event','window.'+this.boxId+'.doClose(event)');
  }
  this.toggle = function(evt) {
    with (this) { if (isOpen) doClose(evt); else doOpen(evt); }
  }

  this.init = function() {

	var iframe = document.getElementById(this.iframeId);
	if (!iframe) iframe = frames[this.iframeId]; 
	if (!this.isSafari) iframe.style.display = 'none';
	

	this.dom.iframe = iframe;
	var doc = (iframe.contentWindow) ? iframe.contentWindow.document : iframe.document;
	var dbody = (doc.body) ? doc.body : doc;
	var h = 0;
// in case mason spits out stuff we don't want
  if (dbody.hasChildNodes()) {
     var cns = dbody.childNodes;
	for (var i=cns.length-1;i>=0;i--) {
	if (cns[i].style) cns[i].style.display = 'none';
        else dbody.removeChild(cns[i]);
	
    }
  }

	var enclosing = doc.createElement('DIV');
	for (var i=0;i<this.options.length;i++) {

	  var div = doc.createElement('DIV');
	  var div2 = doc.createElement('DIV');


	  div.appendChild(div2);
	  div.setAttribute('index',i);
	  var opt = this.options[i];
	  //div.style.cssText = this.style.required;
          this.setCssText(div,this.style.required);
	  if (opt.type == 'hr') {
	    h+=this.hrHeight;
		//div2.style.cssText = this.style.hr;
		this.setCssText(div2,this.style.hr);
		div2.style.height = '2px';
		div2.style.overflow = 'hidden';
		div.style.height = this.hrHeight + 'px';
		div.style.backgroundColor = '#FFFFFF';
	  }
	  else {
	    var txt = doc.createTextNode(opt.text);
	    div2.appendChild(txt);
		h+=this.optionHeight;
		if (opt.type == 'title') {
		  //div2.style.cssText = this.style.title;
		            this.setCssText(div2,this.style.title);
		}
		else {
		  this.setCssText(div2,this.style.norm);
		  //div2.style.cssText = this.style.norm;
	      div.onmouseover = new Function('top.'+this.boxId+'.optionOver(this)');
	      div.onmouseout = new Function('top.'+this.boxId+'.optionOut(this)');
	      div.onclick = new Function('top.'+this.boxId+'.optionClick(this)');
		}
	  }


	  enclosing.appendChild(div);
  //dbody.appendChild(div);	  
	}
dbody.appendChild(enclosing);	

var eh = (window.event) ? h+2 : h;
var ew = (window.event) ? this.width+2 : this.width;

//enclosing.style.cssText = 'border: 1px solid black;overflow:hidden;width:'+ew +'px;height:'+ eh + 'px;';  
          this.setCssText(enclosing,'border: 1px solid black;overflow:hidden;width:'+ew +'px;height:'+ eh + 'px;')  
	var dbs = (dbody.style) ? dbody : dbody.firstChild;
	dbs.style.margin='0px';
        dbs.style.padding = '0px';
	dbs.style.backgroundColor='#FFFFFF';

        //alert(h+2);
	iframe.style.height = (h+2) + 'px';
	iframe.style.width = (this.width+2) + 'px';
	if (this.isSafari || this.isIE) {
	  iframe.style.height = (h+4) + 'px';
	  iframe.style.width = (this.width+4) + 'px';
     	}

	iframe.style.position = 'absolute';
	iframe.style.zIndex = 1000;
	var input = document.getElementById(this.inputId);
	input.onchange=new Function('top.'+this.boxId+'.handleChange()');
	iframe.style.top = (this.getClientY(input) + input.offsetHeight) + 'px';
	iframe.style.left = this.getClientX(input) + 'px';
	
	this.initialized = true;
	if (!this.isSafari) iframe.style.display = 'block';
  }
  this.setCssText = function(obj,txt) {

    if (!this.isOpera) {
      obj.style.cssText = txt;
    }
    else {
      var data = txt.split(';');
      for (var i=0;i<data.length;i++) {
        var pair = data[i].split(':');
        if (pair.length == 2) {
        var st = this.trim(pair[0]);
        var val = this.trim(pair[1]);
        var dash = st.indexOf('-');
        if (dash >=0) {
          st = st.substring(0,dash) + new String(st.substr(dash+1,1)).toUpperCase() + st.substring(dash+2);
        }
        obj.style[st] = val;
      }     
      }

    }
  }
  this.trim = function (s) {
  if (s.length == 0) return '';
  while (s.substring(0,1) == ' ') {
    s = s.substring(1,s.length);
  }
  while (s.substring(s.length-1,s.length) == ' ') {
    s = s.substring(0,s.length-1);
  }
  return s;
}
  this.handleChange = function() {

    var input = document.getElementById(this.inputId);
    if (this.selectedIndex >= 0 && input.value != this.options[this.selectedIndex].text) {
	this.clearSelection();
    }
    if (this.onchangeListener) onchangeListener();
  }

  this.optionOver = function(div) {

    if (this.selectedIndex >=0) this.optionOutIndex(this.selectedIndex); 
    //div.firstChild.style.cssText = this.style.over;
    this.setCssText(div.firstChild,this.style.over);

  }
  this.optionOut = function(div) {
    //div.firstChild.style.cssText = this.style.norm;
    this.setCssText(div.firstChild,this.style.norm);
  }
  this.optionOutIndex = function(i) {
     
     var doc  = (this.dom.iframe.contentWindow) ? this.dom.iframe.contentWindow.document : this.dom.iframe.document;
     var divs = doc.getElementsByTagName('div');
	
     var d = divs[i*2+1];
     this.optionOut(d);

  }

  this.optionClick = function(div) {
    with (this) {  
      var newIndex = parseInt(div.getAttribute('index'));
      var input = document.getElementById(inputId);
      var oldIndex = selectedIndex;
      input.value = options[newIndex].text;
      clearSelection();
    
      selectedIndex = parseInt(newIndex);
      doClose();
      if (oldIndex != newIndex && onchangeListener) onchangeListener();
    }
  }
  this.clearSelection = function() {
    with (this) {
      if (selectedIndex >=0) {
	  optionOutIndex(selectedIndex);
	}
      selectedIndex = -1;
    }

  }
  
  this.getClientX = function(obj) {
  	return (obj) ? obj.offsetLeft + this.getClientX(obj.offsetParent) : 0;
  }
  this.getClientY = function(obj) {
    return (obj) ? obj.offsetTop + this.getClientY(obj.offsetParent) : 0;
  }
}

/* SEARCH */

function ypComboOnChange() {
  if (ypLocationCombo.selectedIndex >=0) {
    var opt = ypLocationCombo.options[ypLocationCombo.selectedIndex];
    if (opt.value == 'change_address_page') {
      document.location = ypChangeLocationUrl;
    }
    else {

      var f = document.forms['curLocForm'];
      f.locationDropdown.value = opt.value;
    }
  }
  else {
    var f = document.forms['curLocForm'];	
    f.locationDropdown.value = '';	

  }
}

/* BIG MAP */


function ypGetMap(i) {
  if (document.getElementById) {
    return document.getElementById("ypMap"+i);
  }
}

function ypGetEnt(i) {
  if (document.getElementById) {
    return document.getElementById("_ypEnt"+i);
  }
}

function ypOver(i) {
  var e = ypGetMap(i);
  if (e) {
    e.className = "ypMapHover";
    e.style.zIndex |= 1024;
  }
  e = ypGetEnt(i);
  if (e) {
    e.className = "ypMapHover";
  }
}

function ypOut(i) {
  var e = ypGetMap(i);
  if (e) {
    e.className = "ypMapOut";
    e.style.zIndex &= ~1024;
  }
  e = ypGetEnt(i);
  if (e) {
    e.className = "ypMapOut";
  }
}



/* CATEGORIES */

function ypToggleBrowseCategories(anc) {
  var tab = document.getElementById('browseHidable');
  if (tab.style.display == 'none') {
    tab.style.display = 'block';
    anc.firstChild.data = 'Show Fewer Categories';
    anc.setAttribute('title','Show Fewer Categories');
  }
  else {
    tab.style.display = 'none';
    anc.firstChild.data = 'See All Categories';
    anc.setAttribute('title','See All Categories');
  }

}

/* FILMSTRIP */

function YpFilmstrip(id,defaultSelected,left,right) {
  this.id = id;
  this.asin = '';
  this.left = left;
  this.right = right;
  this.defaultSelected = defaultSelected;
  this.images = [];
  /*
  this.addImage = function(id,thumb,view,pop) {
    var idx = this.images.length;
    this.images[idx] = new YpImage(idx, id, thumb, view, pop);
  }
  */
  this.addImage = function(data) {
    var idx = this.images.length;
    this.images[idx] = new YpImage(idx, data.id, data.thumb, data.view, data.pop,this.id);
  }
  this.canPanLeft = function() {
    return (this.left != null && this.left != '');
  }
  this.canPanRight = function() {
    return (this.right != null && this.right != '');
  }
  this.loadXML = function(xmlDoc) {
    //alert('loadxml');
    var children = xmlDoc.documentElement.childNodes;
    var xImgs = [];
    for (var i=0;i<children.length;i++) {
      var n = children.item(i);
      
      if (n.tagName == 'images') xImgs=n;
      else if (n.tagName) this[n.tagName] = (n.childNodes.length>0 ? n.firstChild.data : '');

    }
    this.loadXMLImages(xImgs);
  }
  this.loadXMLImages = function(imagesNode) {
    var children = imagesNode.childNodes;
    var count = 0;
    for (var i=0;i<children.length;i++) {
      var n = children.item(i);
      if (n.tagName != 'image') continue;
      var nodes = n.childNodes;
      var img = new YpImage();
        img.idx = count;
        img.fsId = this.id;
      for (var j=0;j<nodes.length;j++) {
        var imgProp = nodes.item(j);
        img[imgProp.tagName] = (imgProp.childNodes.length>0 ? imgProp.firstChild.data : '');
      }
      this.images[count] = img;
      count++;
    }
  }
}

function YpImage(idx, id, thumb, view, pop, fsId) {
  this.idx = idx;
  this.id = id;
  this.view = view;
  this.thumb = thumb;
  this.pop = pop;
  this.fsId = fsId;
  this.unHilite = function() {
 
    var td = document.getElementById('ypi_' + this.fsId + '_' + this.idx).parentNode;
    if (td.tagName.toUpperCase() != 'TD') td = td.parentNode;
    td.className = 'ypImageNorm';
  }
  this.hilite = function() {
    
    var td = document.getElementById('ypi_' + this.fsId + '_' + this.idx).parentNode;
    if (td.tagName.toUpperCase() != 'TD') td = td.parentNode;
    td.className = 'ypImageHover';
  }
  this.hideView = function() {
	var img = document.getElementById('yps_' + this.fsId + '_' + this.idx);
	img.style.display = 'none';
    img.style.zIndex = 0;
  }
  this.showView = function() {
 	var img = document.getElementById('yps_' + this.fsId + '_' + this.idx);
	img.style.zIndex = 100;
	img.style.display = 'block';
  }
  this.swapView = function(prev) {

 	var p = document.getElementById('yps_' + prev.fsId + '_' + prev.idx); 	
	var img = document.getElementById('yps_' + this.fsId + '_' + this.idx);
        img.style.zIndex=100;
        p.style.zIndex = 0;
	img.style.display = 'block';      
	//p.style.display = 'none';

  }

}

function YpSnapshotControl(props) {
  this.isSafari = ((navigator.userAgent.toLowerCase()).indexOf('safari') > 0);
  this.isOpera = ((navigator.userAgent.toLowerCase()).indexOf('opera') > 0);

  this.bestImageId = '';
  this.initCount = 0;
  this.id = 'YSC' + Math.floor(Math.random()*1000);

  eval('window.'+this.id+' = this');
  
  
  for (p in props) {
    this[p] = props[p];
  }
  this.filmstrips = [];
  this.wheelActive = false;
  this.wheelOver = function() {

    if (window.event && !this.wheelActive) {
	this.wheelActive = this.curImage;  
	setTimeout('window.'+this.id+'.checkWheel()',500);
    }
  }
  this.wheelOut = function() {
    if (window.event) {
    this.wheelActive = false; 
    var tab = document.getElementById('thumbTab');
    tab.onmousewheel = null;
}
  }
  this.checkWheel = function() {
    if (this.wheelActive == this.curImage) {
      var tab = document.getElementById('thumbTab');
      tab.onmousewheel = new Function('window.'+this.id + '.wheel()');
    }
  }
  this.wheel = function() {
  if (window.event) {
    var e = window.event;

    if (e.wheelDelta > 0) {
      if (this.curImage.idx < this.curFilmstrip.images.length-1) this.swap(this.curImage.idx+1);
    }
    else if (e.wheelDelta < 0) {
      if (this.curImage.idx > 0) this.swap(this.curImage.idx-1);
    }
   e.cancelBubble = true;
   e.returnValue = false;
   return false;
  }
}


  this.thumbOver = function(idx) {
    this.swap(idx);

  }
  this.thumbOut = function() {
  
  }
  this.voteOver = function(idx) {
    this.thumbOver(idx);
  }
  this.voteOut = function(idx) {
    this.thumbOut(idx);
  }

  this.panRequested = false;

  this.panLeft = function() {
    if (this.panRequested) return false;
    this.pan('left');
    return false;
  }
  this.panRight = function() {
    if (this.panRequested) return false;
    this.pan('right');
    return false;
  }

  this.pan = function(direction) {
    
    var fsId = this.curFilmstrip[direction];
    if (fsId) {
      var fs = this.filmstrips[fsId];
      if (fs) {
        this.panRequested = direction;
        this.doPan(direction);
      }
      else {
        this.panRequested = direction;
        this.showWait();
      }
    }
  }
  this.hideAllView = function(fs) {
     for (var i=0;i<fs.images.length;i++) {
       var img = fs.images[i];
       img.hideView();
    }
  }

  this.animateDx = 100;
  this.animateDt = 50;
  this.animateId = '';
  this.panWidth = 553;
  this.doPanAnimate = function(direction,left,right) {
  	  var dLeft = document.getElementById(left);
	  var dRight = document.getElementById(right);
	  var x1 = parseInt(dLeft.style.left);
	  var x2 = parseInt(dRight.style.left);
	var isEnding = false;
    if (direction == 'left') {
       x1 += this.animateDx;
	   x2 += this.animateDx;
	   if (x1 >= 0) {
		  x1 = 0; x2 = this.panWidth; isEnding = true;
		  dRight.style.display = 'none';
	   }
	}
	else {
       x1 -= this.animateDx;
	   x2 -= this.animateDx;
	   if (x2 <= 0) {
	     x1 =this.panWidth; x2 = 0; isEnding = true;
		 dLeft.style.display = 'none';
	   }
	}
	     dLeft.style.left = x1+'px';
		 dRight.style.left = x2+'px';
	if (isEnding) {
	  clearInterval(this.animateId);
	  this.completePan(direction);
	}
  }
  
  this.completePan = function(direction) {
    var oldFs = this.curFilmstrip;
    var fsId = this.curFilmstrip[direction];
    var fs = this.filmstrips[fsId];
	
    this.panRequested = false;
    this.curFilmstrip = fs;
    this.checkDisablePans();
    if (direction == 'left') {
      this.thumbOver(this.curFilmstrip.images.length-1);
    }
    else {
      this.thumbOver(0);
    }
	
    this.resetVotes();
    this.preloadPan();
    this.hideAllView(oldFs);
  }
 
  this.doPan = function(direction) {
    var oldFs = this.curFilmstrip;
    var fsId = this.curFilmstrip[direction];
    var fs = this.filmstrips[fsId];
    var fsDomId = 'fs_'+fs.id;
    var oldFsDomId = 'fs_'+this.curFilmstrip.id
    this.hideWait();
	var fsDom = document.getElementById('fs_'+fs.id);
   //if (this.isOpera) {
    if (direction == 'left') {
	  fsDom.style.left = '-'+this.panWidth+'px';
	  fsDom.style.display = '';
	  this.animateId = window.setInterval('window.'+this.id+'.doPanAnimate("'+direction+'","'+fsDomId+'","'+oldFsDomId+'")',this.animateDt);
	}
	else {
	  fsDom.style.left = this.panWidth+'px';
	  fsDom.style.display = '';
	  this.animateId = window.setInterval('window.'+this.id+'.doPanAnimate("'+direction+'","'+oldFsDomId+'","'+fsDomId+'")',this.animateDt);
	}

  }

  this.preloadPan = function() {

    var x = this.doPreloadPan('left');
    var y = this.doPreloadPan('right');
    return (x || y);
  }

  this.doPreloadPan = function(direction) {

    var id = this.curFilmstrip[direction];
    if (id && !this.filmstrips[id]) {

      var revDir = (direction == 'left') ? 'right' : 'left';
      var url = this.panBaseUrl;

      if (this.panBaseUrl.indexOf('?') > 0) url +='&snapshot='+id + '&'+revDir+'='+this.curFilmstrip.id;
      else url +='?snapshot='+id + '&'+revDir+'='+this.curFilmstrip.id;

      //var url = this.panBaseUrl + '&snapshot='+id + '&'+revDir+'='+this.curFilmstrip.id;
      
      try {
       var frm = document.getElementById('ypFSLoadFrame_' + direction);
       var doc = frm.contentWindow ? frm.contentWindow.document : frm.document;
       doc.location.replace(url);
      }
      catch (e) {

      }
       //prompt('debug',url);
       //frm.location = url;
    }
  }

  this.showWait = function() {
    var mess = document.getElementById('ypFSLoadMess');
    var holder = document.getElementById('filmstripHolder');
    var x = this.getClientX(holder)+ Math.floor((this.panWidth-mess.offsetWidth)/2);
    var y = this.getClientY(holder)+18;
    mess.style.top = y + 'px';
    mess.style.left = x + 'px';
    mess.style.visibility = 'visible';
  }
  this.hideWait = function() {
    var mess = document.getElementById('ypFSLoadMess');
    mess.style.visibility = 'hidden';
  }
  this.getClientX = function(obj) {
  	return (obj) ? obj.offsetLeft + this.getClientX(obj.offsetParent) : 0;
  }
  this.getClientY = function(obj) {
    return (obj) ? obj.offsetTop + this.getClientY(obj.offsetParent) : 0;
  }

  this.resetVotes = function(showMessage) {
     for (var i=0;i<this.curFilmstrip.images.length;i++) {
      var img = this.curFilmstrip.images[i];
      var imgDom = document.getElementById('yp_bi_'+i);
      if (imgDom) {
      if (img.id == this.bestImageId) {
	if (showMessage) {
          if (imgDom.src != this.vote_ch_thanks) imgDom.src = this.vote_ch_thanks;
        }
        else {
          if (imgDom.src != this.vote_ch_nomess) imgDom.src = this.vote_ch_nomess;
         }
      }
      else {
	if (imgDom.src != this.vote_unch_nomess) imgDom.src = this.vote_unch_nomess;
      }
     }
    }    
  }
  this.voteClick = function(idx) {
    var selImg = this.curFilmstrip.images[idx];
    if (selImg.id == this.bestImageId) return;
    if (this.isSignedIn) {
    this.bestImageId = selImg.id;
    this.resetVotes(true);
    var frm = document.getElementById("ypBIFrame");  
    var doc = frm.contentWindow ? frm.contentWindow.document : frm.document;
    var url = this.voteBaseUrl + '&imgid='+selImg.id+'&url='+this.blankUrl;
    //prompt('debug',url);
    doc.location.replace(url);

   }
   else {
     var url = this.signInUrl + document.location.search;
     if (url.indexOf('?') > 0) url +='&';
     else url += '?';
     url += 'imgid='+selImg.id+'&ASIN='+this.asin+'&flag=BestImage&baseURL='+escape(this.baseUrl);
     document.location = url;

   }
  
  }
  this.rightImage = function() {
    if (this.curImage.idx != this.curFilmstrip.images.length-1) {
      this.swap(this.curImage.idx+1);
    }
    else if (this.curFilmstrip.canPanRight()) {
      this.panRight();
    }
  }
  this.leftImage = function() {
    if (this.curImage.idx != 0) {
      this.swap(this.curImage.idx-1);
    }
    else if (this.curFilmstrip.canPanLeft()) {
	  this.panLeft();
	}
  }

  this.changeFilmstrip = function(fsId) {
    this.curFilmstrip = this.filmstrips[fsId];
  }
  this.init = function(fsId) {

    this.curFilmstrip = this.filmstrips[fsId];
    this.curImage = this.curFilmstrip.images[this.curFilmstrip.defaultSelected];
    this.resetVotes();
    //this.addVoteImages();
    this.checkDisablePans();
/*
       var frm = document.getElementById('ypFSLoadFrame_left');
       var doc = frm.contentWindow ? frm.contentWindow.document : frm.document;
       doc.location.replace(this.panLoaderUrl);
       frm = document.getElementById('ypFSLoadFrame_right');
       doc = frm.contentWindow ? frm.contentWindow.document : frm.document;
       doc.location.replace(this.panLoaderUrl);
*/       
    //this.preloadPan();
    setTimeout('window.'+this.id+'.checkInitPan()',2500);
  }
  this.checkInitPan = function() {
   if (ypSnapCtrl.initCount == 0) {
    try {
       this.preloadPan();   
    }
    catch (e) { }
    }
  }
  this.doCheckDisable = function(img,txt,canPan,no_img,ok_img) {
    if (!canPan) {
      img.src = no_img;
      txt.style.color = '#999999';
      txt.style.textDecoration = 'none';
    }
    else if (img.src == no_img) {
      img.src = ok_img;
      txt.style.color = '';
      txt.style.textDecoration = '';
    }
  }
  this.checkDisablePans = function() {
    var left = document.getElementById(this.panLeftImageId);
    var leftTxt = document.getElementById(this.panLeftTxtId);
    this.doCheckDisable(left,leftTxt,this.curFilmstrip.canPanLeft(),this.pan_left_image_no,this.pan_left_image_ok);

    var right = document.getElementById(this.panRightImageId);
    var rightTxt = document.getElementById(this.panRightTxtId);
    this.doCheckDisable(right,rightTxt,this.curFilmstrip.canPanRight(),this.pan_right_image_no,this.pan_right_image_ok);
  }
  this.curImage = null;
  this.curVoted = -1;
  this.curFilmstrip = null;
  
  this.popUp = function(idx) {
    var img = (idx) ? this.curFilmstrip.images[idx] : this.curImage;
    snapshotImage = window.open(img.pop,'snapshotImage','width=740,height=500');
	return false;
  }

  this.swap = function(idx) {

    var prevImage = this.curImage; // note: might be from previous filmstrip
    var selImg = this.curFilmstrip.images[idx];
    if (prevImage == selImg) return;
    selImg.hilite();

    if (prevImage) {
	prevImage.unHilite();
      if(prevImage.idx == idx && prevImage.fsId == this.curFilmstrip.id) { return; }

	if (this.bestImageId != prevImage.id) {
	 var txt = document.getElementById('yp_bi_'+this.curImage.idx);
	 txt.src = this.vote_unch_nomess;

	}
    }	  

	  var voteImg = document.getElementById('yp_bi_'+idx);
	  if (voteImg) {
             if (this.bestImageId != selImg.id) {

		voteImg.src = this.vote_unch_mess;
              }
	  }
	 this.curImage = selImg;
	//

	  if (prevImage) {
		
		this.curImage.swapView(prevImage);
		//prevImage.hideView();
	  }
          else {
	    this.curImage.showView();
          }

	 //this.checkNextDisable();
  }
  
 this.checkNextDisable = function () {
  
  if (!this.curFilmstrip.canPanRight()) {
    if (this.curImage.idx == this.curFilmstrip.images.length-1) {
      document.images['nextRightImg'].src = this.right_image_no;
    }
    else if (document.images['nextRightImg'].src != this.right_image_ok) {
      document.images['nextRightImg'].src = this.right_image_ok;
    }
  }
  if (!this.curFilmstrip.canPanLeft()) {
    if (this.curImage.idx == 0) {
      document.images['nextLeftImg'].src = this.left_image_no;
    }
    else if (document.images['nextLeftImg'].src != this.left_image_ok) {
      document.images['nextLeftImg'].src = this.left_image_ok;
    }
  }

  }

  this.addFilmstrip = function(fs, tableId) {
	
    this.filmstrips[fs.id] = fs;
	if (tableId) {
	  this.addThumbJS(tableId);
          
	}
	else {

	  //thumbnail
          tableId = 'fs_' + fs.id;
var isLeft = (this.curFilmstrip && this.curFilmstrip.left == fs.id);
	  var holder = document.getElementById('filmstripHolder');
	  var tab = document.createElement('table');
            tab.setAttribute('id',tableId);
/*
	  for (var i=0;i<7;i++) {
	    var col = document.createElement('col');
	    col.setAttribute('width',79);
	    tab.appendChild(col);	
}

*/
//            tab.style.tableLayout = 'fixed';
        tab.cellSpacing = 0;
	tab.border = 0;
	tab.cellPadding = 0;
        tab.setAttribute('cellspacing',0);
          tab.setAttribute('border',0);
          tab.setAttribute('cellpadding',0);
	  tab.style.position = 'absolute';
		tab.style.top = '0px';
		if (!this.curFilmstrip) { tab.style.left = '0px'; }
		else if (isLeft) { tab.style.left = this.panWidth+'px'; }
		else { tab.style.left = '-'+this.panWidth+'px'; }




	  var tbody = document.createElement('tbody');
  
	  var tr = document.createElement('tr');
	  tab.appendChild(tbody);
	  tbody.appendChild(tr);
	  tab.style.display = 'none';
	  // view
	  var viewHolder = document.getElementById('viewImageHolder');
          // first create the dom object with empty images
	  for (var i=0;i<fs.images.length;i++) {	
	    var viewImg = document.createElement('img');
            viewImg.setAttribute('id','yps_' + fs.id + '_' + i);
            viewImg.style.display = 'none';
	    viewImg.style.position = 'absolute';
            viewImg.style.cursor = 'pointer';
            viewImg.setAttribute('border',0);
            viewImg.setAttribute('src',this.view_loading_image);
            viewImg.style.height = '216px';
            viewImg.style.width = '324px';
            viewImg.className = 'ypImage';
            viewImg.onclick = Function('window.'+this.id + '.popUp()');
            viewHolder.appendChild(viewImg); 
          }

	  for (var i=0;i<fs.images.length;i++) {
	    var imgObj = fs.images[i];
	    var td = document.createElement('td');
		var img = null;
                img = document.createElement('img');
                img.setAttribute('id','ypi_' + fs.id + '_' + i);
		img.setAttribute('border',0);
		img.setAttribute('hspace',4);
		img.setAttribute('vspace',4);
                img.className='ypImage';
                img.style.height = '46px';
                img.style.width = '69px';
                img.setAttribute('src',this.thumb_loading_image);
                img.style.cursor = 'pointer';
                td.className = 'ypImageNorm';
		td.setAttribute('width',79);
 		td.appendChild(img);
		tr.appendChild(td);
	   }
	  holder.appendChild(tab);
          
          if (isLeft) {
            for (var i=fs.images.length-1;i>=0;i--) {
                this.setImageSource(fs.id,fs.images[i],i);
            }
          }
	  else {
	    for (var i=0;i<fs.images.length;i++) {	
	        this.setImageSource(fs.id, fs.images[i],i);
            }
          }
          this.addThumbJS(tableId);
          if (this.panRequested && this.curFilmstrip) {
            if (fs.id == this.curFilmstrip[this.panRequested]) {
              this.doPan(this.panRequested);
	      //window.setTimeout('window.'+this.id +'.doPan("'+this.panRequested+'")',250);
            }
          }
	}
  }
  this.setImageSource = function(fsId, imgObj, idx) {
    var viewImg = document.getElementById('yps_' + fsId + '_' + idx);
    var thumbImg = document.getElementById('ypi_' + fsId + '_' + idx);
    thumbImg.src = imgObj.thumb;
    viewImg.src = imgObj.view;
  }

  this.addThumbJS = function(tableId) {
  	var tab = document.getElementById(tableId);
	var imgs = tab.getElementsByTagName('img');
	for (var i=0;i<imgs.length;i++) {
	  var img = imgs[i];
	  img.onclick = new Function ('top.' + this.id+'.popUp('+i+');');
	  img.onmouseover = new Function ('top.' + this.id+'.thumbOver('+i+');');
	  //img.onmouseout = new Function ('window.' + this.id+'.thumbOut('+i+');');
	  var anc = img.parentNode;
	  anc.onclick = new Function('return false;');
	}
  }
}



/* CUSTOMER IMAGES */

function getElement(id, d) {
    if (!d) d = document;
    if (d.getElementById) {
        return d.getElementById(id);
    }
    if (d.layers && d.layers[id]) {
        return d.layers[id];
    }
    if (d.all && d.all[id]) {
        return d.all[id];
    }
    return false;
}

var ciProductImageView = new Object;
ciProductImageView.index = 0;

function ciProductImageSelect (pos) {
    ciProductImageThumbnailBoxRelease(ciProductImageView.index);
    ciProductImageThumbnailBoxSelect(pos);
    ciProductImageZoom(pos);
    ciProductImageView.index = pos;

    return false;
};

function ciProductImageReset () {
    if ( ciProductImageView.index != 0 ) {
        ciProductImageSelect(0);
    }
    return false;
};

function ciProductImageZoom (imageIndex) {
    var oldImage = getElement('ciProductImageZoom' + ciProductImageView.index);
    var newImage = getElement('ciProductImageZoom' + imageIndex);

    oldImage.style.display = 'none';
    newImage.style.display = 'block';

    return false;
};

function ciProductImageThumbnailBoxRelease (pos) {
    var box = getElement('ciProductImageThumbnailBox'+pos);
    box.className = "ciImgboxlink";
    return false;
};

function ciProductImageThumbnailBoxSelect (pos) {
    var box = getElement('ciProductImageThumbnailBox'+pos);
    box.className = "ciImgselected";
    return false;
};

function ciTrapEvent (e) {
    if (!e) var e = window.event;
    e.cancelBubble = true;
    if (e.stopPropagation) e.stopPropagation();
    return true;
};

function ciProductImageTrapMouseEvents () {
    var trap = getElement('ciProductImageTrapMouseEventsContainer');
    if (trap.addEventListener) {
        trap.addEventListener('mouseout', ciTrapEvent, false);
        trap.addEventListener('mouseover', ciTrapEvent, false);
    } else if ( trap.attachEvent ) {
        trap.attachEvent('onmouseout', ciTrapEvent);
        trap.attachEvent('onmouseover', ciTrapEvent);
    }
};

function ciAddMouseEventsToDocument () {
    var moverCurrent = document.onmouseover;
    var moutCurrent = document.onmouseout;

    if ( typeof moverCurrent != 'function' ) {
        document.onmouseover = function () { ciProductImageReset(); };
    } else {
        document.onmouseover = function () {
            moverCurrent();
            ciProductImageReset();
        };
    }
    if ( typeof moutCurrent != 'function' ) {
        document.onmouseout = function () { ciProductImageReset(); };
    } else {
        document.onmouseout = function () {
            moutCurrent();
            ciProductImageReset();
        };
    }
};

/* MINI-MAP */


function ypGetCookie(name) {
    var start = document.cookie.indexOf(name+"=");
    var len = start+name.length+1;
    if ((!start) && (name != document.cookie.substring(0,name.length))) return null;
    if (start == -1) return null;
    var end = document.cookie.indexOf(";",len);
    if (end == -1) end = document.cookie.length;
    return unescape(document.cookie.substring(len,end));
}

function ypSetCookie(name,value,expires,path,domain,secure) {
    document.cookie = name + "=" +escape(value) +
        ( (expires) ? ";expires=" + expires.toGMTString() : "") +
        ( (path) ? ";path=" + path : "") + 
        ( (domain) ? ";domain=" + domain : "") +
        ( (secure) ? ";secure" : "");
}

function ypDelCookie(name,path,domain) {
    if (Get_Cookie(name)) document.cookie = name + "=" +
       ( (path) ? ";path=" + path : "") +
       ( (domain) ? ";domain=" + domain : "") +
       ";expires=Thu, 01-Jan-70 00:00:01 GMT";
}

var yp_cookie_name = 'yp-prefs';
var yp_cookie_map_scale = 'detail-map-scale';

function ypGetMapScale() {
  var scale = 5280;
  var c = ypGetCookie(yp_cookie_name);
  if (c) {
    var cx = c.split('&');
    for (var i=0;i<cx.length;i++) {
      var p = cx[i];
      if (p) {
        px = p.split('=');
        if (px[0] == yp_cookie_map_scale) {
          return px[1];
        }
      }
    }
  }
  return scale;
}

function ypSaveAllValues(map) {
  var cdata = [];
  var str='';
  for (var name in map) {
    str += name + '=' + escape(map[name]) + '&';
  }
  var c = ypGetCookie(yp_cookie_name);
  var cx;
  if (c) {
    cx = c.split('&');
    for (var i=0;i<cx.length;i++) {
      var p = cx[i];
      if (p) {
        px = p.split('=');
        var found = false;
        for (var n in map) {
           if (n+'' == px[0]+'') { found = true; break; }
        }
        if (!found) str += p + '&';
      }
    }
  }
  ypSetCookie(yp_cookie_name,str,new Date(new Date().getTime()+365*24*3600*1000),'/','amazon.com','');

}


function ypSaveMapScale(val) {
var map = [];
map[yp_cookie_map_scale] = val;
ypSaveAllValues(map);

}

function getZoomIndex() {
  var i=0;
  for (;i<ypMapScaleVals.length;i++) {
    var s = ypMapScaleVals[i];
    if (s == ypMapScale) {
      break;
    }
  }
  return i;
}

function ypMapZoomIn() {
  var sc = ypMapScaleVals[Math.max(0,getZoomIndex()-1)];
  ypMapZoomTo(sc);

}

function ypMapZoomOut() {
  var sc = ypMapScaleVals[Math.min(ypMapScaleVals.length-1,getZoomIndex()+1)];
  ypMapZoomTo(sc);
}
function ypMapZoomToIdx(i) {
  var sc = ypMapScaleVals[i];
  ypMapZoomTo(sc);
}


function ypMapZoomTo(sc) {

  var idx = getZoomIndex();
  document.getElementById('ypMapLevel'+idx).src = ypMapImgLevelOff;
  ypMapScale = sc;
  var u = ifMapURL + '?scale='+sc+'&lat'+ypMapLat+'&long'+ypMapLong+'&ASIN='+ypMapASIN+'&mapPopupURL='+escape(ypMapPopupURL);
  //document.getElementById('ypMiniMapFrame').src = u;
  var ifrm = document.getElementById('ypMiniMapFrame');
  var doc = ifrm.contentWindow ? ifrm.contentWindow.document : ifrm.document;
  doc.location.replace(u);
  var idx = getZoomIndex();
  document.getElementById('ypMapLevel'+idx).src = ypMapImgLevelOn;
  ypSaveMapScale(sc);
}


/* ESTARA */

var wv_vars = typeof(wv_vars)=="undefined" ? new Array() : wv_vars;
wv_vars["ui_width"]     = "430";
wv_vars["ui_height"]    = "378";
wv_vars["ui_version"]   = "UI0001";
wv_vars["ui_newwindow"] = "yes";
wv_vars["ui_accountid"] = "200106280444";
wv_vars["ui_host"]      = "as00.estara.com";
wv_vars["ui_window"]    = null;

function webCall()
{
  if (wv_vars["ui_window"]==null || wv_vars["ui_window"].closed)
  {
    var wv_argscopy = arguments;
    wv_argscopy[wv_argscopy.length++] = "calltype=webcall";
    wv_vars["ui_window"] = wv_start(wv_argscopy);
  }
}

function webCallBack()
{
  var wv_argscopy = arguments;
  wv_argscopy[wv_argscopy.length++] = "calltype=webcallback";
  wv_vars["ui_window"] = wv_start(wv_argscopy);
}

function webVoicePop()
{
  var wv_argscopy = arguments;
  wv_argscopy[wv_argscopy.length++] = "calltype=webvoicepop";
  wv_start(wv_argscopy);
}

function webChatPop()
{
  var wv_argscopy = arguments;
  wv_argscopy[wv_argscopy.length++] = "calltype=webchatpop";
  wv_argscopy[wv_argscopy.length++] = "wndname=_blank";
  wv_start(wv_argscopy);
}

function wv_start(a)
{
  var wv_wndname = "webVoiceWindow";
  var wv_referrer = escape(window.location);
  if (wv_referrer.length>350)
  {
    var wv_tmp  = (window.location).toString().indexOf("?");
    wv_referrer = wv_tmp>0 ? escape((window.location).toString().substring(0,wv_tmp)+"---TRUNCATED") : escape("UNAVAILABLE - URL IS TOO LONG");
    if (wv_referrer.length>350)
	{
	  wv_referrer = escape("UNAVAILABLE - URL IS TOO LONG");
	}
  }
  var wv_pagetitle = typeof(document.title)!="undefined" ? escape(document.title) : "UNKNOWN";
  if (wv_pagetitle.length>255)
  {
    wv_pagetitle = escape((document.title).toString().substring(0,243)+"---TRUNCATED");
  }
  var wv_newwindow = wv_vars["ui_newwindow"];
  var wv_width = wv_vars["ui_width"];
  var wv_height = wv_vars["ui_height"];
  var wv_ui = wv_vars["ui_version"];
  var wv_accountid = wv_vars["ui_accountid"];
  var wv_features = "";
  var wv_baseurl = "";
  var wv_getRequest = "";
  var wv_protocol = document.location.protocol=="file:" ? "http:" : "";

  for (var i=0 ; i<a.length ; i++)
  {
    var wv_Arg = a[i].toString();
    var wv_indexDelim = wv_Arg.indexOf("=");
    if (wv_indexDelim!=-1)
    {
      var Name = (wv_Arg.substring(0,wv_indexDelim)).toLowerCase();
      var Value = wv_Arg.substring(wv_indexDelim+1,wv_Arg.length);
      switch(Name)
      {
        case "wndname": wv_wndname = Value;
        break;
        case "referrer": wv_referrer = escape(Value);
        break;
        case "newwindow": wv_newwindow = Value;
        break;
        case "width": wv_width = Value;
        break;
        case "height": wv_height = Value;
        break;
        case "accountid": wv_accountid = Value;
        break;
        case "wv_ui": wv_ui = Value;
        break;
        case "features": wv_features = Value;
        break;
        case "baseurl": wv_baseurl = Value;
        break;
        case "ppwinname": if (Value=="") { Value = "PagePushWindow" + (new Date()).getTime() + Math.round(Math.random()*1000000); this.name = Value; }
                default: wv_getRequest += "&" + Name + "=" + escape(Value);
        break;
      }
    }
    else
    {
      alert("ERROR: Invalid argument passed to webXXX() function - Arg" + i + " is missing '=' sign : " + wv_Arg );
      return null;
    }
  }

  
  if (wv_baseurl=="")  wv_baseurl  = wv_protocol + "//" + wv_vars["ui_host"] + "/UI/" + wv_ui + "/" + wv_ui + ".php";
  if (wv_features=="") wv_features = "width=" + wv_width + ",height=" + wv_height + ",menubar=no,toolbar=no,directories=no,scrollbars=no,status=no,left=0,top=0,resizable=no";
  wv_getRequest = wv_baseurl +
                  ( wv_baseurl.indexOf("?")==-1 ? "?" : "&" ) +
                  "donotcache=" + (new Date()).getTime() +
                  "&accountid=" + wv_accountid +
                  "&referrer="  + wv_referrer +
                  "&pagetitle="  + wv_pagetitle +
                  "" +
                  wv_getRequest;
  if (wv_newwindow!="yes")
  {
    window.location = wv_getRequest;
  }
  else
  {
    return window.open( wv_getRequest, wv_wndname, wv_features );
  }
}


