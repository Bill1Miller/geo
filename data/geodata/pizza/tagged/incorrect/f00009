function tog_revs(o,o2,onoff) {
  o = d.getElementById(o);
  o2 = d.getElementById(o2);
  if( onoff == 0 ) {
    o.style.display = "none";
    o2.style.display = "block";
  }
  else if( onoff == 1 ) {
    o.style.display = "block";
    o2.style.display = "none";
  }  
}
