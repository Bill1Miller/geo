<script language="JavaScript">
function distScript (v) {
if (v == "STYPE")
return document.distance.RR.selectedIndex == 0 ? "S" : "D";
else if (v == "R")
return document.distance.RR.selectedIndex == 0 ? "": "D";
}
function distLink () {
// Generate a distance sort link:
document.location.href='http://yellowpages.superpages.com/listings.jsp?SRC=portals&T=Los+Angeles&S=CA&PP=N&STYPE='+distScript('STYPE')+'&L=Los+Angeles+CA&CID=00000509710&IP=142.166.23.210&OO=1&RR='+document.distance.RR.value+'&R='+distScript('R')+'&CP=Food+%26+Dining%5ERestaurants%5EPizza%5E';
}
</script>
