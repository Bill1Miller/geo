<script type="text/javascript">
function google_ad_request_done( google_ads ) {
	if (google_ads.length < 1 ) {
		document.getElementById('bottom_ad_block').style.display = 'none';
		return;
	}
	var ads_length;
	if (google_ads.length > 3) {
		ads_length = 3;
	} else {
		ads_length = google_ads.length;
	}
	for(i = 0; i < ads_length; ++i) {
document.write("<TR>");
document.write(" <TD  CLASS=\"network\" valign=\"top\" width=\"85\">");
document.write("  <TABLE BORDER=\"0\" CELLPADDING=\"0\" CELLSPACING=\"0\" width=\"85\">   ");
