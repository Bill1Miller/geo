			//<![CDATA[
			
			function showGMap() {
				if (GBrowserIsCompatible()) {
					// resize the map
					var m = document.getElementById("LocationGoogleMap");
					m.style.height = "250px";
					m.style.width = "390px";
					var map = new GMap(m);
					map.addControl(new GSmallMapControl());
					map.centerAndZoom(new GPoint(-122.69830, 45.52715), 2);
					var point = new GPoint(-122.69830, 45.52715);
					marker = new GMarker(point);
					var html = '<div style="white-space:nowrap;"><strong>Escape From New York Pizza</strong><br />622 NW 23rd Ave<br />Portland, OR 97210</div>';
						GEvent.addListener(marker, 'click', function() {
						marker.openInfoWindowHtml(html);
					});
					map.addOverlay(marker);
				}
			}
			
			function toTarget () {
				var addr = document.directions.fromaddress.value;
				addr = addr.replace(/\+/g, '%2B');
				addr = addr.replace(/ /g, '+');
				var googleAddress = '622+NW+23rd+Ave,';
				googleAddress += 'Portland,OR';
				var txt = 'http://maps.google.com/?q=from:'+addr+'+to:+';
				txt += googleAddress;
				return txt;
			}
			
			//]]>
			</script>
  
