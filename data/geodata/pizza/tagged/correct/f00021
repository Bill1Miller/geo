<script language="JavaScript">
function mapIt () {
if (document.ypform.L.value == "") {
alert("You must enter a Location.");
} else {
if (document.ypform.C.value == "") {
document.ypform.CID.value = "";
document.ypform.scale.value = "";
document.ypform.lng.value = "";
document.ypform.lat.value = "";
}
document.ypform.action = "http://cgi.superpages.com/giemap/map-based/map.cgi?";
document.ypform.submit();
}
}
</script>
