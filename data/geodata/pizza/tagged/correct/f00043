function ZTo(zl) {if (browserok) {if (zl != czl && zl >= 0 && zl <= max_zoom) {cc = E('zl' + czl);nc = E('zl' + zl);cl = E('l' + czl);nl = E('l' + zl);while (czl < zl) {zp += 'O';czl += 1;}while (czl > zl) {zp += 'I';czl -= 1;}if (!changemapurl()) {return true;}cc.style.backgroundColor = '#ffffff';cl.style.textDecoration = 'underline';cl.style.color = '#0000cc';cl.style.fontWeight = 'normal';nc.style.backgroundColor = '#eeeeee';nl.style.textDecoration = 'none';nl.style.color = '#000000';nl.style.fontWeight = 'bold';EZOut(czl < max_zoom);EZIn(czl > 0);}return false;} else {return true;}}
function Z(diff) {return ZTo(czl + diff);}

function recentermap(e) {if (!browserok) return true;var x, y;if (e.offsetX) {x = e.offsetX;y = e.offsetY;} else if (e.layerX) {x = e.layerX;y = e.layerY;} else { return true; }if (x >= 0 && x < map_x && y >= 0 && y < map_y) {var oldzp = zp;zp += "P" + (x - (map_x / 2)) + "," + (y - (map_y / 2));if (!changemapurl()) { zp = oldzp; return true; }}return false;}
//-->







</script><table>
