function changeImg(imgName, imgNum) {
  // Only use JavaScript capable browsers able to see image arrays
  if ((navigator.appName.substring(0,9) == "Microsoft" && 
      ((navigator.appVersion.indexOf("Win") > 0 & parseInt(navigator.appVersion) >= 4) ||
       (navigator.appVersion.indexOf("Mac") > 0 && parseInt(navigator.appVersion) >= 3)))
      ||
      (navigator.appName.substring(0,8) == "Netscape" && parseInt(navigator.appVersion) >= 3))

    imgName.src = imgArray[imgNum].src;
 }

