	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		tools_over = newImage("images/tools-over.gif");
		media_over = newImage("images/media-over.gif");
		materials_over = newImage("images/materials-over.gif");
		preloadFlag = true;
	}
