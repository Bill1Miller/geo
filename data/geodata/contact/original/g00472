//validate function for T-Mobile search site
function validateSearch(isActive) {
//alert (isActive);
	if ( isActive == 'False') {
		alert('This feature is currently unavailable. Please try again shortly.');
		return;
	}
		var toSearch =  self.document.frmSearch;
		var box;
		var question;
		question = toSearch.question_box.value;
		//alert (question)
		box = toSearch.question_box;

		//validate that something was entered; warn for truncation;
		if (question == "")	{
			alert ("Please enter a question or keywords in the search box.");
			box.focus();
		}
		else if (question.length > 200) {
				alert("Your question is too long. It may be truncated at 200 characters.");
				//extra: we are already setting maxlength in the form, so this should never fire
				toSearch.submit();
			}
			else {
			toSearch.submit();
			}
	}

	function formatQ () {
		txtValue = document.frmSearch.question_box.value;
		txtContent = new String(txtValue);
		txtContent = "";
		document.frmSearch.question_box.value = txtContent;
		}
