		function popUp(url, width, height, left, top, toolbar) {
			var intWidth=568;
			var intHeight=400;
			var intLeft=30;
			var intTop=45;
			var intTool=0;
			if(width > 0) intWidth = width;
			if(height > 0) intHeight = height;
			if(left > 0) intLeft = left;
			if(top > 0) intTop = top;
			if(toolbar < 2) intTool = toolbar;
			var winPopUp = window.open(url,'popup','width=' + intWidth + ',height=' + intHeight + ',left=' + intLeft + ',top=' + intTop + ',toolbar=' + intTool + ',scrollbars=1,resizable=1');
		    if(typeof window.focus != 'undefined') javascript: winPopUp.focus();
		}

		function tab_goThere(el)
						{
							str = el.innerHTML;
							re = /href='([^"]+)'/i;
							if(re.test(str))
							{
								url = RegExp.$1;
								location.href=url;
							}
						}
		
		<!-- Alert prompt when the Store Locator tool is not enabled -->
function storeLocator(url, isActive)
{
	
	if ( isActive == 'False') {
		alert('Sorry, but we are currently unable to perform your search.\nPlease try again later, or call 1-877-387-4324 to find the\nT-Mobile location nearest you.');
	}
	else 
	{
		document.location = url;
	}
}

<!-- End Alert -->

<!-- Alert prompt when the jobs site is not enabled -->
function jobsStatus(url, isActive)
{
	
	if ( isActive == 'False') {
		alert('Sorry, our jobs site is temporarily unavailable.');
	}
	else 
	{
		document.location = url;
	}
}

<!-- End Alert -->

	
		//-->	
</script>

<script type="text/javascript">

	var demoWindow = null;
	
	function popUpDemo(url, name, width, height, left, top, toolbar, scrollbar, resizable ) {
		
		var params = 'width=' + width + ',height=' + height + ',left=' + left + ',top=' + top + ',toolbar=' + toolbar + ',scrollbars=' + scrollbar + ',resizable=' + resizable;
		
		popUpDemo(url, name, params );
		
	}		

	function popUpDemo(url, name, params ) {
	
		if (demoWindow != null) {
		
			demoWindow.close();		
		} 		
											
		demoWindow = window.open(url,name,params);
				
		if(typeof window.focus != 'undefined') javascript: demoWindow.focus();
	}		
	
</script><!-- START OF LOGIC FOR STORE SELECTOR FUNCTIONALITY -->
<SCRIPT LANGUAGE="JavaScript">
<!--
//error checking for zip and area code input
function isZip(str_input) {
	if(str_input == "") {
		return true;
	}
	var digit_flag; //set to true if a digit
	temp_string = new String;

	for (i=0; i < str_input.length; i++) {
		chr_value = str_input.charAt(i);
		digit_flag = false;
		for(y = 0; y < 10; y++)  //Loop through all digits 0 - 9
		{
			if(chr_value == (y + ''))
			{
				digit_flag = true;
				continue;
			}
		}
		if(digit_flag == true) {
			temp_string += chr_value;
		}
	}
	if(temp_string.length != 5) {
		return false;
	}
	return true;  //valid Zip Code
}

function selectState(){
	if (self.document.frmSelectState.selState.selectedIndex == 0) {
		alert('Please select a state.');
		self.document.frmSelectState.selState.focus();
	}
	else {
		self.document.frmSelectState.submit();
	}
}

function storeSearch(){
	if(self.document.frmLocate.rdoMode[0].checked){
		searchZip();
	} else if (self.document.frmLocate.rdoMode[1].checked){
		searchArea();
	} else {
		alert("Please select an option to search.");
	}
}

function searchZip(){
	if (self.document.frmLocate.txtCriteria.value == "") {
		alert("Please enter a zip code to begin your search.");
	} else if (!isZip(self.document.frmLocate.txtCriteria.value)) {
		alert("Invalid Zip Code.");
	} else {
		//self.document.frmSearch.hdnValue.value = self.document.frmSearch.txtCriteria.value;
		//self.document.frmSearch.hdnMode.value = "z";
		self.document.frmLocate.submit();
	}
}

function searchArea(){
	var strArea = self.document.frmLocate.txtCriteria.value;
	if (strArea == "") {
		alert("Please enter an area code to begin your search.");
	} else if (strArea.length != 3) {
		alert("Area Code must be 3 digits.");
	} else if (strArea <= 999){
		//self.document.frmLocate.hdnValue.value = self.document.frmLocate.txtCriteria.value;
		//self.document.frmLocate.hdnMode.value = "a";
		self.document.frmLocate.submit();
	} else {
		alert("Invalid characters in Area Code.");
	}
}

//-->
</SCRIPT>
<!-- END OF LOGIC FOR STORE SELECTOR FUNCTIONALITY -->

