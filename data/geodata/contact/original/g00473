function check_browser()	{
	// run if netscape friendly and atleast version 3 ...
	ver = Math.round(parseFloat(navigator.appVersion) * 1000);
	if (navigator.appName.substring(0,8) == "Netscape" && ver >=3) store_navigation_graphics();
}
