function hilite(imgname,srcchange)      {
        var iname = imgname;
        var newsrc = srcchange;
        document.images[iname].src = newsrc;
}
function nav_menu(section,onoff)        {
        // left navigational graphic named 'leftnav'
        // right navigational graphic named 'rightnav'
        var rw = section;
        if (onoff == 'on')     {
                document.images['leftnav'].src = 'http://ad6.horvitznewspapers.net/custom/img/tdt/navigation/ltnav_' + rw + '.gif';
                document.images['rightnav'].src = 'http://ad6.horvitznewspapers.net/custom/img/tdt/navigation/rtnav_' + rw + '.gif';
        }
        if (onoff == 'off')    {
                // default back to ...
                document.images['leftnav'].src = 'http://ad6.horvitznewspapers.net/custom/img/tdt/navigation/ltnav_main.gif';
                document.images['rightnav'].src = 'http://ad6.horvitznewspapers.net/custom/img/tdt/navigation/rtnav_unselected.gif';
        }
}
function aprestrictions()	{
	var APres = window.open("/ap.html","","HEIGHT=300,WIDTH=400,SCROLLBARS");
}

