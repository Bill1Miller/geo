<script language="JavaScript">

function getCookieVal(offset) {  
	var endstr=document.cookie.indexOf(";", offset);  
	if (endstr==-1) endstr=document.cookie.length;
	return unescape(document.cookie.substring(offset, endstr));
}

function getCookie(name) {
	var arg=name+"=";
	var alen=arg.length;
	var clen=document.cookie.length;
	var i=0;
	while (i<clen) {
		var j=i+alen;
		if (document.cookie.substring(i, j)==arg)
			return getCookieVal(j);
		i=document.cookie.indexOf(" ", i) + 1;
		if (i == 0) break;
	}
	return null;
}

var vRLNName = getCookie("RLNNAME");
if (vRLNName != null) {
	document.write("<td width='25'><img src='/images/spacer.gif' width='25' height='17' alt='Spacer'></td>");
	document.write("<td valign='bottom'>Welcome back, "+vRLNName+". If you are not "+vRLNName+", please <a href='/registration/login/login.cfm' class='blulinkstreb'>click here</a>.</td>");
	}
else
	{
	document.write("<td width='25'><img src='/images/spacer.gif' width='25' height='17' alt='Spacer'></td>");
	document.write("<td valign='bottom'><strong>Welcome to SPSS.com.</strong> <a href='/registration/login/login.cfm' class='blulinkstreb'>Click here to login or register.</a></td>");
	}
	
</script>

