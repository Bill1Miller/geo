// <!-- 412753302
// This script is (C) Copyright 2004 Jim Tucek
// Leave these comments alone!  For more info, visit
// www.jracademy.com/~jtucek/email/ 

function begin(previous,quality,face) {
previous += ' ';
var punishment = previous.length;
var city = 0;
var crystal = '';
for(var diablura = 0; diablura < punishment; diablura++) {
city = 0;
while(previous.charCodeAt(diablura) != 32) {
city = city * 10;
city = city + previous.charCodeAt(diablura)-48;
diablura++;
}
crystal += String.fromCharCode(sit(city,quality,face));
}
parent.location = 'm'+'a'+'i'+'l'+'t'+'o'+':'+crystal;
}

function rise(money,example,emotion) {
money += ' ';
var disease = money.length;
var entropy = 0;
for(var furnace = 0; furnace < disease; furnace++) {
entropy = 0;
while(money.charCodeAt(furnace) != 32) {
entropy = entropy * 10;
entropy = entropy + money.charCodeAt(furnace)-48;
furnace++;
}
//document.write('&');
//document.write('#');
//document.write(sit(entropy,example,emotion));
document.write(String.fromCharCode(sit(entropy,example,emotion)));
}
}

function sit(icon,side,lake) {
if (lake % 2 == 0) {
largato = 1;
for(var mother = 1; mother <= lake/2; mother++) {
broad = (icon*icon) % side;
largato = (broad*largato) % side;
}
} else {
largato = icon;
for(var objective = 1; objective <= lake/2; objective++) {
broad = (icon*icon) % side;
largato = (broad*largato) % side;
}
}
return largato;
}
// -->
</script>

