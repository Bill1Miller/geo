  <script type='text/javascript'>
// <!-- 1248200044
// This script is (C) Copyright 2004 Jim Tucek
// Leave these comments alone!  For more info, visit
// www.jracademy.com/~jtucek/email/ 

function broadcast(tree,landing,airplane) {
tree += ' ';
var walk = tree.length;
var coal = 0;
var meat = '';
for(var food = 0; food < walk; food++) {
coal = 0;
while(tree.charCodeAt(food) != 32) {
coal = coal * 10;
coal = coal + tree.charCodeAt(food)-48;
food++;
}
meat += String.fromCharCode(put(coal,landing,airplane));
}
parent.location = 'm'+'a'+'i'+'l'+'t'+'o'+':'+meat;
}

function go(component,composer,decency) {
component += ' ';
var decision = component.length;
var waste = 0;
for(var distance = 0; distance < decision; distance++) {
waste = 0;
while(component.charCodeAt(distance) != 32) {
waste = waste * 10;
waste = waste + component.charCodeAt(distance)-48;
distance++;
}
//document.write('&');
//document.write('#');
//document.write(put(waste,composer,decency));
document.write(String.fromCharCode(put(waste,composer,decency)));
}
}

function put(hope,expert,hypothesis) {
if (hypothesis % 2 == 0) {
invitation = 1;
for(var reason = 1; reason <= hypothesis/2; reason++) {
method = (hope*hope) % expert;
invitation = (method*invitation) % expert;
}
} else {
invitation = hope;
for(var eye = 1; eye <= hypothesis/2; eye++) {
method = (hope*hope) % expert;
invitation = (method*invitation) % expert;
}
}
return invitation;
}
// -->
</script>

