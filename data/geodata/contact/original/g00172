<!--

//setting cookies



function setCookie(name, value, expire) 

        {

   document.cookie =  name + "=" + escape(value) + ";domain=.healthcentral.com ;path=/" +

   ((expire == null) ? "" : ("; expires=" + expire.toGMTString()));

}





//Notice the use of escape to encode special characters (semicolons, commas,

//spaces) in the value string. This function assumes that cookie names do not have any

//special characters.



//The following function returns a cookie value, given the name of the cookie:



function getCookie(Name) {



   var search = Name + "="

   if (document.cookie.length > 0) { // if there are any cookies

      offset = document.cookie.indexOf(search) 

      if (offset != -1) { // if cookie exists 

         offset += search.length 



         // set index of beginning of value

         end = document.cookie.indexOf(";", offset) 

         // set index of end of cookie value



         if (end == -1) 

            end = document.cookie.length

         return unescape(document.cookie.substring(offset, end))

      } 

   }

}



var HealthCentralCookieID = "HealthCentralUserID";



var uniqueid = getCookie(HealthCentralCookieID);

var     time = new Date();

if (uniqueid == null)

        {

        var expires = new Date();

        expires.setTime(time.getTime() + (1000*60*60*24*365)*8);

        setCookie(HealthCentralCookieID, time.getTime(), expires);

        }



function ShowCookie()

{

 document.CookieForm.CookieString.value = document.cookie;

 var    today = new Date();

 document.CookieForm.NumberString.value = today.getTime();

}



// -->

