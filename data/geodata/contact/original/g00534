	function formHandlerIWant(form){
	var URL = document.iwant.iwant.options[document.iwant.iwant.selectedIndex].value;
	window.location.href = URL;
	}
	function formHandlerSearchBy(form){
	var URL = document.pension.location.options[document.pension.location.selectedIndex].value;
	window.location.href = URL;
	}
	function formHandlerViewPlan(form){
	var URL = document.view.plan.options[document.view.plan.selectedIndex].value;
	window.location.href = URL;
	}
