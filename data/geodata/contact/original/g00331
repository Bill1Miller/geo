function get_random(maxNum)
{
if (Math.random && Math.round)
{
var ranNum= Math.round(Math.random()*(maxNum-1));
ranNum+=1;
return ranNum;
} else {
today= new Date();
hours= today.getHours();
mins=  today.getMinutes();
secn=  today.getSeconds();
if (hours==19)    hours=18;
var ranNum= (((hours+1)*(mins+1)*secn)%maxNum)+1;
return ranNum;
}
}

var ads_to_display = new Array(-1,-1,-1);
var count = 0;

while (count < 3) {
number_to_try = get_random(number_of_ads_in_pool);
number_to_try--;
if ((ads_to_display[0] != number_to_try && ads_to_display[1] !=number_to_try && ads_to_display[2] != number_to_try) && count == 0) {
ads_to_display[0] = number_to_try;
count++;
} else
if ((ads_to_display[0] != number_to_try && ads_to_display[1] !=number_to_try && ads_to_display[2] != number_to_try) && count == 1) {
ads_to_display[1] = number_to_try;
count++;
} else
if ((ads_to_display[0] != number_to_try && ads_to_display[1] !=number_to_try && ads_to_display[2] != number_to_try) && count == 2) {
ads_to_display[2] = number_to_try;
count++;
}
}

document.writeln("<P CLASS=\"ii\" ALIGN=center>");
document.writeln("<A HREF=\"http://www.infidels.org\">");
document.writeln("<IMG SRC=\"http://www.infidels.org/images/home.jpg\" BORDER=0 ALT=\"Home\"><\/A>&nbsp;&nbsp;");
