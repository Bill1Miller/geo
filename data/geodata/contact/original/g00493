<!-- 
//setup browser detection


var isNetscape = 0;

isNetscape = nn4;


function fillSel(selObj)
{
  var destList = "acctcode"; //name of the lower dropdown list
  var i = j = 0;
  var newItem;
  var src;
  var srcName = "";
  var numItems = 0;
  
  //get list of items based on users selection in selObj
  for (i = 0; i < selObj.length; i++)
      if (selObj.options[i].selected)
         srcName = selObj.options[i].value;
         
  //return of the value of the state drop down is blank.
  if (srcName.length == 0) return;
  
  if(substr(srcName,0,1) == "/") {
    // open URL
    window.location  = srcName;
  }
  else {
    // jump to state map for the selected map
    window.location  = "/homefinder/browse_search.asp?state=" + srcName;
  }

}

function fillArea(selObj)
{
  var destList = "acctcode"; //name of the lower dropdown list
  var i = j = 0;
  var newItem;
  var src;
  var srcName = "";
  var numItems = 0;
  
  //get list of items based on users selection in selObj
  for (i = 0; i < selObj.length; i++)
      if (selObj.options[i].selected)
         srcName = selObj.options[i].value;
         
  //return of the value of the state drop down is blank.
  if (srcName.length == 0) return;
  
  //if the value contains a Target URL rather than a list name, jump to that page
  if (substr(srcName, 0,4) == 'URL:') 
  {
     window.location  = substr (srcName, 4, (srcName.length - 4));
  } else {

    //otherwise go on to setup the lower list 
    //get the name of the list
    src = eval(srcName);
  
    //get the destination object
//    var dest = eval("document.quicksearch." + destList);  
    var dest = eval("selObj.form." + destList);
  

    with (dest)   
    {
      options.length = 0;
      //go thought the cuplets on the list, loading the destination list
      for (i = 0; i < src.length; i++)
      {
        numItems++;
        newItem = options.length;
        options[newItem] = new Option(src[i]);
        options[newItem].value = src[i+1];
        i++;
      }    
      disabled = false;
     
      //if the lower combo only contains one market,  then jump to the search page using that item's criteria
      if ((numItems == 2 && !isNetscape) || (numItems == 1 && isNetscape)) //just one market 
      {
        //if there is just one item, then select it
        if (isNetscape) {
          options[0].selected = true;
        } else {
          options[1].selected = true;
        }
        //submit the form
        if (form.name == 'quicksearch') { 
          document.quicksearch.submit(); 
          disabled = true;
          if (!isNetscape )
          {
            style.visible = "hidden";
          }
        }
      } else {
        if (!isNetscape )
        {
         style.display = "inline";
         style.visibility = "visible";
        }
      }
    }
  }
}

//submit the quicksearch form
function setForm()
{
  //submit the form
  document.quicksearch.submit();
}


