function doSearch()
{
  var r = document.sform.r.value;
  var q = document.sform.q.value;
  if( r == 2 )
  document.location.href = "/sub/" + q;
  else if( r == 3 )
  document.location.href = "http://web.ask.com/web?o=10235&q=" + escape(q);
  else
  document.location.href = "/search?t=1&r=" + r + "&q=" + escape(q);
}
</script>
