function swapIt(imgName,imgType,imgAction) {
      if (document.images) {
        if (imgAction == 1) {
          switch(imgType){
            case "Inform_Yourself": newSrc="/Graphics/informyourself_on.gif"; break
          case "Find_Support": newSrc="/Graphics/findsupport_on.gif"; break
          case "Take_Action": newSrc="/Graphics/takeaction_on.gif"; break
        }
        }
        else {
          switch(imgType){
            case "Inform_Yourself": newSrc="/Graphics/informyourself_off.gif"; break
          case "Find_Support": newSrc="/Graphics/findsupport_off.gif"; break
          case "Take_Action": newSrc="/Graphics/takeaction_off.gif"; break
        }
        }
        document.images[imgName].src = newSrc;
      }
    }
