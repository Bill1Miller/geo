var browserName=navigator.appName; 
var browserVer=parseInt(navigator.appVersion); 
var ua = window.navigator.userAgent;
var msie = ua.indexOf ( "MSIE " );
if (browserName=="Netscape" && browserVer>=4){
   version="pop"; }
else {
   if ( parseInt (ua.substring (msie+5, ua.indexOf (".", msie ))) >= 5 ) {
      version="pop"; }
   else { 
      version="no"; }
}
 
var popunder='http://www.tigerdirect.com/sectors/ultralcd_sweep/popunder.asp';
//Pop-under only once per browser session? (0=no, 1=yes)
//Specifying 0 will cause popunder to load every time page is loaded
var once_per_session=1;
///No editing beyond here required/////
function get_cookie(Name) {
  var search = Name + "="
  var returnvalue = "";
  if (document.cookie.length > 0) {
    offset = document.cookie.indexOf(search)
    if (offset != -1) { // if cookie exists
      offset += search.length
      // set index of beginning of value
      end = document.cookie.indexOf(";", offset);
      // set index of end of cookie value
      if (end == -1)
         end = document.cookie.length;
      returnvalue=unescape(document.cookie.substring(offset, end))
      }
   }
  return returnvalue;
}
function loadornot(){
   if (version=="pop") {
   //alert("Your browser passes the test");
      if (once_per_session==0) {
         win2=window.open(popunder,"popunder","width=450,height=355,left=0,top=0,screenX=0,screenY=0");
         win2.blur();
         window.focus();
      }
      else
         if (get_cookie('popunder')=='')
         {
            {
            win2=window.open(popunder,"popunder","width=450,height=355,left=0,top=0,screenX=0,screenY=0");
            win2.blur();
            window.focus();
            }
         document.cookie="popunder=yes";
         }
   }
}
loadornot();
//-->
