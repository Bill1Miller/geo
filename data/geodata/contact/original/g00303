<script>
//give random coordinates for each node before draw
if (!document.layers) 
for (var i in tree.nodes) {
	if (!tree.nodes[i].obj.el) tree.nodes[i].obj.init(tree.nodes[i].parent);
	tree.nodes[i].obj.move(tree.nodes[i].obj.x+200-Math.random()*400, tree.nodes[i].obj.y+Math.random()*200);
}
//draw all with animation
tree.draw(0);
</script>

