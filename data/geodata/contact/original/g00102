<script language="javascript">  
function submitSimpleSearch()
{
	var searchBox = document.getElementById("SimpleSearchText");
	
  if (searchBox.value == '') {
		alert("Please enter a query.");
    searchBox.focus();
	}
	else {
	  // validation for the frames
	  if (window.parent)
	   window.parent.location.href="Search.aspx?section=Information&SimpleQuery=" + escape(searchBox.value);
	  else 
	   location.href="Search.aspx?section=Information&SimpleQuery=" + escape(searchBox.value);
	}
}
</script>
