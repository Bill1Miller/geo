<script>
function changeUSStateName() {
for (var i=0; i<document.locations_form.locationId.options.length; i++) {
document.locations_form.locationId.options[i].text = getUSStateName(document.locations_form.locationId.options[i].text);
}
}
</script>

<script>
var currForm;

function checkFormElements(formName) {
	if (document.DepartmentSearch.departmentId.selectedIndex==0 ) {
		alert("Please select a department");
	} else if (document.DepartmentSearch.departmentId.selectedIndex==1 ) {
		alert("Please select a department");
	} else {
		document.DepartmentSearch.submit();
	}
}
</script>

