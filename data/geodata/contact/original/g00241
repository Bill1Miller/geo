	var str ="";
	var sPrevStateName = "default";
	var sNextStateName = "";

	for(i=0;i<LocationCollection.length;i++){
		sNextStateName  = LocationCollection[i].state;
		if (sPrevStateName  != sNextStateName)	
		{
			str= str + "<option value='" + LocationCollection[i].locationId + "'>" + LocationCollection[i].state + "</option>";
			sPrevStateName  = sNextStateName;
		}

	}
	document.writeln(str);
