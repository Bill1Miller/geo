  function menuItem(pid, sid) {
 		this.parentMenu = getMenu(pid);
		this.subMenu = getSubMenu(pid, sid);
 	   
	}

   function menuNode(title, href, width, height, image, imageName, mouseOverImage, selectedImage) {
		this.title = title;
		this.href = href;
		this.width = width;
		this.height = height;
		this.image = image;
		this.imageName = imageName;
		this.mouseOverImage = mouseOverImage;
		this.selectedImage = selectedImage;
	}
   
    
    function swapImg(myImage, imgName) {
		document.images[myImage].src = "/images/nav/" + imgName;
	}
	
   function getMenu (menuId) {
		var pmenu = '';
		var myImage;

		pmenu += '<td height="15"  bgcolor="#000000" align="right"><img src="/images/nav/spacer.gif" height="15" width=172 alt=""></td>'
		for (var currMenu = 0; currMenu < menu.length; currMenu++) with (menu[currMenu][0]) {
			if (menuId == currMenu) {
				myImage = selectedImage;
			} else {
				myImage = image;
			}
			
		  if(currMenu!=0) {	
