function ExternalSitePopUp(desturl) {
	if (desturl.indexOf("alternativeanticoagulation") > 0 || desturl.indexOf("tissuedurability")>0) {
		window.open(desturl,'_blank', 'toolbar=0,scrollbars=1,location=0,statusbar=0,menubar=0,resizable=0,width=785,height=550,left = 20,top = 20');
	} else if (desturl.indexOf("catsyminvitation") > 0) { 
		//Symposium on Catheter AblationTechniques 10/13-10/15; Ask Gretchen b4 remove
		var win =window.open(desturl,'54', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=680,height=520,left = 20,top = 20');
		win.focus();
	} else if (desturl.indexOf("1.5millionth") > 0) { 
		//1.5 million implant; remove with removal of ad
		window.name="sjmmain";
		var win =window.open(desturl,'_blank', 'toolbar=1,scrollbars=1,location=1,statusbar=1,menubar=1,resizable=1,width=800,height=600,left =0,top = 0');
		win.focus();
	} else {
		window.open(desturl,'_blank', 'toolbar=1,scrollbars=1,location=1,statusbar=1,menubar=1,resizable=1,width=610,height=520,left = 20,top = 20');
	}
}

