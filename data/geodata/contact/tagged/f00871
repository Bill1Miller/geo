<script language="javascript">

var timeout0;
var lastElement = '';
var lastElement2 = '';

function getObj(name)
{
 if (document.getElementById){
	   this.obj = document.getElementById(name);
	   this.style = document.getElementById(name).style;
 }
 else if (document.all){
	   this.obj = document.all[name];
	   this.style = document.all[name].style;
 }
 else if (document.layers)
 {
	   if (document.layers[name]){
	   	this.obj = document.layers[name];
	   	this.style = document.layers[name];
	   }
	   else{
	    this.obj = document.layers.testP.layers[name];
	    this.style = document.layers.testP.layers[name];
	   }
 }
}

function findPosX(obj)
{
	var curleft = 0;
	if (obj.offsetParent){
		while (obj.offsetParent){
			curleft += obj.offsetLeft
			obj = obj.offsetParent;
		}
	}
	else if (obj.x)
		curleft += obj.x;
	return curleft;
}

function findPosY(obj)
{
	var curtop = 0;
	if (obj.offsetParent){
		while (obj.offsetParent){
			curtop += obj.offsetTop
			obj = obj.offsetParent;
		}
	}
	else if (obj.y)
		curtop += obj.y;
	return curtop;
}

function setPosition(parent,target){
	var newX = findPosX(parent);
	var newY = findPosY(parent);

	var e = new getObj(target);
	e.style.top = newY - 5;
	e.style.left = newX + 204;
}

function getElement(x){
	var temp;
	if(document.getElementById){
		var temp = document.getElementById(x);
		temp = temp.style;
	}else if(document.all){
		var temp = eval('document.all.' + x);
		temp = temp.style;
	}else{
		var temp = eval('document.' + x);
	}
	return temp;
}

function hideNow(){
	var hidden = 'hidden';
	clearTimeout(timeout0);
	if(lastElement!=''){
		var element = getElement(lastElement);
		element.visibility = hidden;
		lastElement = '';
	}
	if(lastElement2!=''){
		unHiliteBackground(lastElement2);
		lastElement2 = '';
	}
}

function show(x,y){	
	var visible = 'visible';
	if (document.layers) visible = 'show';
	
	if (lastElement!=x){
		if (lastElement!='') hideNow();
		var element = getElement(x);
		element.visibility = visible;	
		lastElement = x;
		hiliteBackground(y);
		lastElement2 = y;
	}else{
		clearTimeout(timeout0);
	}
}

function hide(x,y){
	lastElement = x;
	lastElement2 = y;
	timeout0 = setTimeout('hideNow()',500);
}

function hiliteBackground(y){
	if(y!=''){
		var temp = getElement(y);
		if(temp!=null){
			temp.backgroundColor = '#C6C3C6';
		}
	}
}

function unHiliteBackground(y){
	if(y!=''){
		var temp = getElement(y);
		if(temp!=null){
			temp.backgroundColor = 'transparent';
		}
	}
}

</script>


