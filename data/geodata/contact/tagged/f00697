<script language="JavaScript">
function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}
function changeImages() {
  if (document.images && (preloadFlag == true)) {
    for (var i=0; i<changeImages.arguments.length; i+=2) {
      document[changeImages.arguments[i]].src = eval(changeImages.arguments[i+1] + ".src");
    }
  }
}
var preloadFlag = false;
function preloadImages() {
	if (document.images) {
	image1on = newImage("/images/interior/nav/int_new_ovr.gif");
	image1off = newImage("/images/interior/nav/int_new_up.gif");
	image2on = newImage("/images/interior/nav/int_ind_ovr.gif");
	image2off = newImage("/images/interior/nav/int_ind_up.gif");
	image3on = newImage("/images/interior/nav/int_cli_ovr.gif");
	image3off = newImage("/images/interior/nav/int_cli_up.gif");
	image4on = newImage("/images/interior/nav/int_mem_ovr.gif");
	image4off = newImage("/images/interior/nav/int_mem_up.gif");
	image5on = newImage("/images/interior/nav/int_car_ovr.gif");
	image5off = newImage("/images/interior/nav/int_car_up.gif");
	image6on = newImage("/images/interior/nav/int_who_ovr.gif");
	image6off = newImage("/images/interior/nav/int_who_on.gif");
	image7on = newImage("/images/interior/nav/int_fin_ovr.gif");
	image7off = newImage("/images/interior/nav/int_fin_up.gif");
	preloadFlag = true;
	}
}
preloadImages();
</script>
