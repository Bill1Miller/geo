// normal image rollovers
function img_act(imgName) {
	if (loaded && document.images) {
		imgOn = eval(imgName + "on.src")
		document [imgName].src = imgOn
	}
}

function img_inact(imgName) {
	if (loaded && document.images) {
		imgOff = eval(imgName + "off.src")
		document [imgName].src = imgOff
	}
}

// navigation image rollovers
function nav_act(imgName1,imgName2) {
	if (loaded && document.images) {
		imgOn = eval(imgName1 + "on.src")
		document [imgName1].src = imgOn
		document [imgName2].src = imgOn
	}
}

function nav_inact(imgName1,imgName2) {
	if (loaded && document.images) {
		imgOff = eval(imgName1 + "off.src")
		document [imgName1].src = imgOff
		document [imgName2].src = imgOff
	}
}

function donothing() {}

function ready(){
	loaded = true
}
