<!--//

var gForm = "";
var gErrorLog = "";
var gOkDate = 0;

function checkform(form) {

    gForm = form;
    validate("Salutation","Salutation","isSelected")
    validate("FirstName","First Name","isEmpty")
    validate("LastName","Last Name","isEmpty")
	
	if (gForm.poBox.value == "")
	{
	    validate("HouseNumber","House Number","isEmpty")
	    validate("StreetName","Street Name","isEmpty")
	}
	else
	{
	    validate("poBox","PO Box","isAlphaNumeric")
	}
	
	if ( gForm.poBox.value != "" && ( gForm.HouseNumber.value != "" || gForm.StreetName.value != "" ) )
	{
        gErrorLog += "- Either PO Box or House Number & Street Name must be entered but not both.\n"
	}
	
    validate("City","City","isEmpty")
    validate("Zip","Zip/Postal Code","isEmpty")
    validate("Country","Country","isSelected")
    validate("HomePhone","Home Phone","isEmpty")
    validate("WorkPhone","Work Phone","isEmpty")
    validate("Email","Email Address","isValidEmail")
    validate("Message","Message","isEmpty")
    validate("DirectTo","Message to the appropriate Society resource","isChecked")
	
    return showErrors()
}

