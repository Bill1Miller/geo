function getObject(which)
{
	if (document.all)
		return document.all[which];
	else if (document.getElementById)
		return document.getElementById(which);
	else if (document.layers)
		return document.layers[which];
}

function rotateFeatureImage ()
{
	// skip the first one since it is already displaying
	featureCounter++;
	if (featureCounter > featureImageArraySrc.length-1)
		featureCounter=0;
	var object = getObject("featuredImage");
	if (object)
	{
		object.src="http://www.civilrights.org/banner/small/"+featureImageArraySrc[featureCounter];
		object.alt=featureImageArrayAlt[featureCounter];
		object = getObject("featuredLink");
		object.href=featureImageArrayLink[featureCounter];
	}
}

function startFeatureRotate()
{
	setInterval("rotateFeatureImage()",5000);
}
