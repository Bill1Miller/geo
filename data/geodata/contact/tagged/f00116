<!--
function selectThis (idx) 
{
  switch (idx){
    case "grac": idx = 1; break;
    case "ebuc": idx = 2; break;
    case "cden": idx = 3; break;
    case "dgol": idx = 4; break;
    case "jfer": idx = 5; break;
    case "efic": idx = 6; break;
    case "lhay": idx = 7; break;
    case "when": idx = 8; break;
    case "cirv": idx = 9; break;
    case "bkom": idx = 10; break;
    case "klaw": idx = 11; break;
    case "rran": idx = 12; break;
    case "tsum": idx = 13; break;
    case "lvil": idx = 14; break;
    case "rkel": idx = 15; break;
    case "kton": idx = 16; break;
    case "cyu": idx  = 17; break;
    case "nzir": idx = 18; break;
    case "crwb": idx = 19; break;
    case "cred1": idx= 20; break;
    case "cred2": idx= 21; break;
    case "atha": idx = 22; break;
    case "mhil": idx = 23; break;
    case "slat": idx = 24; break;
    case "biso": idx = 25; break;
    case "jtra": idx = 26; break;
    case "mkos": idx = 27; break;
    case "cano": idx = 28; break;
  }
  formObj = document.contactForm.mail_to;
  formObj.selectedIndex = idx;
  self.location='#contact';
}

function checkForm (formObj) 
{
	var dataOK = true;
	var errorMsg = "";

	if (dataOK && formObj.mail_to.options[formObj.mail_to.selectedIndex].value == "")
	{ 
		dataOK = false; 
		errorMsg = "You must select a recipient."; 
	}
	if (dataOK && formObj.mail_from.value.length == 0) 
	{ 
		dataOK = false; 
		errorMsg = "Your email address is required."; 
	}
	if (dataOK && formObj.mail_from.value.search(/^[\w-\.]{1,}\@([\da-zA-Z-]{1,}\.){1,}[\da-zA-Z-]{2,3}$/i) == -1)
	{
		dataOK = false; 
		errorMsg = "The email address you specified is malformed";
	}
	if (dataOK && formObj.mail_subj.value.length == 0) 
	{ 
		dataOK = false; 
		errorMsg = "A subject is required."; 
	}
	if (dataOK && formObj.mail_msg.value.length == 0) 
	{ 
		dataOK = false; 
		errorMsg = "A message is required."; 
	}
	if (dataOK) 
	{ 
		return true;
	}
	else
	{
		alert(errorMsg);
		return(false);
	}
}
// -->
</script>

