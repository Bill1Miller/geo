IMG01 = "http://www.unocal.com/images/on.gif"  //image when mouse is over the link
IMG02 = "http://www.unocal.com/images/off.gif" //image when mouse isn't over the link


function imgover(imgname){
     imgname.src = IMG01
}

function imgout(imgname){
     imgname.src = IMG02
}

/* Added 93/04 by BC
 Stores the value of the "p" parameter in the URL for
 use in a portion of the governance section */
var pageName = "";
var largeString = "";
var smallString = "";
var cutPlace = "";

largeString = self.location.href;
smallString = largeString.split("p=");
cutPlace = smallString[1].indexOf("&");
if (cutPlace != -1) {
	pageName = smallString[1].substring(0, cutPlace);
	}
	else {
		pageName = smallString[1];
	}
</script><style type="txt/css">

form {   display: inline;}
input {   display: inline;}

