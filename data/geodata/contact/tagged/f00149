<!-- Javascript to handle redirects -->
<script language="javascript">
checkit(location.pathname, "/events.html", "/events.html/", "/resources/events.jhtml");

function checkit(source, rexp1, rexp2 ,dest) {
   if (source == rexp1) {
      doRedirect(dest);
   }
   var findstring = new RegExp(rexp2);
   if (findstring.test(source)) {
      doRedirect(dest);
   }
}

function doRedirect(dest) {
   window.location.replace(dest);
}
</script>
<!-- //Javascript to handle redirects -->


  
    

			
					
				

            
                    
                

