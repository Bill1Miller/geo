      	function buildDelimitedString(valueArray,delimiter)
      	{
      		var delimitString = "";
	      	for(i =0;i < valueArray.length;i++)
	      	{
	      		delimitString += valueArray[i].replace(/ /g,"_").toLowerCase();
	      		if((i+1) != valueArray.length)
	      		{
	      			delimitString += "|";
	      		}
	      	}
	      	return delimitString;
      	}
      	
