<script language="Javascript">

function ShowLayer(theLayerName){
		var theLayer = eval("document." + theLayerName)
		if (theLayer!=null) theLayer.visibility="show"
		}
function HideLayer(theLayerName){
	// alert("HideLayer: "+theLayerName)
		var theLayer = eval("document." + theLayerName)
		// if (theLayer==null) alert("Problem hiding: "+theLayerName)
		if (theLayer!=null) theLayer.visibility="hide"
		}

function HideAllLayers(){
	var theLayerName
	
