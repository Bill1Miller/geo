<script language="JavaScript">
<!--
// Begin hiding script from old browsers.

var validated

function validZIP( fld ) { // 5-digit ZIP
	var rx = /^[0-9][0-9][0-9][0-9][0-9]$/
    if ( rx.exec(fld.value)!=null ) 
       return true // ok
	else {
		validated = false
		alert ("Please enter a 5-digit ZIP code.")
		fld.focus()
		return false
	}
}

function validEmail( fld ) {
	var rx =/^[A-Za-z0-9\.\-_]+@[A-Za-z0-9-_]+(\.[A-Za-z0-9\.\-_]+)*\.[A-Za-z]+$/
    if ( rx.exec(fld.value)!=null ) 
       return true // ok
	else {
		validated = false
		alert ("Please enter a valid email address.")
		fld.focus()
		return false
	}
}

function validQuestion( fld ) { // not null or blank
	var rx =/\w{3,}/
    if ( rx.exec(fld.value)!=null ) 
       return true // ok
	else {
		validated = false
		alert ("Please enter a question.")
		fld.focus()
		return false
	}
}


function validform( frm ) {
	// force validation of all fields
	var i
	validated=true
	for (i=0;i<frm.elements.length && validated;i++)
	if (frm.elements[i].onchange!=null)
		frm.elements[i].onchange()  // force execution of onchange event handler
	return validated
}


// End hiding script from old browsers. -->
    </script>
