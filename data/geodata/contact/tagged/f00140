var OAS_sitepage = 'www.cleveland.com';
var OAS_query = "";
var docURL = location.href;
if (docURL.indexOf('?')>-1) {
    var docURLtemp = docURL.split('?');
    var docURLparts = docURLtemp[0].split('/');
    for (var i=1; i < docURLtemp.length; i++) {
     OAS_query = OAS_query + '?' + docURLtemp[i];
    }
} else {
    var docURLparts = docURL.split('/');
    OAS_query = 'NQS';
}
for (var j=3; j < docURLparts.length; j++) {
   OAS_sitepage = OAS_sitepage + '/' + docURLparts[j];
}
OAS_rn = '001234567890'; OAS_rns = '1234567890';
OAS_rn = new String (Math.random()); OAS_rns = OAS_rn.substring (2, 11);
OAS_sitepage = OAS_sitepage + '/1' + OAS_rns;

