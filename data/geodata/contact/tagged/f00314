}
function GLbd4b30_resetSubMenus() {
	var resetSubMenu=1;

if (!GLV_doReset["e48e9b"] && GLV_currentLayer["e48e9b"]!=null)	resetSubMenu=0;
if (!GLV_doReset["59d761"] && GLV_currentLayer["59d761"]!=null)	resetSubMenu=0;
