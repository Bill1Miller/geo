if (!GLV_doReset["27dee0"] && GLV_currentLayer["27dee0"]!=null)	resetSubMenu=0;
if (!GLV_doReset["af7d57"] && GLV_currentLayer["af7d57"]!=null)	resetSubMenu=0;
	return resetSubMenu;
}

