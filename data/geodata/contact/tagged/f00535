<META NAME="keywords" CONTENT="contact us, address, phone, customer service, residential, business, support, call, fpl customer care, hearing or speech disabled, write, e-mail, email, settings, technical, problems"><SCRIPT TYPE="text/javascript" language="JavaScript" SRC="/library_content_script/new_window.js"></SCRIPT><SCRIPT TYPE="text/javascript" language="JAVASCRIPT">
function contactCall()
{
  var drop    = document.callingForm.contactUs.selectedIndex;
  var dropdwn = document.callingForm.contactUs.options[drop].value;
  document.callingForm.PageName.value =dropdwn ;
  if(document.callingForm.PageName.value == " ")
	{
	  return false;
	}
  document.callingForm.submit();
}
</SCRIPT>
