	<script language="JavaScript" type="text/javascript">
	<!--
	function submitValid(form) {
		if (form.categorySubject.value == "") {
			showError(form.categorySubject, "Please select a category.");
			return false;
		}
		if (form.name.value.length == 0) {
			showError(form.name, "Please enter your name.");
			return false;
		}
		if (form.email.value.length == 0) {
			showError(form.name, "Please enter your email address.");
			return false;
		}
		// validate email address
		// checks for a word character or '-' one or more times followed by an '@' followed 
		// by [a-z] [A-Z] or [0-9] or '-' one or more times followed by a "." followed by a 
		// word character or "." one for more times.
		var emailPattern = /^(\w|-|\.)+\@([a-zA-Z0-9]|-)+\.(\w|\.)+/;
		var emailResult;
		emailResult = emailPattern.exec(form.email.value);
		if (emailResult) {
			// good to go
		} else {
			showError(form.email, "Please double-check and re-enter your email address.\nPlease be sure your email address is properly formatted\n(e.g., user@company.com, user@sub.company.yz).");
			return false;
		}
		if (form.question.value.length == 0) {
			showError(form.question, "Please enter a message.");
			return false;
		}
		
	}
	function showError(ctrl, msg) {
		ctrl.focus();
		alert(msg);
	}
	//-->
	</script>
	
