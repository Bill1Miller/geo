	<!--this script handles all of the rollovers in the top nav-->
	<script language="JavaScript">

	function jumptoorg(){
	        {
			var i,newloc
			newloc=document.form.jumporg.options[document.form.jumporg.selectedIndex].value
			if (newloc.length > 0)
				{
				location.href=newloc
				}
	        }
	 }

	function openPrintable(){
	        {
	        var printableWindow = window.open("","","toolbar=yes,menubar=yes,scrollbars=yes,width=550,height=400");
			printableWindow.document.location = "?article=1007&print=true&start=1";
	        }
	 }

	function clean(arg)
		{
		var j
		var goodchars='1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ@-_.'
		var inval=arg+' '
		var outval=''
		for (j=0;j<inval.length;j++)
			{
			var c=inval.charAt(j)
			if (goodchars.indexOf(c)!=-1)
				{
				outval+=c
				}
			}
			if (outval.length==0)
				{
				return ''
				}
		return outval
		}

	function checkEmail()
	{
		arg = clean(document.subform.email.value);
		document.subform.email.value = arg
		if (arg=='')
			{
			alert('Please enter an Email address.');
			document.subform.email.select();
			document.subform.email.focus();
			return false;
			}
		var len = arg.length;
		var ats = arg.indexOf('@');
		var prd = arg.indexOf('.',ats);
		if ((ats < 1) || (ats == len - 1) || (ats + 2 > prd) || (prd + 1 == len))
			{
			alert('Please enter an Email address in the format "name@domain.dom".');
			document.subform.email.select();
			document.subform.email.focus();
			return false;
			}
		return true;
	}

	function cleanPW(arg)
		{
		var j
		var goodchars='1234567890abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ'
		var inval=arg+' '
		var outval=''
		for (j=0;j<inval.length;j++)
			{
			var c=inval.charAt(j)
			if (goodchars.indexOf(c)!=-1)
				{
				outval+=c
				}
			}
			if (outval.length==0)
				{
				return ''
				}
		return outval
		}

	function checkPW()
	{
		arg = cleanPW(document.subform.pw0.value);
		if (arg != document.subform.pw0.value)
			{
			alert('Please use only letters and numbers in your password.');
			document.subform.pw0.value = ""
			document.subform.pw1.value = ""
			document.subform.pw0.select();
			document.subform.pw0.focus();
			return false;
			}
		if (arg.length<6)
			{
			alert('Please use a password of at least 6 characters.');
			document.subform.pw0.value = ""
			document.subform.pw1.value = ""
			document.subform.pw0.select();
			document.subform.pw0.focus();
			return false;
			}
		if (document.subform.pw0.value!=document.subform.pw1.value)
			{
			alert('Your passwords do not match.\n\nPlease Re-Enter.');
			document.subform.pw0.value = ""
			document.subform.pw1.value = ""
			document.subform.pw0.select();
			document.subform.pw0.focus();
			return false;
			}
		return true;
	}

	</script>
