<script language="javascript">
// Get the HTML page title
var Title = document.title;

// Remove the regular tagline
var Title = Title.replace(' | Business intelligence, data warehousing and analytics editorial from DMReview','');

// Replace the spaces with plus signs.
rExp = / /g;
newTitle = Title.replace(rExp, '+');

// if this is the homepage, we need to set a title since we can't give it a doc.title
if(newTitle=="Business+intelligence,+data+warehousing+and+analytics+editorial+from+DMReview") 
      var newTitle = "homepage";
else 
   var newTitle = newTitle;
      
