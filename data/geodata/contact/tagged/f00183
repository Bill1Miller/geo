if (document.images) {

	d = "images/";
			addbutton("faq");
			addbutton("court");
			addbutton("email");
			addbutton("webmaster");
			addbutton("majorprograms");

}
function addbutton(imgname) {
	eval(imgname + ' = new Image()');
	eval(imgname + '.src = d + imgname + ".jpg"');
	eval(imgname + '_on = new Image()');
	eval(imgname + '_on.src = d + imgname + "_on.jpg"');
}

function msover(imgname) {
	if (document.images) document.images[imgname].src = eval(imgname + '_on.src');
}
function msout(imgname) {
	if (document.images) document.images[imgname].src = eval(imgname + '.src');
}
</SCRIPT>
