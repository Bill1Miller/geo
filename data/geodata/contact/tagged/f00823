    map_west_on = new Image(); map_west_on.src = "images/map_2.gif";
    }
    
function imgOn(imgName) {
        if (document.images) {
            document.map.src = eval(imgName + "_on.src"); 
						}
}

function imgOff(imgName) {
        if (document.images) {
            document.map.src = eval(imgName + "_off.src"); 
						      
				}
}

function goQuickPick()  {
var URL = document.quickPicks.links.options[document.quickPicks.links.selectedIndex].value;
window.location.href = URL;

}

